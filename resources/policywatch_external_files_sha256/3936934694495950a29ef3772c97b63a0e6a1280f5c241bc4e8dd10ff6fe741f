<script>

    function openContact() {
        $(".modal-backdrop").show();
        $("body").prepend("<div class='modal-backdrop'></div>");
        $("body").addClass(" openModal");
        $(".modal").css('display', 'block');
        $("div#contactFormContainer .modal-content").slideDown();
    }
    function closeContact() {
        $(".modal-backdrop").remove();
        $("body").removeClass(" openModal");
        $("body").removeClass(" stayBottom");
        $("div#contactFormContainer .modal-content").slideToggle();
        $(".modal").delay(6000).hide();
    }

    function openContactMobile() {
        //$("nav.radiofono-menu__wraper").addClass("hidden");
        $(".modal-backdrop").show();
        $("body").prepend("<div class='modal-backdrop'></div>");
        $("body").addClass(" openModal");
        $(".modal").css('display', 'block');
        $("div#contactFormContainer .modal-content").slideDown();
        var height = $(window).height();
        $('.modal-content').css('height', height - 100);
    }
    function openBottomContactNormal() {
        $(".modal-backdrop").show();
        $("body").prepend("<div class='modal-backdrop'></div>");
        $("body").addClass(" openModal");
        $("body").addClass(" stayBottom");
        $(".modal").css('display', 'block');
        $("div#contactFormContainer .modal-content").slideDown();
    }
    function openBottomContact() {
        $(".modal-backdrop").show();
        $("body#radiofono247").prepend("<div class='modal-backdrop'></div>");
        $("body#radiofono247").addClass(" openModal");
        $("body#radiofono247").addClass(" stayBottom");
        $("body.radiofono247").prepend("<div class='modal-backdrop'></div>");
        $("body.radiofono247").addClass(" openModal");
        $("body.radiofono247").addClass(" stayBottom");
        $(".modal").css('display', 'block');
        $("div#contactFormContainer .modal-content").slideDown();
    }
    $(document).ready(function () {
        //name?mailFrom?adrFrom?telFrom?plainContent
        $("#btn").click(function () {
            $(".contactFormMessage").html(''); 
            var name = $("input#name").val();
            var mailFrom = $("input#mailFrom").val();
            var adrFrom = $("#adrFrom").val();
            var telFrom = $("#telFrom").val();
            var subjFrom = $("#subj option:selected").text();
            var plainContent = $("#plainContent").val();
            var dataString = 'name=' + name + '&mailFrom=' + mailFrom + '&plainContent=' + plainContent + '&adrFrom=' + adrFrom + '&telFrom=' + telFrom + '&subj=' + subjFrom;
            if (name == '' && mailFrom == '' && plainContent == '' && adrFrom == '') {
                $(".form_name .errorForm").html('Παρακαλώ συμπληρώστε το όνομα σας');
                $("input#name").css('border-color', 'red');
                $(".mail_form .errorForm").html('Παρακαλώ συμπληρώστε το email σας');
                $("input#mailFrom").css('border-color', 'red');
                $(".message_form .errorForm").html('Παρακαλώ συμπληρώστε το μήνυμα σας');
                $("#plainContent").css('border-color', 'red');
                //$(".tel_form .errorForm").html('Παρακαλώ συμπληρώστε το τηλέφωνο σας');
                //$("#telFrom").css('border-color', 'red');
                $(".adr_form .errorForm").html('Παρακαλώ συμπληρώστε τη διεύθυνση σας');
                $("#adrFrom").css('border-color', 'red');
            } else if (name == '') {
                $(".form_name .errorForm").html('Παρακαλώ συμπληρώστε το όνομα σας');
                $("input#name").css('border-color', 'red');
            } else if (mailFrom == '') {
                $(".mail_form .errorForm").html('Παρακαλώ συμπληρώστε το email σας');
                $("input#mailFrom").css('border-color', 'red');
            } else if (adrFrom == '') {
                $(".adr_form .errorForm").html('Παρακαλώ συμπληρώστε τη διεύθυνση σας');
                $("#adrFrom").css('border-color', 'red');
            //} else if (telFrom == '') {
                //$(".tel_form .errorForm").html('Παρακαλώ συμπληρώστε το τηλέφωνο σας');
                //$("#telFrom").css('border-color', 'red');
            } else if (plainContent == '') {
                $(".message_form .errorForm").html('Παρακαλώ συμπληρώστε το μήνυμα σας');
                $("input#plainContent").css('border-color', 'red');
            } else if (name != '' && mailFrom != '' && plainContent != '' && adrFrom != '') {
                $(".contactFormMessage.viablePlaceholder").addClass('loading-form');
                $("button#btn").addClass('disable_button');
                $(".placehold").addClass('disable_cursor');
                $.ajax({
                    type: "POST",
                    url: "https://www.news247.gr/widget/id6125158?view=ajax",
                    data: dataString,
                    success: function (data) {
                        console.log(data);
                        $("button#btn").removeClass('disable_button');
                        $(".contactFormMessage.viablePlaceholder").removeClass('loading-form');
                        $(".placehold").removeClass('disable_cursor');
                        $(".contactFormMessage").html('Στάλθηκε επιτυχώς');
                        $("input:text").val("");
                        $("textarea").val("");
                    }
                });
            } else {
                $(".contactFormMessage").html('Παρακαλώ συμπληρώστε τα πεδία');
            }
            $("#contactFormContainer form :input").each(function () {
                var input = $(this).not('#telFrom').val();
                if (input !== '') {
                    $(this).css('border-color', '#cccccc');
                } else {
                    $(this).css('border-color', 'red');
                }
            });
        });
    });


</script>
