            last_known_scroll_position = window.scrollY;

            //console.info("last_known_scroll_position : ", last_known_scroll_position);
            //console.info("_premiun_parent_height : ", _premiun_parent_height);
            //console.info("_premium_isOpen : " , _premium_isOpen);
            //console.info("_preview : " , _preview);
            //console.info("_wrapper_sticky_elem : " , _wrapper_sticky_elem);

            var _canScaleUp = document.querySelector(".main-menu__container-top").classList.contains("header--big") ? true : false;
            var _canScaleDown = document.querySelector(".main-menu__container-top").classList.contains("header--small") ? true : false;

            if (last_known_scroll_position >= _premiun_parent_height ) {

//console.info("last_known_scroll_position", last_known_scroll_position);
//console.info("_premiun_parent_height", _premiun_parent_height);

                _wrapper_sticky_elem.classList.add("wrapper-sticky--enabled");

                if (document.body.classList.value.indexOf("good-news news") > -1) {
console.info("good-news news");
                    document.querySelector(".good-news.news.standardTemplate .container").classList.add("marginTop95");
                }

                if (document.body.classList.value.indexOf("good-news section") > -1) {
console.info("good-news section");
                    document.querySelector(".good-news.section .container").classList.add("marginTop95");
                }

                if (document.body.classList.value.indexOf("art_20-20 news") > -1) {
console.info("art_20-20 news");
                    document.querySelector(".art_20-20.news .container").classList.add("marginTop95");
                }

                if (document.body.classList.value.indexOf("sec_20-20 section") > -1) {
console.info("sec_20-20 section");
                    document.querySelector(".sec_20-20.section .container").classList.add("marginTop95");
                }

                if (document.body.classList.value.indexOf("sustainability section") > -1) {
console.info("sustainability section");
                    document.querySelector(".sustainability.section .container").classList.add("marginTop95");
                }

                if (document.body.classList.value.indexOf("sustainability news") > -1) {
console.info("sustainability news");
                    document.querySelector(".sustainability.news .container").classList.add("marginTop95");
                }

                if (document.body.classList.value.indexOf("ekthesi-pissaridi tag") > -1) {
console.info("ekthesi-pissaridi tag");
                    document.querySelector(".ekthesi-pissaridi.tag .container").classList.add("marginTop95");
                }

            } else {
                document.querySelector(".container").classList.remove("marginTop95");
                _wrapper_sticky_elem.classList.remove("wrapper-sticky--enabled");
            }

            if (last_known_scroll_position > _trigger_point && _isHome) {
                document.querySelector(".main-menu__container-top").classList.add("header--small");
                document.querySelector(".main-menu__container-top").classList.remove("header--big");
            } else if (last_known_scroll_position < _trigger_point && _canScaleDown) {
                document.querySelector(".main-menu__container-top").classList.add("header--big");
                document.querySelector(".main-menu__container-top").classList.remove("header--small");
            }

        }); // end of event scroll

    } //end of function initStuckAndAnimation();

    // Section & Article stuck 
    window.addEventListener('DOMContentLoaded', (event) => {
        if (!_isHome) {
            initStuckAndAnimation();
        } else if (_isHome && _hasAdblock) {
            initStuckAndAnimation();
        }


    });

</script>

