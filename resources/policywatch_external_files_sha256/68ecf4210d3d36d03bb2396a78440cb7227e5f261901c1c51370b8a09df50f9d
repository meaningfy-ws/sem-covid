                    jQuery('.anchor-js-json').click(function(event) {
                        event.preventDefault();
                        window.location.href = jQuery(this).attr('data-href');
                    });                    
                });
            }
        }).fail (function () {

        });
    });

    function getTopArticlesImage (item) {
        var image = '';
        var prefix = 'h_';
        var suffix = '_340';
        if (item.image.path && item.image.name && item.image.ext) {
            var imgSrc = item.image.path + '/' + prefix + item.image.name + suffix + '.' + item.image.ext;
                            image = '<picture>\n\
                        <img class="card__image lazyload" src="/templates/site/images/jpgs/ph_h_no-image_340.jpg?v2" data-src="' + imgSrc + '" onerror="imageFallBack(jQuery(this), \'site\')">\n\
                    </picture>';
                    }

        return image;
    }
