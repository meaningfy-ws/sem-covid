<script type=text/javascript>
	function doSearch(buttonName,e)
	{
		var key;

		if(window.event)
			key = window.event.keyCode;
		else
			key = e.which;

		if (key == 13) {
			var btn = document.getElementById(buttonName);
			if (btn != null) {
				btn.click();
				event.keyCode = 0
			}
		}
	}
</script>

                                        
                                    
