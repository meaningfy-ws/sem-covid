function share(baseUrl,newWindow) {
    newHref= $('#shareUrl').val();
    newHref=newHref.replace(/&/g,'%26');

    if (newWindow==1) {
        window.open(baseUrl+newHref);
    } else {
        window.location=baseUrl+newHref;
    }
}

