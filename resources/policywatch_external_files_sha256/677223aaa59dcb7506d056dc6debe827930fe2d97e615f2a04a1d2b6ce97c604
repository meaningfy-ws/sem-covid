<script>
$(document).ready(function(){
  $('.fokusitems span.item-fokus').unbind('click'); // bcs this code might get included more than once
  $('.fokusitems span.item-fokus').on('click',function(){
    var $me = $(this);
    var $fok = $me.closest('.art-fokus');
    var uid = 0; // not a string!
    if (!uid){
        $fok.find('.fokusitems, .not_logged_in').toggle();
	    return false;
    }
    //$line.find('.message').remove();
    var id = $(this).closest('.item-fokus').attr('data-id');
	var freq = $(this).closest('.item-fokus').attr('data-freq');
	var cat = $(this).closest('.item-fokus').attr('data-category');
	var tip = $(this).closest('.item-fokus').attr('data-tip');
    if (typeof id === 'undefined' || !id){
      alert('?');
      return false;
    }
	$.post('/master.php',{op:'favs_add_'+cat,id:id},function(res){
      if (res.error){ alert(res.error);}
      else {
		$me.hide();
		$(".art-fokus .message").html("Na elektronski naslov vam bomo poslali obvestilo, ko bo objavljen nov članek iz te teme. <a style='text-decoration:underline;color:#000000;' target='_blank' href='https://www.finance.si/fokus'>Vaš FOKUS...</a>");
		// no need to set freq anymore, the favs_add* call will take care of it -- c0w 2020-01
      }
    },'json');
    return false;
  });
});
</script>
