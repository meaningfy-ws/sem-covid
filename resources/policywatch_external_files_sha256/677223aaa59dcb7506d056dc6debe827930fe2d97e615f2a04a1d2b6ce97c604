<script type="text/javascript">
function zlick_load(){
		var da = {op:'zlick_callback',mode:'authenticate',userid:0,sessid:770673788,reqid:'8ebcddd2-bf70-419e-a479-ca42c98212f4',finOrderForm:'50_DIGITAL-1EUR'};
	$.ajax({
		url: 'https://narocilnice.finance.si/master.php',
		data: da,
		dataType:'jsonp',
		crossDomain: true,
	}).done(function(auth_res){
		console.log('auth result: ',auth_res);
		console.log('authentication completed... load widget');
	zlickWidget({
		// Possible values 'sandbox', 'live'
		// Payments are not charged in 'sandbox' env. 
		env: 'live',
		clientToken: 'hC59LqQzz5GAEHqb8iwTireA1ymvHbdF',
		clientUserId: '0',
		clientTransactionId: auth_res.token,
		purpose: 'subscribe', // Possible values are 'purchase', 'subscribe'
		alternatePaymentsLink: 'https://narocilnice.finance.si/narocilo/50_DIGITAL-1EUR?utm_source=finance&utm_medium=cta&utm_campaign=hardlock&utm_content=finArticle_8963254',
		subscription: {
			id: 'SI_FINANCE_RS' // 'SI_DIMOCO_FS' // Subscription ID
		},
		// Called when user is successfully identified and authenticated by the widget. 
		// Note, this is only called for repeat visitors. 
		// Use this callback to give access to content automatically to users.
		onAuthenticated: function ({ phone, userId }) {
			console.log('User authenticated: ', phone, ' userId: ', userId)
			var da = {op:'zlick_callback',mode:'authenticated'};
			$.ajax({
				url: 'https://narocilnice.finance.si/master.php',
				data: da,
				dataType:'jsonp',
				crossDomain: true,
			});
		},
		// Called when purchase or subscription process completes successfully
		onPaymentComplete: function ({ data, signed }) {
			console.log('Payment completed: ', data, ' Signed: ', signed)
			//document.getElementById('widget-message').innerText = 'Nakup naročnine končan.'
			var da = {op:'zlick_callback',mode:'payment_success',userid:0,sessid:770673788,reqid:'8ebcddd2-bf70-419e-a479-ca42c98212f4',payment_data:data,jwt:signed};
			$.ajax({
				url: 'https://narocilnice.finance.si/master.php',
				data: da,
				dataType:'jsonp',
				crossDomain: true,
			});
			//location.reload();
			setTimeout(function(){
				window.location.reload(1);
			}, 3000);
		},
		// Called when there is failure and widget cannot continue further
		onFailure: function ({ message }) {
			console.log('Failure message: ', message)
			document.getElementById('widget-message').innerText = 'Prišlo je do napake pri plačilu. Prosimo, obvestite nas na admin@finance.si .'
			var da = {op:'zlick_callback',mode:'payment_failed',userid:0,sessid:770673788,reqid:'8ebcddd2-bf70-419e-a479-ca42c98212f4',reason:message};
			$.ajax({
				url: 'https://narocilnice.finance.si/master.php',
				data: da,
				dataType:'jsonp',
				crossDomain: true,
			});

		}
	}).render('#zlick-widget') // CSS selector of element that will contain zlick widget.
	});
}
$(document).ready(function(){
console.log('zlick part:',"");
 //

	var zlick_reg_form = '<p id="step1desc"><b><i class="icon-lock" title="naročilo"></i> &nbsp; Do naročnine v dveh korakih. Brez vezave – naročilo velja do pisnega preklica.</b></p><p>Vpišite elektronski naslov, s katerim želite opraviti naročilo:</p><form id="zlick-reg" class="ajaxy"><input type="hidden" name="op" value="zlick_reg" /><input type="hidden" name="target" value= /><input type="text" name="email" placeholder="Vnesite svoj elektronski naslov" value="" /><br id="pass" clear="both" /><input id="pass" type="hidden" name="pass" placeholder="Vnesite vaše geslo" value="" /><button type="submit" id="subscribe" class="login-form-control">Naprej</button></form><br /><div class="clear"></div><div class="other_payments"><button id="sendpass" type="button">Pozabljeno geslo</button></div><br /><div class="clear"></div><div class="other_payments"><a class="other_payments_link" href="https://narocilnice.finance.si/narocilo/50_DIGITAL-1EUR?utm_source=finance&utm_medium=cta&utm_campaign=hardlock&utm_content=finArticle_8963254">Druge možnosti plačila</a></div><div class="form_result"></div><br clear="both" /><p style="font-size:12px;" class="createUser">Če ne obstaja, bomo ob naročilu za vas ustvarili uporabniško ime za dostop do vsebin.<br />S klikom na gumb Naprej sprejemate <a style="text-decoration:underline;color:maroon;" target="_blank" href="https://www.finance.si/pogoji">Splošne pogoje</a>. Za pomoč pokličite 01 30 91 475 ali pišite na <a style="text-decoration:underline;color:maroon;" href="mailto:admin@finance.si">admin@finance.si</a>)</p>';
	$('#zlick-email-reg').show();
	$('#zlick-widget').hide();
	$('.hl_gumb a').attr('href', '#zlick')
  $('.hl_gumb').on('click',function(e){
		e.preventDefault();
    console.log('zlick activate');  
    //$('.ct_block').hide();
		if(typeof zlick_reg_form != 'undefined'){
			$('#zlick-email-reg').append( zlick_reg_form );
			$( "#start_again" ).on('click',function(e){
				event.preventDefault();
				$('#zlick-email-reg .form_result').html('');
				$('#zlick-email-reg .form_result').removeClass("msg");
				$('#zlick-email-reg input[type="password"]#pass').each (function() { this.type = 'hidden'; });
				$('#zlick-email-reg button#subscribe').html('Naprej');
				$('#zlick-email-reg button#sendpass').toggle();
				$('#zlick-email-reg p#step1desc').toggle();
				//$('#zlick-email-reg button#start_again').toggle();
			});
			$( "#sendpass" ).on('click',function(e){
				event.preventDefault();
				$('#zlick-email-reg p#step1desc').toggle();
				$.post('/master.php',{
					op:'form_sendpass',
					target: 'http://www.finance.si%2F8963254%2FJavni-sektor-od-drzave-terja-75-milijonov-evrov-koronadodatko%3Fcctest%26',
					username: $('form#zlick-reg input[name=email][type=text]').val()
				},function(res){
          if (res.error){
            //alert(res.error);
            $('#zlick-email-reg .form_result').html(res.error);
            $('#zlick-email-reg .form_result').addClass("msg");
					}
					if (res.html){
						$('#zlick-email-reg .form_result').html(res.html);
            $('#zlick-email-reg .form_result').addClass("msg");
					}
        },'json');
        return false;
			});
			$( "#zlick-reg" ).submit(function( event ) {
				event.preventDefault();
				$('#zlick-email-reg .form_result').html("Prosimo počakajte trenutek. Preverjamo ali elektronski naslov že obstaja...");
				$.post('/master.php',{
					op:'zlick_reg',
					target: 'http://www.finance.si%2F8963254%2FJavni-sektor-od-drzave-terja-75-milijonov-evrov-koronadodatko%3Fcctest%26',
					email: $('form#zlick-reg input[name=email][type=text]').val(),
					pass: $('form#zlick-reg input[name=pass][type=password]').val(),
					//terms_must: $('#user_create_form input[name=terms_must]').val(),
					//terms_agree: $('#user_create_form input[name=terms_agree]:checked').val(),
				},function(res){
					if (res.error){ 
						//alert(res.error);
						$('#zlick-email-reg .form_result').html(res.error);
						$('#zlick-email-reg .form_result').addClass("msg");
						$('#zlick-email-reg p#step1desc').show();						
					}
					//else { window.location.reload();}
					if (res.email_exists){
						//$('#zlick-email-reg .createUser').prepend(res.html);
						$('#zlick-email-reg .form_result').html(res.html);
						$('#zlick-email-reg .form_result').addClass("msg");						
						$('#zlick-email-reg input[type="hidden"]#pass').each (function() { this.type = 'password'; });
						$('#zlick-email-reg button#subscribe').html('Prijava');
						$('#zlick-email-reg button#sendpass').show();
						$('#zlick-email-reg br#pass').show();
						$('#zlick-email-reg p#step1desc').hide();						
						//$('#zlick-email-reg button#start_again').toggle();						
					} 
					if( res.bad_password ){
						//$('#zlick-email-reg .createUser').prepend(res.html);
						$('#zlick-email-reg .form_result').html(res.html);
						$('#zlick-email-reg .form_result').addClass("msg");
						$('#zlick-email-reg p#step1desc').hide();						
					}
					if( res.login_success) {
						$('#zlick-email-reg .form_result').removeClass("msg");					
						$('#zlick-email-reg .form_result').html('');
						$('#zlick-email-reg p#step1desc').hide();						
						setTimeout(function(){
							window.location.href += "#zlick";
							location.reload(1);
						}, 1000);
					}
				},'json');
				return false;    				
			});			
		}
		$('#article-teaser-blocks').hide();
				$('#zlick-widget').after( $('#trust-badge') );
		$('#trust-badge').show();
		//$('#block_10134').show();
    return false;
  });
}); 
