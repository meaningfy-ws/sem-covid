var url_root = "/";



function show_js_login_form() {
js_lf = document.getElementById('js_login_form');

	if (js_lf.style.display == "none")
	{
		js_lf.style.display = "inline";
	}

}

function swap(id)
{
	
	//if(lastElement!=elm) cls.remove(lastElement,"selected");
elm = document.getElementById(id);
	

	if (elm.style.display == "")
	{
		elm.style.display = "none";
		//cls.remove(elm,"selected");
	}
	else
	{
		elm.style.display = "";
		
		//cls.add(elm,"selected");
	}

}

function pass_input(pass, txt_pass)
{
	
// ked ideme focus na text_pass tak zobraz naozajsty pass
elm = document.getElementById(pass);
	
  if (elm.style.display == "none") {
    elm.style.display = "inline";
  }
document.forms['formular'].password.focus();

// a povodny text pass skovaj
elm_txt = document.getElementById(txt_pass);

//elm_txt.blur();

  if (elm_txt.style.display == "inline") {
    elm_txt.style.display = "none";
  } else {
    elm_txt.style.display = "";
  }

}

</SCRIPT>

