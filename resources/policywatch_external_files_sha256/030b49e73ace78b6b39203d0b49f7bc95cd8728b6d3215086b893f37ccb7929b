      <h2 class="panel-title">
         <a role="button" data-toggle="collapse" href="#PP3Contents" aria-expanded="true"
            aria-controls="PP3Contents"
            class=" "
            onclick="createDocPartCookie(this);">Multilingual display</a>
      </h2>
   </div>
   <div id="PP3Contents" class="panel-collapse collapse in" role="tabpanel"
        aria-labelledby="PP3">
      <div class="panel-body PanelBodyB">
         <form action="./../../../legal-content/EN/TXT/?uri=&#xA;&#x9;&#x9;&#x9;&#x9;CELEX:52020DC0318"
               class="form-inline"
               id="form"
               name="multilingualForm"
               method="post"><input type="hidden" name="_csrf" value="a1e10209-7f17-498b-becf-13ae24ea2ef4" /><input type="hidden" name="from" value="EN"/>
            <div class="form-group">
               <label for="MDLang1" class="sr-only">Language 1 </label>
               <select id="MDLang1" name="lang1" class="input-sm form-control NoBorders">
                  <option value="BG">Bulgarian (bg)</option>
                  <option value="ES">Spanish (es)</option>
                  <option value="CS">Czech (cs)</option>
