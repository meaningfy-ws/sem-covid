    <script>
        $(document).ready(function () {
            setTimeout(function () {
                var keys = $('#edit-keys').val();
                if ($('.language-link.is-active').length > 0) {
                    var ln = $('.language-link.is-active').html().toLowerCase();
                    $('#search_link').attr('href', "/" + ln + "/" + (ln === "fr" ? "rechercher" : "zoeken") + "/groups?keys=" + keys);
                    $('.language-link').each(function () {
                        if ($(this).attr('hreflang') === ln) {
                            $(this).hide();
                        }
                    });
                } else {
                    $('#search_link').attr('href', "/fr/rechercher/groups?keys=" + keys);
                    $('.language-link').each(function () {
                        if ($(this).attr('hreflang') === "fr") {
                            $(this).hide();
                        }
                    });
                }
            }, 500);
        });
    </script>
    
