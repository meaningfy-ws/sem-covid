<script>
    // if no witness of done
    window.addEventListener('load', function () {
        const _form = document.getElementById("surveyForm");
        const _content = document.getElementById("surveyContent");
        const _thanks = document.getElementById("surveyThanks");
        const _formSubject = _form.dataset.survey;
        const cname = 'survey';

        function getCookie(cname) {
            let name = cname + "=";
            let decodedCookie = decodeURIComponent(document.cookie);
            let ca = decodedCookie.split(';');
            for (let i = 0; i < ca.length; i++) {
                let c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return false;
        }

        function surveyX(a) {
            var expiryDate = new Date();
            expiryDate.setMonth(expiryDate.getMonth() + 6);
            document.cookie = a + "=" + _formSubject + "; expires=" + expiryDate.toGMTString() + "; path=/";
        }

        function validateSurveyForm() {
            var q1filled = false;
            var q2filled = false;
            $('#surveyForm input[type=radio]:checked').each(function () {
                if ($(this).attr('name') === 'csuq_12') {
                    q1filled = true;
                }
                if ($(this).attr('name') === 'csuq_16') {
                    q2filled = true;
                }
            });
            return (q1filled || q2filled);
        }

        if (getCookie(cname) === false) {
            let _toggle = document.getElementById('surveyToggle');
            let _close = document.getElementById('surveyButtonClose');
            let _send = document.getElementById('submit_survey');

            setTimeout(function () {
                $('#surveyForm').css({'visibility': 'visible'});
                $('#surveyForm').show();
            }, 20000);
            //setTimeout(() => _form.style.visibility = "visible", 200);
            _toggle.style.cursor = "pointer";
            _toggle.onclick = () => _form.classList.toggle('open');
            _close.onclick = () => {
                _form.remove();
                surveyX(cname);
            }
            _send.onclick = () => {
                if (validateSurveyForm()) {
                    surveyX(cname);
                    _content.style.display = "none";
                    _thanks.style.display = "block";
                    setTimeout(function () {
                        $.ajax({
                            url: '/submit/survey',
                            type: 'POST',
                            data: $('#surveyForm').serialize()
                        });
                        $('#surveyForm').hide();
                    }, 3000);
                    //setTimeout(() => _form.style.display = "none", 3000);
                } else {
                    $('#lab_survey__csuq_12, #lab_survey__csuq_16').css({'color': 'red'});
                    return false;
                }
            }
        }
    });
</script>

