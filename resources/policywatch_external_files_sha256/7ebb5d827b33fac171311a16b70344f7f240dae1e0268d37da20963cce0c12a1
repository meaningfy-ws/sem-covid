<script>
$(document).ready(function() {
    var $billboard = $("#superheader");
    var $wpEl = $("#superheader");
    var $wpElx = $("#stopx");

    var wpElHeight = $wpEl.height();
    var stickyMenuHeight = $("#superheader").height() - 1;

    var $skin = $(".das-bg");
    $skin.css({
    top: stickyMenuHeight
    });

    var handleSkinOffset = function(e) {
        var offset = window.pageYOffset;
        $skin.css({
            top: wpElHeight - offset
        })
    }

    window.addEventListener('scroll', handleSkinOffset);
    var waypoint = new Waypoint({
      element: $wpElx[0],
      handler: function(direction) {
      if (direction === "down") {
        window.removeEventListener('scroll', handleSkinOffset);
        $skin.css({
        top: 0
        });
      } else if (direction === "up") {
        window.addEventListener('scroll', handleSkinOffset);
        }
      },
      offset: stickyMenuHeight
      })

      var handleScroll = function() {
      var scrollTop = $(window).scrollTop();
        if (scrollTop >= $wpElx.offset().top + $wpElx.height()) {
          document.removeEventListener('scroll', handleScroll, false);
        }
      }
      handleScroll();
      document.addEventListener('scroll', handleScroll, false);
});

</script>


