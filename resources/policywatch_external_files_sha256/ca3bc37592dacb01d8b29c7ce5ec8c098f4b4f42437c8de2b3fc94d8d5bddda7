<script type="text/javascript">
    // <![CDATA[
    $(document).ready(function () {
        $('#btn-accetta-cookie').click(function () {
            var d = new Date(2050, 0);
            document.cookie = "__COOKIEALERTLAVOROISTITUZIONALE=OK" +
                          "; expires=" + d.toGMTString() +
                          "; path=/";
            fade();
        });

        $('#btn-close-cookie').click(function () {
            fade();
        });

        setTimeout(function () { fade(); }, 30000);

    });

    function fade() {
        $('#div-alert-cookie').fadeOut('fast');
    }
    // ]]>
</script>
<script type="text/javascript">
    /* <![CDATA[ */

    if (typeof mInstance === "undefined") {
        var mInstance = null;
    }

    if (typeof (Sys) != "undefined") {
        if (mInstance == null)
            mInstance = Sys.WebForms.PageRequestManager.getInstance();

        mInstance.add_pageLoaded(function (sender, args) {
            if ($.cookie('__COOKIEALERTLAVOROISTITUZIONALE')) {
                $('#div-alert-cookie').hide();
            } else {
                $('#div-alert-cookie').show();
            }
        });
    }
    /* ]]> */
</script>

<script type="text/javascript">

    function apriPopUpPrivacy() {

        var element = document.createElement('div');
        element.innerHTML = $("#testo-privacy").html();

        var titolo = $("#titolo-privacy").html();

        var options = {
            title: titolo,
            //width: 600,
            //height: 800,
            html: element
        };

        //SP.UI.ModalDialog.showModalDialog(options);
        SP.SOD.execute('sp.ui.dialog.js', 'SP.UI.ModalDialog.showModalDialog', options);

    }


</script>

