<script type="text/javascript">
    $(document).ready(function () {
        var config = {
            "id": '468341409724915713',
            "domId": 'twitter-feed',
            "maxTweets": 5,
            "enableLinks": true,
            "showImages": false,
            "showUser": false,
            "showTime": false,
            "lang": "it",
            "customCallback": function (tweets) {

                var x = tweets.length;
                var n = 0;
                var element = document.getElementById('twitter-feed');
                var html = '<ul>';
                while (n < x) {
                    html += '<li>' + tweets[n] + '</li>';
                    n++;
                }
                html += '</ul>';
                element.innerHTML = html;

                $("#twitter-feed > ul").owlCarousel({
                    singleItem: true,
                    slideSpeed: 200,
                    paginationSpeed: 800,
                    autoPlay: true,
                    pagination: false,
                    stopOnHover: true
                });

                var owl = $("#twitter-feed > ul").data('owlCarousel');

                $(".twitter-bar .controls #twitter-prev").click(function () {
                    owl.prev();
                });

                $(".twitter-bar .controls #twitter-next").click(function () {
                    owl.next();
                });
            }
        };
        twitterFetcher.fetch(config);
    });
</script>

