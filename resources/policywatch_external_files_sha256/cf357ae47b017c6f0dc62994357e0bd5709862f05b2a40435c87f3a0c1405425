<script>
    function isCookieTypeAllowed(cookieType) {
        gdprCookieValue = JSON.parse(getCookie(cookieConfig.settings.GDPRCookieName));
        if (gdprCookieValue[cookieType] != undefined) {
            return gdprCookieValue[cookieType];
        }
        else {
            return false;
        }
    }

    function isCookieAllowed(cookieName) {
        gdprCookieValue = JSON.parse(getCookie(cookieConfig.settings.GDPRCookieName));
        if (gdprCookieValue) {
            if (Object.keys(cookies).some(function(cookieType) {
                if (cookies[cookieType].some(function(cookie) {
                    return cookie.name === cookieName;
                })) {
                    if (gdprCookieValue[cookieType] == true) {
                        return true;
                    }
                }
                return false;
            })) {
                return true;
            }
        }
        return false;
    }

    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        var expires = "expires=" + d.toUTCString();
        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    }

    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) === ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) === 0) {
                return c.substring(name.length, c.length);
            }
        }
        return false;
    }

    function deleteCookie(name, domain, path) {
        if (domain === window.location.hostname) {
            document.cookie = name + '=;expires=Thu, 01 Jan 1970 00:00:00 GMT;path=' + path;
        } else {
            if (domain instanceof Array) {
                domain.forEach(function (domain) {
                    document.cookie = name + '=;expires=Thu, 01 Jan 1970 00:00:00 GMT;domain=' + domain + ';path=' + path;
                });
            } else {
                document.cookie = name + '=;expires=Thu, 01 Jan 1970 00:00:00 GMT;domain=' + domain + ';path=' + path;
            }
        }
    }

    function zeraxoGDPRCookies(config) {

        var cookieName = config.settings.GDPRCookieName;
        var cookieExpiration = config.settings.GDPRCookieExpiration;
        var cookieValue;
        var cookieRun;
        var cookieLegacy;

        /**
         * Lang
         */
        var lang = config.settings.lang.langFunction();
        if (config.settings.lang.allLang.indexOf(lang) !== -1) {
            config.settings.lang.currentLang = lang;
        }

        /**
         * Logic
         */
        cookieRun = {};
        config.cookies.forEach(function (cookie) {
            cookieRun[cookie.type] = false;
        });

        cookieValue = JSON.parse(getCookie(cookieName));
        if (cookieValue) {
            cookiesOnAccept();
            cookieLegacy = false;
        } else {
            console.log('No Cookie!');
            console.log('Setting Cookie...');

            cookieLegacy = true;
            cookieValue = {};
            config.cookies.forEach(function (cookie) {
                if (cookie.type == "default") {
                    cookieValue[cookie.type] = true;
                }
                else {
                    cookieValue[cookie.type] = false;
                }
            });
            document.getElementsByTagName('body')[0].appendChild(createZeraxoCookieBar(config));
        }
        document.getElementsByTagName('body')[0].appendChild(createZeraxoCookiePopUp(config));
        //document.getElementsByTagName('head')[0].appendChild(createZeraxoCookieStyle());

        var settings = document.querySelectorAll('.' + config.settings.GDPRSettingsClass);
        for (var i = 0; i < settings.length; i++) {
            settings[i].addEventListener('click', function () {
                document.querySelector('#zeraxo-cookie-popup-container').classList.add('visible');
            });
        }

        /**
         * Run Accepted Functions
         */
        function cookiesOnAccept() {
            setCookie(cookieName, JSON.stringify(cookieValue), config.settings.GDPRCookieExpiration);
            config.cookies.forEach(function (cookie) {
                if (cookieValue.hasOwnProperty(cookie.type) && cookieRun.hasOwnProperty(cookie.type)) {
                    if (cookieValue[cookie.type] === true && cookieRun[cookie.type] === false) {
                        cookie.onAccept();
                        cookieRun[cookie.type] = true;
                    } else if (cookieValue[cookie.type] === false && cookieRun[cookie.type] === true) {
                        cookie.onRevoke();
                        cookieRun[cookie.type] = false;
                    } else if (cookieLegacy && cookieValue[cookie.type] === false) {
                        cookie.onRevoke();
                    }
                }
            });
            if (reload === true) {
                location.reload();
            }
        }

        /**
         * Create Styles
         */
        function createZeraxoCookieStyle() {

            return createElement({
                tag: 'link',
                text: '',
                attributes: {
                    type: 'text/css',
                    href: 'zeraxo-cookie.css',
                    rel: 'stylesheet'
                }
            });
        }


        /**
         * Create Zeraxo Cookie Bar
         */
        function createZeraxoCookiePopUp(config) {

            var tabs = [];
            var tabsContent = [];

            config.cookies.forEach(function (cookie, index) {

                if (config.design.version === 'v1') {
                    tabs.push(createElement({
                        tag: 'li',
                        classes: function () {
                            if (index === 0) {
                                return ['tab', 'active'];
                            } else {
                                return 'tab';
                            }
                        }(),
                        style: '',
                        events: {
                            click: function () {
                                var active = document.querySelectorAll("#zeraxo-cookie-popup .active");
                                active[0].classList.toggle('active');
                                active[1].classList.toggle('active');
                                this.classList.add('active');
                                document.querySelectorAll('.zeraxo-cookie-popup-tab-content')[index].classList.toggle('active');
                            }
                        },
                        child: createElement({
                            tag: 'p',
                            style: '',
                            text: cookie.label[config.settings.lang.currentLang]
                        })
                    }));
                }



                tabsContent.push(createElement({
                    tag: 'div',
                    classes: function () {
                        var classes = ['zeraxo-cookie-popup-tab-content', config.design.version];
                        if (index === 0) {
                            classes.push('active');
                        }
                        return classes;
                    }(),
                    style: 'border-color:' + config.design.popup.tabs.borderColor,
                    child: [
                        createElement({
                            tag: 'div',
                            classes: 'zeraxo-cookie-popup-form-container',
                            child: [
                                createElement({
                                    tag: 'h3',
                                    style: '',
                                    text: cookie.label[config.settings.lang.currentLang]
                                }),
                                createElement({
                                    tag: 'input',
                                    attributes: function () {
                                        var attributes = {
                                            id: 'box-' + index,
                                            type: 'checkbox',
                                            value: cookie.type,
                                            checked: true
                                        };
                                        if (cookie.type === 'default') {
                                            attributes.disabled = true;
                                        }
                                        if (cookieValue.hasOwnProperty(cookie.type) && cookieValue[cookie.type] === false) {
                                            delete (attributes.checked);
                                        }
                                        return attributes;
                                    }()
                                }),
                                createElement({
                                    tag: 'label',
                                    attributes: {
                                        for: 'box-' + index
                                    }
                                })
                            ]
                        }),
                        createElement({
                            tag: 'p',
                            style: 'color:' + config.design.popup.textColor,
                            text: cookie.description[config.settings.lang.currentLang]
                        }),
                        createElement({
                            tag: 'ul',
                            classes: 'zeraxo-cookie-popup-cookielist',
                            style: 'color:' + config.design.popup.textColor,
                            text: '',
                            child: function () {
                                var children = [];

                                children.push(createElement({
                                    tag: 'h3',
                                    style: 'color:' + config.design.popup.textColor,
                                    text: config.labels.popup.cookieList[config.settings.lang.currentLang]
                                }));

                                var cookiesToShow = [];
                                switch (cookie.type) {
                                    case 'default':
                                        cookiesToShow = cookies.default;
                                        break;
                                    case 'functional':
                                        cookiesToShow = cookies.functional;
                                        break;
                                    case 'targeting':
                                        cookiesToShow = cookies.targeting;
                                        break;
                                    default:
                                    // code block
                                }

                                for (var i = 0; i < cookiesToShow.length; i++) {
                                    var cookieInfo = cookiesToShow[i];
                                    var cookieTitle = cookieInfo.name + " (" + cookieInfo.domain + ")";
                                    var cookieDescription = "";
                                    if (cookieInfo.description) {
                                        if (cookieInfo.description[config.settings.lang.currentLang]) {
                                            cookieDescription = cookieInfo.description[config.settings.lang.currentLang];
                                        }
                                    }
                                    var expirationAndParty = getExpirationAndPartyText(cookieInfo, config.settings.lang.currentLang);
                                    if (expirationAndParty.length > 0) {
                                        expirationAndParty = "- " + expirationAndParty;
                                    }
                                    children.push(createElement({
                                        tag: 'li',
                                        classes: 'zeraxo-cookie-popup-cookieinfo',
                                        style: 'color:' + config.design.popup.textColor,
                                        text: cookieTitle,
                                        child: [
                                            createElement({
                                                tag: 'div',
                                                classes: 'zeraxo-cookie-popup-expiration-party',
                                                style: 'color:' + config.design.popup.textColor,
                                                text: expirationAndParty,
                                            }),
                                            createElement({
                                                tag: 'div',
                                                classes: 'zeraxo-cookie-popup-cookiedescription',
                                                style: 'color:' + config.design.popup.textColor,
                                                text: cookieDescription
                                            })
                                        ]
                                    }));
                                }
                                return children;
                            }()
                        })
                    ]
                }));
            });

            var tabContainer = [
                createElement({
                    tag: 'div',
                    id: 'zeraxo-cookie-popup-tabs-content-container',
                    classes: config.design.version,
                    child: tabsContent
                })
            ];

            if (config.design.version === 'v1') {
                tabContainer.unshift(createElement({
                    tag: 'div',
                    classes: 'vertical-divider'
                }));
                tabContainer.unshift(createElement({
                    tag: 'div',
                    id: 'zeraxo-cookie-popup-tabs-container',
                    child: createElement({
                        tag: 'ul',
                        id: 'zeraxo-cookie-popup-tabs',
                        child: tabs
                    })
                }));
            }

            return createElement({
                tag: 'div',
                id: 'zeraxo-cookie-popup-container',
                child: createElement({
                    tag: 'div',
                    id: 'zeraxo-cookie-popup',
                    style: 'background-color:' + config.design.popup.backgroundColor,
                    child: [
                        createElement({
                            tag: 'div',
                            id: 'zeraxo-cookie-popup-top',
                            style: 'border-color:' + config.design.popup.borderColor,
                            child: [
                                createElement({
                                    tag: 'div',
                                    id: 'zeraxo-cookie-popup-image-container',
                                    style: 'border-color:' + config.design.popup.borderColor,
                                    child: createElement({
                                        tag: 'img',
                                        attributes: {
                                            src: config.design.popup.logo
                                        }
                                    })
                                }),
                                createElement({
                                    tag: 'div',
                                    id: 'zeraxo-cookie-popup-title',
                                    child: createElement({
                                        tag: 'h4',
                                        style: '',
                                        text: config.labels.popup.title[config.settings.lang.currentLang]
                                    })
                                }),
                                createElement({
                                    tag: 'div',
                                    classes: 'zeraxo-button-dismiss',
                                    child: createElement({
                                        tag: 'a',
                                        style: 'color:' + config.design.popup.borderColor,
                                        events: {
                                            click: function () {
                                                document.querySelector('#zeraxo-cookie-popup-container').classList.remove('visible');
                                            }
                                        }
                                    })
                                })
                            ]
                        }),
                        createElement({
                            tag: 'div',
                            id: 'zeraxo-cookie-popup-body',

                            style: 'min-height:' + config.design.popup.minHeight,
                            child: tabContainer
                        }),
                        createElement({
                            tag: 'div',
                            id: 'zeraxo-cookie-popup-more-info',
                            child: [
                                createElement({
                                    tag: 'p',
                                    id: 'zeraxo-cookie-popup-more-info-text',
                                    text: '<p><span>Više informacija o kolačićima pronađite na <a href="/vise-o-kolacicima/">https://www.sberbank.hr/vise-o-kolacicima/</a></span></p>'
                                })
                            ]
                        }),
                        createElement({
                            tag: 'div',
                            id: 'zeraxo-cookie-popup-bottom',
                            style: 'background-color:' + config.design.popup.footer.backgroundColor,
                            child: [
                                createElement({
                                    tag: 'div',
                                    id: 'zeraxo-cookie-popup-save-button',
                                    child: createElement({
                                        tag: 'a',
                                        style: 'background-color:' + config.design.button.backgroundColor + ';border-color:' + config.design.button.borderColor,
                                        text: config.labels.popup.saveSettings[config.settings.lang.currentLang],
                                        events: {
                                            click: function () {
                                                var inputChecked = document.querySelectorAll('#zeraxo-cookie-popup-container input:checked');
                                                for (var i = 0; i < inputChecked.length; i++) {
                                                    if (cookieValue.hasOwnProperty(inputChecked[i].value)) {
                                                        cookieValue[inputChecked[i].value] = true;
                                                    }
                                                }
                                                var inputUnchecked = document.querySelectorAll('#zeraxo-cookie-popup-container input:not(:checked)');
                                                for (var i = 0; i < inputUnchecked.length; i++) {
                                                    if (cookieValue.hasOwnProperty(inputUnchecked[i].value)) {
                                                        cookieValue[inputUnchecked[i].value] = false;
                                                    }
                                                }
                                                reload = true;
                                                cookiesOnAccept();
                                                var popup = document.querySelector('#zeraxo-cookie-popup-container.visible');
                                                if (popup) popup.classList.remove('visible');
                                                var bar = document.querySelector('#zeraxo-cookie-bar.visible');
                                                if (bar) bar.classList.remove('visible');
                                            }
                                        }
                                    })
                                }),
                                createElement({
                                    tag: 'div',
                                    id: 'zeraxo-cookie-popup-save-button',
                                    style: '',
                                    child: createElement({
                                        tag: 'a',
                                        style: 'background-color:' + config.design.button.backgroundColor + ';border-color:' + config.design.button.borderColor,
                                        text: config.labels.popup.onlyRequired[config.settings.lang.currentLang],
                                        // TODOOOOOOO
                                        events: {
                                            click: function () {
                                                var inputChecked = document.querySelectorAll('#zeraxo-cookie-popup-container input');
                                                for (var i = 0; i < inputChecked.length; i++) {
                                                    cookieValue[inputChecked[i].value] = false;
                                                }
                                                if (cookieValue.hasOwnProperty('default')) {
                                                    cookieValue['default'] = true;
                                                }
                                                reload = true;
                                                cookiesOnAccept();
                                                var popup = document.querySelector('#zeraxo-cookie-popup-container.visible');
                                                if (popup) popup.classList.remove('visible');
                                                var bar = document.querySelector('#zeraxo-cookie-bar.visible');
                                                if (bar) bar.classList.remove('visible');
                                            }
                                        }
                                    })
                                }),
                                createElement({
                                    tag: 'div',
                                    id: 'zeraxo-cookie-popup-save-button',
                                    child: createElement({
                                        tag: 'a',
                                        style: 'background-color: rgb(235, 127, 46); border-color: rgb(235, 127, 46)',
                                        text: config.labels.popup.acceptAll[config.settings.lang.currentLang],
                                        // TODOOOOO
                                        events: {
                                            click: function () {
                                                config.cookies.forEach(function (cookie) {
                                                    cookieValue[cookie.type] = true;
                                                });
                                                reload = true;
                                                cookiesOnAccept();
                                                var popup = document.querySelector('#zeraxo-cookie-popup-container.visible');
                                                if (popup) popup.classList.remove('visible');
                                                var bar = document.querySelector('#zeraxo-cookie-bar.visible');
                                                if (bar) bar.classList.remove('visible');
                                            }
                                        }
                                    })
                                })
                            ]
                        })
                    ]
                })
            });
        }

        /**
         * Create Zeraxo Cookie Bar
         */
        function createZeraxoCookieBar(config) {

            return createElement({
                tag: 'div',
                id: 'zeraxo-cookie-bar',
                style: 'background-color:' + config.design.bar.backgroundColor + ';opacity:' + config.design.bar.opacity,
                classes: ['zeraxo-cookie-reset', config.design.bar.position, 'visible'],
                child: createElement({
                    tag: 'div',
                    classes: 'zeraxo-cookie-container',
                    child: [
                        createElement({
                            tag: 'div',
                            classes: 'zeraxo-cookie-info-container',
                            child: createElement({
                                tag: 'p',
                                style: 'color:' + config.design.bar.textColor,
                                text: config.labels.bar.text[config.settings.lang.currentLang]
                            })
                        }),
                        createElement({
                            tag: 'div',
                            classes: 'zeraxo-cookie-buttons-container',
                            child: [
                                createElement({
                                    tag: 'div',
                                    classes: 'zeraxo-button-dismiss',
                                    child: createElement({
                                        style: 'color:' + config.design.bar.textColor,
                                        tag: 'a'
                                    }),
                                    events: {
                                        click: function () {
                                            document.querySelector('#zeraxo-cookie-bar.visible').classList.remove('visible');
                                        }
                                    }
                                }),
                                createElement({
                                    tag: 'div',
                                    classes: 'zeraxo-button-allow',
                                    child: [
                                        //createElement({
                                        //    tag: 'i',
                                        //    style: 'border-color:' + config.design.textColor
                                        //}),
                                        createElement({
                                            tag: 'a',
                                            style: 'background-color:' + config.design.bar.button.backgroundColor + ';border-color:' + config.design.bar.button.borderColor
                                                + ';color:' + config.design.bar.button.textColor,
                                            text: ' ' + config.labels.bar.settings[config.settings.lang.currentLang],
                                            events: {
                                                click: function () {
                                                    document.querySelector('#zeraxo-cookie-popup-container').classList.add('visible');
                                                }
                                            }
                                        })
                                    ]
                                }),
                                createElement({
                                    tag: 'div',
                                    classes: 'zeraxo-button-allow',
                                    child: createElement({
                                        tag: 'a',
                                        text: config.labels.bar.acceptAll[config.settings.lang.currentLang],
                                        style: 'background-color:' + config.design.bar.button.backgroundColor + ';border-color:' + config.design.bar.button.borderColor
                                            + ';color:' + config.design.bar.button.textColor,
                                        events: {
                                            click: function () {
                                                config.cookies.forEach(function (cookie) {
                                                    cookieValue[cookie.type] = true;
                                                });
                                                reload = true;
                                                cookiesOnAccept();
                                                document.querySelector('#zeraxo-cookie-bar.visible').classList.remove('visible');
                                            }
                                        }
                                    })
                                })
                            ]
                        })
                    ]
                })
            });
        }
        function createElement(options) {
            var element = document.createElement(options.tag);
            if (options.id) element.id = options.id;
            if (options.attributes) {
                for (var key in options.attributes) {
                    if (options.attributes.hasOwnProperty(key)) {
                        element.setAttribute(key, options.attributes[key]);
                    }
                }
            }
            if (options.classes) {
                if (options.classes instanceof Array) {
                    options.classes.forEach(function (cssClass) {
                        element.classList.add(cssClass);
                    });
                } else {
                    element.classList.add(options.classes);
                }
            }
            if (options.style) element.style.cssText += options.style;
            if (options.text) element.innerHTML = options.text;
            if (options.child) {
                if (options.child instanceof Array) {
                    options.child.forEach(function (node) {
                        if (node.nodeType === 1) {
                            element.appendChild(node);
                        }
                    })
                } else {
                    if (options.child.nodeType === 1) {
                        element.appendChild(options.child);
                    }
                }
            }
            if (options.events) {
                for (var key in options.events) {
                    if (options.events.hasOwnProperty(key)) {
                        element.addEventListener(key, options.events[key])
                    }
                }
            }
            return element;
        }
    }

    function getExpirationAndPartyText(cookieInfo, language) {
        var text = "";
        if (cookieInfo.expirationType == 'temporary') {
            if (cookieInfo.party == 'first') {
                if (language == 'hr') {
                    text = "Privremeni kolačić prve strane";
                }
                else {
                    text = "Temporary first party cookie";
                }
            }
            else if (cookieInfo.party == 'third') {
                if (language == 'hr') {
                    text = "Privremeni kolačić treće strane";
                }
                else {
                    text = "Temporary third party cookie";
                }
            }
            else if (cookieInfo.party == '') {
                if (language == 'hr') {
                    text = "Privremeni kolačić";
                }
                else {
                    text = "Temporary cookie";
                }
            }
        }
        else if (cookieInfo.expirationType == 'permanent') {
            if (cookieInfo.party == 'first') {
                if (language == 'hr') {
                    text = "Trajni kolačić prve strane";
                }
                else {
                    text = "Permanent first party cookie";
                }
            }
            else if (cookieInfo.party == 'third') {
                if (language == 'hr') {
                    text = "Trajni kolačić treće strane";
                }
                else {
                    text = "Permanent third party cookie";
                }
            }
            else if (cookieInfo.party == '') {
                if (language == 'hr') {
                    text = "Trajni kolačić";
                }
                else {
                    text = "Permanent cookie";
                }
            }
        }
        else if (cookieInfo.expirationType == '') {
            if (cookieInfo.party == 'first') {
                if (language == 'hr') {
                    text = "Kolačić prve strane";
                }
                else {
                    text = "First party cookie";
                }
            }
            else if (cookieInfo.party == 'third') {
                if (language == 'hr') {
                    text = "Kolačić treće strane";
                }
                else {
                    text = "Third party cookie";
                }
            }
        }
        return text;
    }

