    <script src="/Scripts/imagesloaded.pkgd.min.js"></script>
    <script src="/Scripts/nouislider.min.js"></script>
    <script src="/Scripts/wNumb.js"></script>
    <script src="/Scripts/jquery-ui-1.11.4.min.js"></script>
    <script src="/Scripts/bootstrap-select.js"></script>
    <script src="/Scripts/custom.js?20170908"></script>
    
    <script src="/scripts/sweetalert.min.js"></script>


    <!--[if lt IE 10]>
        <script src="/Scripts/Placeholders.min.js"></script>
    <![endif]-->

    <script type="text/javascript">

        //NE BRISATI, ostaviti kad se Sberbank ponovno odluci ovdje staviti nesto da pada po ekranu
        //initializeTopSlideIn();
        //initializeBottomSlideIn();
        //initializeRightSlideIn();

        //slowDownBackgroundAnimation();


        $('#spinnercontainer, #spinner').ajaxStart(function () {
            $(this).fadeIn('fast');
        }).ajaxStop(function () {
            $(this).stop().fadeOut('fast');
        });

        $("#backontop").click(function () {
            $("html, body").animate({ scrollTop: 0 }, 1000);
        });

        var allowButton = $("#acceptcookies");

        if (allowButton != null) {
            allowButton.off('click');
            allowButton.on('click', function () {
                $.ajax({
                    type: 'POST',
                    url: '/Home/AllowCookies',
                    data: "",
                    success: function (response) {
                        if (response.indexOf('[[Success]]') == 0) {
                            //window.location.href = response.replace("[[Success]]", "");
                            $("#cookiesinfo").fadeOut("slow");
                        } else {
                            alert('greska');
                        }
                    },
                    complete: function () {
                        $("form").each(function () { $.data($(this)[0], 'validator', false); });
                        $.validator.unobtrusive.parse("form");
                    },
                    cache: false
                });
            });
        }

        var blockButton = $("#blockcookies");

        if (blockButton != null) {
            blockButton.off('click');
            blockButton.on('click', function () {
                $.ajax({
                    type: 'POST',
                    url: '/Home/BlockCookies',
                    data: "",
                    success: function (response) {
                        if (response.indexOf('[[Success]]') == 0) {
                            //window.location.href = response.replace("[[Success]]", "");
                            $("#cookiesinfo").fadeOut("slow");
                        } else {
                            alert('greska');
                        }
                    },
                    complete: function () {
                        $("form").each(function () { $.data($(this)[0], 'validator', false); });
                        $.validator.unobtrusive.parse("form");
                    },
                    cache: false
                });
            });
        }

        function slowDownBackgroundAnimation() {
            var body = $('body');
            var totalHeight = (document.height !== undefined) ? document.height : document.body.offsetHeight;
            var className = "confetti-background-animated-";

            switch (true) {
                case (totalHeight < 2000):
                    className += '2000';
                    break;
                case (totalHeight < 3000):
                    className += '3000';
                    break;
                case (totalHeight < 4000):
                    className += '4000';
                    break;
                case (totalHeight < 5000):
                    className += '5000';
                    break;
                default:
                    className += '6000';
                    break;
            }

            body.addClass(className);
        }

        function initializeTopSlideIn() {

            var windowHeight = $(window).height();

            var slideInElementTopLeft = document.getElementsByClassName('slide-in-top-left')[0];
            var slideInElementTopRight = document.getElementsByClassName('slide-in-top-right')[0];

            var slideInElementTopLeftHeight = slideInElementTopLeft.getBoundingClientRect().height;
            var slideInElementTopLeftWidth = slideInElementTopLeft.getBoundingClientRect().width;
            var slideInElementTopRightHeight = slideInElementTopRight.getBoundingClientRect().height;

            var slideInElementTopLeftPosition = windowHeight - slideInElementTopLeftHeight;
            var slideInElementTopRightPosition = windowHeight - slideInElementTopRightHeight;

            function slideOneTopCycle() {

                setTimeout(function () {
                    $(slideInElementTopLeft).addClass('transition');
                    slideInElementTopLeft.style.top = slideInElementTopLeftPosition + 'px';
                }, 900);

                setTimeout(function () {
                    $(slideInElementTopLeft).addClass('transition');
                    slideInElementTopLeft.style.top = -(slideInElementTopLeftHeight) + 'px';
                }, 30900);


                setTimeout(function () {
                    $(slideInElementTopRight).addClass('transition');
                    slideInElementTopRight.style.top = slideInElementTopRightPosition + 'px';
                    slideInElementTopRight.style.left = slideInElementTopLeftWidth + 'px';
                }, 3900);

                setTimeout(function () {
                    $(slideInElementTopRight).addClass('transition');
                    slideInElementTopRight.style.top = -(slideInElementTopRightHeight) + 'px';
                    slideInElementTopRight.style.left = '0';
                }, 28900);
            }

            slideOneTopCycle();

            setInterval(function () {

                slideOneTopCycle();

            }, 50000);
        }

        function initializeBottomSlideIn() {

            var slideInElement = document.getElementsByClassName('slide-in-bottom')[0];

            slideInElementHeight = slideInElement.getBoundingClientRect().height;

            slideInElement.style.bottom = -(slideInElementHeight) + 'px';

            slideInElement.style.display = 'block';


            function slideOneCycle() {

                if ($(slideInElement).hasClass('right')) {
                    $(slideInElement).removeClass('right');
                } else {
                    $(slideInElement).addClass('right');
                }

                setTimeout(function () {
                    $(slideInElement).addClass('transition');
                    slideInElement.style.bottom = '0';
                }, 900);

                setTimeout(function () {
                    slideInElement.style.bottom = -(slideInElementHeight) + 'px';
                }, 10900);

            }

            slideOneCycle();

            setInterval(function () {

                slideOneCycle();

            }, 16000);
        }

        function initializeRightSlideIn() {

            var slideInElement = document.getElementsByClassName('slide-in-right')[0];

            slideInElementWidth = slideInElement.getBoundingClientRect().width;

            slideInElement.style.left = -(slideInElementWidth) + 'px';

            slideInElement.style.display = 'block';


            function slideOneCycle() {
                setTimeout(function () {
                    $(slideInElement).removeClass('remove-transition');
                    $(slideInElement).addClass('transition');
                    slideInElement.style.left = '100%';
                }, 900);

                setTimeout(function () {
                    $(slideInElement).removeClass('transition');
                    $(slideInElement).addClass('remove-transition');
                    slideInElement.style.left = '-1500px';
                }, 10900);

            }

            slideOneCycle();

            setInterval(function () {

                slideOneCycle();

            }, 16000);
        }

    </script>

    

</body>
</html>
