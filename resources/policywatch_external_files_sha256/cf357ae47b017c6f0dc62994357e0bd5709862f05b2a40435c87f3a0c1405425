<script type="text/javascript">
    /* Javascript kod za pretragu */
    function newSearch(searchTerm) {
        var href = $('#searchBtnHeader').attr("href");
        window.location = href + '?keywords=' + searchTerm;
    }

    $(document).ready(function () {
        $.widget("custom.catAutocomplete", $.ui.autocomplete, {
            _renderMenu: function (ul, items) {
                var that = this, currentCategory = "";
                $.each(items, function (index, item) {
                    if (item.category !== currentCategory) {
                        ul.append("<li class='ui-autocomplete-category'>" + item.category + "</li>");
                        currentCategory = item.category;
                    }
                    if (currentCategory !== "Nema rezultata") {
                        that._renderItemData(ul, item);
                    }
                });
                if (currentCategory !== "Nema rezultata") {
                    ul.append("<li class='all-search-results'>Prikaži sve rezultate</li>");

                    $('.all-search-results').on('click', function () {
                        var searchTerm = $('#searchTBHeader').val();
                        if (!searchTerm.trim()) {
                            searchTerm = $('#searchTBHeaderMobile').val()
                        }
                        newSearch(searchTerm);
                    });
                }
            }
        });

        $("#searchTBHeader").catAutocomplete({
            source: function (request, response) {
                $.ajax({
                    url: '/umbraco/surface/Search/Search',
                    data: "{ 'param': '" + request.term + "', 'rootId': '" + "1123" + "' }", //maxRows: 5
                    dataType: "JSON",
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    dataFilter: function (data) { return data; },
                    success: function (data) {
                        response($.map(data, function (item) {
                            return {
                                label: item.Name,
                                category: item.Category,
                                url: item.Url
                            }
                        }));
                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown) {
                        alert(textStatus);
                    }
                });
            },
            appendTo: "#searchTBResultsContainer",
            minLength: 3,
            search: function () {
            },
            open: function () {
            },
            close: function () {
            },
            response: function (event, ui) {
                if (ui.content.length === 0) {
                    var noResult = {
                        label: "Nema rezultata",
                        category: "Nema rezultata",
                        url: ""
                    };
                    ui.content.push(noResult);
                }
            },
            select: function (event, ui) {
                window.location = ui.item.url;
            }

        });

        $('#searchTBHeader').keypress(function (e) {
            if (e.keyCode == 13) {
                newSearch($('#searchTBHeader').val());
                return false;
            }
        });

        $('#searchBtnHeader').click(function () {
            newSearch($('#searchTBHeader').val());
            return false;
        });

        $("#searchTBHeaderMobile").catAutocomplete({
            source: function (request, response) {
                $.ajax({
                    url: '/umbraco/surface/Search/Search',
                    data: "{ 'param': '" + request.term + "', 'rootId': '" + "1123" + "' }", //maxRows: 5
                    dataType: "JSON",
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    dataFilter: function (data) { return data; },
                    success: function (data) {
                        response($.map(data, function (item) {
                            return {
                                label: item.Name,
                                category: item.Category,
                                url: item.Url
                            }
                        }))
                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown) {
                        alert(textStatus);
                    }
                });
            },
            appendTo: "#searchTBHeaderMobileContainer",
            minLength: 3,
            search: function () {
            },
            open: function () {
            },
            close: function () {
            },
            response: function (event, ui) {
                if (ui.content.length === 0) {
                    var noResult = {
                        label: "Nema rezultata",
                        category: "Nema rezultata",
                        url: ""
                    };
                    ui.content.push(noResult);
                }
            },
            select: function (event, ui) {
                window.location = ui.item.url;
            }

        });

        $('#searchTBHeaderMobile').keypress(function (e) {
            if (e.keyCode == 13) {
                newSearch($('#searchTBHeaderMobile').val());
                return false;
            }
        });
        $('#searchBtnHeaderMobile').click(function () {
            newSearch($('#searchTBHeaderMobile').val());
            return false;
        });
    });
</script>
<script type="text/javascript">
    /* Javascript kod za pretragu */
    var states = [];
    var autoComplete = '';
    var jezik = 'sr';
    if (jezik == "en") {
        for (var i = 0; i < autoComplete.length; i++) {
            if (autoComplete[i]["TitleEn"] != null)
                states.push(autoComplete[i]["TitleEn"]);
            if (autoComplete[i]["SubTitleEn"] != null)
                states.push(autoComplete[i]["SubTitleEn"]);
        }
    } else {
        for (var i = 0; i < autoComplete.length; i++) {
            if (autoComplete[i]["Title"] != null)
                states.push(autoComplete[i]["Title"]);
            if (autoComplete[i]["SubTitle"] != null)
                states.push(autoComplete[i]["SubTitle"]);
        }
    }

    function newSearch(searchTerm) {
        var href = $('#searchBtnHeader').attr("href");
        window.location = href + '?keywords=' + searchTerm;
    }

    $(document).ready(function () {
        $('#searchTBHeader').typeahead({
            hint: true,
            highlight: true,
            minLength: 1
        },
            {
                name: 'states',
                source: substringMatcher(states)
            });
        $('#searchTBHeaderMobile').typeahead({
            hint: true,
            highlight: true,
            minLength: 1
        },
            {
                name: 'states',
                source: substringMatcher(states)
            });
    });

    var substringMatcher = function (strs) {
        return function findMatches(q, cb) {
            var matches, substringRegex;

            // an array that will be populated with substring matches
            matches = [];

            // regex used to determine if a string contains the substring `q`
            substrRegex = new RegExp(q, 'i');

            // iterate through the pool of strings and for any string that
            // contains the substring `q`, add it to the `matches` array
            $.each(strs, function (i, str) {
                if (substrRegex.test(str)) {
                    matches.push(str);
                }
            });

            cb(matches);
        };
    };
</script>


