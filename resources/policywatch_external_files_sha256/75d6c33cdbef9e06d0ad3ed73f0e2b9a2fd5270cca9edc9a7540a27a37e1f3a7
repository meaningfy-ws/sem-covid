<script>
var oewa_path = "RedCont/Politik/PolitikInland/v1news/stories/grid";
var oewa_unique = "fulltext";
var oewa_postdate = new Date(1584621740000);
var oewa_delta = 7 * 86400000;
if (isNaN(parseInt(oewa_unique)) === false && (new Date() - oewa_postdate) > oewa_delta) {
   oewa_unique = "fulltext";
}
var oewa_data = {
  "cn": "at",
  "st": "at_w_atorf",
  "sv": "in",
  "ps": "lin",
  "sc": "yes"
};
