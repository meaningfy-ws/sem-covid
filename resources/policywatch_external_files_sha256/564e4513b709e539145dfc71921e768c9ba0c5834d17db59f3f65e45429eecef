    <script>
    window.airliners = {
        mobileWidth: 576,
        tabletWidth: 768,
        smallDesktopWidth: 1024,        
        getClientWidth: function() {
            return Math.max( document.documentElement.clientWidth || 0, window.innerWidth || 0 );
        },
        isMobile: function() {
            return this.getClientWidth() < this.tabletWidth;
        },
        isTablet: function() {
            return this.getClientWidth() >= this.tabletWidth && this.getClientWidth() <= window.airliners.smallDesktopWidth;
        },
        viewport: {
            meta: document.getElementById('meta-viewport'),
            standard: document.getElementById('meta-viewport').getAttribute('content'),            
            mobile: 'width=768',
            tablet: 'width=1024',
            desktop: 'width=1280',
            set: function( key ) {
                this.meta.setAttribute( 'content', this[ key ] );
            }
        },
        isDebug:  false ,

        user: null,

        gtag: function() {
            var arg = arguments;

            if(window.airliners.analytics) {
                window.airliners.analytics.apply(null, arguments)
                return ;                
            }

            window.setTimeout(
                function() {
                    window.airliners.gtag.apply(null, arg)
                }, 
                100
            );
        },

        analytics: null,

                consentedVendors: null,
        
        waitForConsent: [],

        whenConsentFor: function(vendor, callback, altCallback) {
            if(window.airliners.consentedVendors) {
                var consent = false;
                
                window.airliners.consentedVendors.forEach(function(name) {
                    if (name.name == vendor) {
                        consent = true
                        callback()
                    }
                });
                
                if(typeof altCallback === 'function' && consent === false) {
                    altCallback()
                }

                return ;                
            }

            window.airliners.waitForConsent.push({
                name: vendor,
                callback: callback,
                altCallback: altCallback,
            });
        },

        callConsent: function() {
            window.airliners.waitForConsent.forEach(function(vendor) {
                if(window.airliners.isDebug) { console.log('check consent for: '+ vendor.name); }
                window.airliners.whenConsentFor(vendor.name, vendor.callback, vendor.altCallback);
            });            
        }
    }

        if ( window.airliners.isTablet() ) {        
        window.airliners.viewport.set( "tablet" );
    }
    
    function Ads_BA_cons() {
        if(window.airliners.consentedVendors) {
            if (window.airliners.isDebug) {
                console.log('consent given: consentedVendors');
            }
            return ;
        }

        if(typeof __tcfapi === 'function') {
            if(window.airliners.isDebug) {
                console.log('consent given: tcfapi');
            }
            __tcfapi('getCustomVendorConsents', 2, function(vendorConsents, success) {
                if(success){
                    window.airliners.consentedVendors = vendorConsents.consentedVendors
                    
                    window.airliners.callConsent();
                } else {
                    window.airliners.consentedVendors = []                        
                }
            });
        }
    }  
</script>    
