<script id="webappScript_12_5b35a6251761e6914203a4a">
    (function() {
        var ie = detectIE();
        var container = document.getElementById("webapp_12_5b35a6251761e6914203a4a");
        var script = document.getElementById("webappScript_12_5b35a6251761e6914203a4a");

        (function() {
            if (ie === false || ie > 11 || cookieExists()) {
                destroy();
                return;
            }

            container.style.display = "";

            var text = container.querySelector(".skv-ie-banner__banner-text");
            text.innerHTML = '<p>Från den 12 maj 2021 fungerar inte vår webbplats med din nuvarande webbläsare Internet Explorer.</p>';

            var linkLabel = container.querySelector(".skv-ie-banner__banner-link-label");
            linkLabel.innerHTML = 'Rekommenderade webbläsare';

            var closeLabel = container.querySelector(".skv-ie-banner__banner-close-label");
            closeLabel.innerHTML = "Stäng";

            var button = container.querySelector(".skv-ie-banner__banner-close");
            button.addEventListener("click", function() {
                addCookie();
                destroy();
            });
        })();

        script.parentElement.removeChild(script);

        function cookieExists() {
            return document.cookie.indexOf("skv-ie-banner=1") !== -1;
        }

        function addCookie() {
            var acceptsCookies = document.cookie.indexOf("SKVCookieAlert=agree") !== -1;
            
            if (acceptsCookies) {
                document.cookie = "skv-ie-banner=1; path=/;";
            }
        }

        function destroy() {
            container.parentElement.removeChild(container);
        }

        function detectIE() {
            var ua = window.navigator.userAgent;

            var msie = ua.indexOf('MSIE ');
            if (msie > 0) {
                // IE 10 or older => return version number
                return parseInt(ua.substring(msie + 5, ua.indexOf('.', msie)), 10);
            }

            var trident = ua.indexOf('Trident/');
            if (trident > 0) {
                // IE 11 => return version number
                var rv = ua.indexOf('rv:');
                return parseInt(ua.substring(rv + 3, ua.indexOf('.', rv)), 10);
            }

            var edge = ua.indexOf('Edge/');
            if (edge > 0) {
                // Edge => return version number
                return parseInt(ua.substring(edge + 5, ua.indexOf('.', edge)), 10);
            }

            // other browser
            return false;
        }
    })();
</script>
