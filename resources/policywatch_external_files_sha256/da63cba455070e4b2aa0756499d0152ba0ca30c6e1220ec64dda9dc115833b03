<script>
   function onSearchButtonMobileClick(){
      var form = document.getElementById("searchformMobile");
      var input = document.getElementById("queryMobile");
      var show = form.style.display !== "block";
      
      form.style.display = show ? "block" : "";
      
      if(show) {
         input.focus();
      } else {
         input.blur();
      }
   }
</script></div>
