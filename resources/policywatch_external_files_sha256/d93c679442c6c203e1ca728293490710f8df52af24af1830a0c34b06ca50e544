    var getWindowOptions = function () {
        var width = 500;
        var height = 350;
        var left = (window.innerWidth / 2) - (width / 2);
        var top = (window.innerHeight / 2) - (height / 2);

        return [
            'resizable,scrollbars,status',
            'height=' + height,
            'width=' + width,
            'left=' + left,
            'top=' + top,
        ].join();
    };
    var url = 'https://www.delo.si/novice/slovenija/poslanci-sprejeli-paket-za-gospodarstvo-s-podtaknjenci/'

    var fbShare = function () {
        FB.ui({
            method: 'share',
            href: url
        }, function (response) {
        });
    };

    var fbMessenger = function () {
        FB.ui({
            method: 'send',
            link: url,
        }, function (response) {
        });
    };

