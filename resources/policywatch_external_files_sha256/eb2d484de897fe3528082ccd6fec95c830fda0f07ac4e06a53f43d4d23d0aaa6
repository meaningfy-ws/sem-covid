			// Creation d'un tag avec envoi en mode securise.
			var tag = new ATInternet.Tracker.Tag();
			// Marquage de la page avec son niveau 2.
			tag.page.set({
	    		level2:'59'
			});
			// Envoi du hit.
			tag.dispatch();
			
			// Pour le chargement du sommaire d'un texte JORF a la demande.
			function buildSummaryOnClick(){
				var data = {
					cid: "JORFTEXT000041746298",
					textCid : "JORFTEXT000041746298",
					searchedString : null,
					summaryFund : "JORF_TEXT"
				};
				buildSummary(data);
			}
		/*]]>*/
