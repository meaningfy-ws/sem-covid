<script language="Javascript">
window.addEventListener("load", function(event) {
 		var x = new Date(document.lastModified);
    document.getElementById("angularAppVersion").innerHTML += ' Last modified: ' + x;
		// piwik
		piwikObject = { siteSection : 'commission presscorner', lang : 'en' };
		if ( document.getElementById("piwikType") !== null){
			piwikObject.siteSection = piwikObject.siteSection + ' ' + document.getElementById("piwikType").value;
		}
		if ( document.getElementById("piwikPArea") !== null){
			piwikObject.siteSection = piwikObject.siteSection + ' ' + document.getElementById("piwikPArea").value;
		}
		if (document.getElementById("piwikLng") !== null){
			piwikObject.lang = document.getElementById("piwikLng").value;
		}
		if (document.getElementById("piwikSearchResult") !== null) {
			if (document.getElementById("piwikSearchResult").value.length){
					piwikObject.search = document.getElementById("piwikSearchResult").value;
			}
    }
    setTimeout(function(){ $wt.trackPageView(piwikObject); }, 2000);

  });
</script>

