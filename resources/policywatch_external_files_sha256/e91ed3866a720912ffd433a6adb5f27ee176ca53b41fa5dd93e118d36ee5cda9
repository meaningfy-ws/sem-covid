<script>var googletag = googletag || {};
googletag.cmd = googletag.cmd || [];
googletag.slots = googletag.slots || {};googletag.dfp_service = {
  ads_enabled: [],
  initialized: false,
};googletag.dfp_service.loadSlots = function() {
  var width = document.documentElement.clientWidth;
  for (adslot in googletag.slots) {
    if (this.ads_enabled.indexOf(adslot) == -1 && (googletag.slots[adslot].breakpoint === undefined || width >= googletag.slots[adslot].breakpoint.from || width < googletag.slots[adslot].breakpoint.to)) {
      googletag.slots[adslot].addService(googletag.pubads());
      this.ads_enabled.push(adslot);
      if (this.initialized) {
        googletag.pubads().refresh([googletag.slots[adslot]]);
      }
    }
  }
};
googletag.dfp_service.init = function() {
  googletag.cmd.push(function() {
    googletag.pubads().enableSingleRequest();
    googletag.pubads().collapseEmptyDivs();
  });

  googletag.enableServices();
  this.initialized = true;
};
googletag.dfp_service.loadSlots();
googletag.dfp_service.init();
googletag.pubads().addEventListener("slotRenderEnded", function(event) {
  Drupal.behaviors.dfpAds.reactToResize(document.getElementById("google_ads_iframe_" + event.slot.B + "_0__container__"), event.size);
});</script>
