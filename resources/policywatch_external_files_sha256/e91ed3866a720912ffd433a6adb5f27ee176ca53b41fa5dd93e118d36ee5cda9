      <section class="main-content with-aside">
              <div class="node-content">
          <div class="panel-pane pane-views pane-featured-image"  >
  
      
  
  <div class="pane-content">
    <div class="view view-featured-image view-id-featured_image view-display-id-block_1 view-dom-id-1b5665a72a94fbdd402efec9368393a8" >
        
  
  
      <div class="view-content">
        <div>
      
          
  <img src="https://www.yrittajat.fi/sites/default/files/styles/main_image/public/news_main_image/fyspiste_forselius.jpg?itok=ZLYQjr9U" width="750" height="442" alt="Fyspiste Jarmo Forselius" title="Fyspiste Jarmo Forselius" />    </div>
