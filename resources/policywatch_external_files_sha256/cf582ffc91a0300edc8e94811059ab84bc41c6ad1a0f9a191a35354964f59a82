    <script type="text/javascript">
    try {
      adition.srq.push(function(api) {
        api.renderSlot("renderSlot_Right1");
        api.configureRenderSlot("renderSlot_Right1").events.onFinishLoading(function(renderSlotElement, bannerDescriptionApi,renderControlApi) {
          setTimeout(function() { 
            if (tcfData && tcfData.eventStatus == 'cmpuishown') return;
            if (bannerDescriptionApi.getOptions().reload === 'block') return;
            if (!document.body.classList.contains('wallpaper') && !document.getElementById('groupM_iPageSitebar') && document.getElementsByClassName('adform-adbox').length == 0) {
              if (document.hasFocus()) {
                console.log('renderSlot_Right1 hasFocus ' + document.hasFocus());
                api.reload(["renderSlot_Right1"]);
                console.log('renderSlot_Right1 reload');
                window.dataLayer.push({ event: 'renderSlot_Right1 reload', eventAction: 'renderSlot_Right1 reload' });
              }
              else {
                console.log('renderSlot_Right1 hasFocus ' + document.hasFocus());
                window.addEventListener("focus", function reloadOnFocus() {
                  setTimeout(function() { 
                    console.log('renderSlot_Right1 hasFocus ' + document.hasFocus());
                    api.reload(["renderSlot_Right1"])
                    console.log('renderSlot_Right1 reload');
                    window.dataLayer.push({ event: 'renderSlot_Right1 reload', eventAction: 'renderSlot_Right1 reload' });
                    window.removeEventListener("focus", reloadOnFocus);
                  }, 25000);
                });
              }
            }
          }, 25000);
          //api.configureRenderSlot("renderSlot_Right1").events.onFinishLoading(function() { return; });
		})
      });
    } catch (e) { }
    </script>

  </div>
