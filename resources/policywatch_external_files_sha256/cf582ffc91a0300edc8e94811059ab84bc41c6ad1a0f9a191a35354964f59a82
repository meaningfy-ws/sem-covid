<script type="text/javascript">

  function fillPostingBetreff() {
       var postingTextareaText = jQuery('#bd\\[po_text\\]').val();
       if (postingTextareaText.length > 40) {
            postingTextareaText = postingTextareaText.substring(0,40)+"...";
      }
      jQuery('#bd\\[po_title\\]').val(postingTextareaText);
  }

  function replaceUmlaute(string){
    var transformations = [
      ["Ä", "Ae"],
      ["ä", "ae"],
      ["Ö", "Oe"],
      ["ö", "oe"],
      ["Ü", "Ue"],
      ["ü", "ue"],
      ["ß", "ss"],
      ["é", "e"]
    ];

    for (var i=0; i < transformations.length; i++){
      myRegExp = new RegExp(transformations[i][0],"g");
      string = string.replace(myRegExp, transformations[i][1]);
    }
    return string;
  }

  function reportPosting(po_id,po_user,po_created) {

    if (jQuery('#melden_email').val().length < 4 || jQuery('#melden_email').val().indexOf('@') < 1) {
      alert("Bitte geben Sie Ihre E-Mailadresse ein");
      jQuery('#melden_email').focus();
      return false;
    }
    try {
      jQuery('#meldenFormular').hide();
      jQuery('#melden_status').show();
      jQuery('#melden_status').append("Bitte warten...");


      var response = jQuery.ajax({
          url: "/scripts/kommentar_melden.php?username=&sessid=ppu7vs846sg44sq63oiss1juqg",
          type: "GET",
          data: {  po_id: po_id,
            po_thema: " ",
            po_user: po_user,
            po_email: jQuery('#melden_email').val(),
            po_grund: jQuery('#melden_grund').val(),
            po_titel: " ",
            po_text: replaceUmlaute(jQuery('#posting-'+po_id+'-text').html().replace(/]+>/gi, '')),
            ar_titel: replaceUmlaute(jQuery('h1.artDetail__headline').html().replace(/]+>/gi, '')),
            ar_url: location.href
          },
          success:  function(transport) {
          	console.dir(transport);
          	jQuery('#melden_status').html(transport);
          },
          error: function( jqXHR, textStatus ) {
            jQuery('#melden_status').html('<p>Fehler: '+textStatus+'</p>');
          }
        });

    }
    catch(e){
      console.log("Exception:"+e);
    }
  }

  function showNewPostingReportForm(po_id,po_user,po_created) {
      var postingId = po_id;
      var postingUser = po_user;
      var postingCreated = po_created;

      var meldenForm = '<div id="meldenFormularWrap"><div id="meldenFormular" class="kommentarArt__posting__meldenForm"><h3>Kommentar melden</h3><label for="melden_email" class="kommentarArt__posting__label">E-Mail *</label><input name="fr[0]" id="melden_email" size="40" maxlength="50" value="" type="text"><label class="kommentarArt__posting__label" for="melden_grund" style="margin-top:20px;">Warum möchten Sie den Kommentar melden?</label><textarea name="fr[1]" id="melden_grund" rows="4" cols="50" wrap="virtual" class="kommentarArt__posting__textarea"></textarea><input class="button" style="margin-top:10px;" type="button" value="Kommentar melden" name="absenden" class="kommentarArt__posting__textarea" onClick="reportPosting(\''+po_id+'\',\''+po_user+'\',\''+po_created+'\');" /></div><div id="melden_status" class="kommentarArt__posting__meldenForm" style="display:none;"><h3>Kommentar melden</h3></div></div>';

	  jQuery('#meldenFormularWrap').remove();

      jQuery('#posting-'+postingId+'-melden').append(meldenForm);
      jQuery('#posting-'+postingId+'-melden').show();
      jQuery('#meldenFormularWrap').show();

    }
</script>

