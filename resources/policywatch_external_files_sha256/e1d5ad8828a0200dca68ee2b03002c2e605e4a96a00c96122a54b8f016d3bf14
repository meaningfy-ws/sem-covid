          var htmlcontent6 = "<div class='topic'><a  href='"+data.items[5].link + "?utm_source=news.bg&utm_medium=native&utm_campaign=incontent' target='_blank' class='ec-thumb'><img src='" + data.items[5].enclosure + "' class='thumb' alt='" + data.items[5].title + "'></a><h2><a href='"+data.items[5].link + "?utm_source=news.bg&utm_medium=native&utm_campaign=incontent' target='_blank' class='title'>" + data.items[5].title + "</a></h2></div>";
           $(htmlcontent6).appendTo("#syndicated");		   
	
		   
