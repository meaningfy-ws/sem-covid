<script type="text/javascript">

function getCookie(name) {
    // Split cookie string and get all individual name=value pairs in an array
    var cookieArr = document.cookie.split(";");
    
    // Loop through the array elements
    for(var i = 0; i < cookieArr.length; i++) {
        var cookiePair = cookieArr[i].split("=");
        
        /* Removing whitespace at the beginning of the cookie name
        and compare it with the given string */
        if(name == cookiePair[0].trim()) {
            // Decode the cookie value and return
            return decodeURIComponent(cookiePair[1]);
        }
    }
    
    // Return null if not found
    return null;
}

$(function() {

    var firstName = getCookie("newsletter");

    if(firstName != "1") {

 
	$(window).scroll(function(){

    var scrollTop = window.pageYOffset || document.documentElement.scrollTop;
   
    if (scrollTop > 250) {
        $('#slidebox').animate({'right':'0px'},300);
    } else
			$('#slidebox').stop(true).animate({'right':'-430px'},100);
    });
    
};

	$('#slidebox .close').bind('click',function(){
       
        document.cookie = "newsletter=1";
		$(this).parent().remove();
	});
});
</script>
<div class="newsletterboxhidden">
<div id="slidebox" class="">
	<a class="close"></a>
    <p class="headline-box">Jetzt Mitglied werden!</p>
    <p>Unterstütze das Ökosystem, profitiere von der Community und werde Mitglied des Startup-Verbandes.<br />
    <a class="more" target="_blank" href="https://mitglieder.deutschestartups.org/antrag/">Mitgliedsantrag</a></p>
</div>
</div>



<script>

//$(".top-header").next().height($(".top-header").height())
$(window).scroll(function() {
    var scrollTop = window.pageYOffset || document.documentElement.scrollTop;
   
    if (scrollTop > 180) {
       var element = document.getElementById("top-header");
        element.classList.add("hidden");
    }
    else {
        $('div.top-header').css('opacity',35-scrollTop/5);
        $('div.top-nav-left').css('opacity',1-scrollTop/80);
        $('div.top-nav-right').css('opacity',1-scrollTop/80);
        $('div.top-header').css('height',35-scrollTop/5);
        var element = document.getElementById("top-header");
        element.classList.remove("hidden");

       
    }
}).scroll()
    </script>

    <script>
    $(window).scroll(function() {
    var scrollTop = window.pageYOffset || document.documentElement.scrollTop;
   
    if (scrollTop > 10) {
       var element = document.getElementById("main-header");
        element.classList.add("sub");
    }
    else {
        var element = document.getElementById("main-header");
        element.classList.remove("sub");
       
    }
}).scroll()
	</script>

    
