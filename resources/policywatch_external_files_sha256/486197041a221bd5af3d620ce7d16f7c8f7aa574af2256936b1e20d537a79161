	<script type="text/javascript">
	//<![CDATA[
	// Prida polozky do vyberu
	function Add(sSource, sDestination) {
		oSource = document.getElementById(sSource);
		oDestination = document.getElementById(sDestination);
		result = "";
		for (var x = 0; x < oSource.length; x++) {
			for (var i = 0; i < oDestination.length; i++) {
				if (oSource.options[x].selected) {
					if (oSource.options[x].text == oDestination.options[i].text) {
						oSource.options[x].selected=false;
					}
				}
			}
		}
		len = oDestination.length;
		if (len == 1 && oDestination.options[0].text == "kliknite na >>") { len = 0;}
		for (var i = 0; i < oSource.length; i++) {
			if (oSource.options[i].selected) {
				kam = 0;
				for (var x = 0; x < len; x++) {
					for (var y = 0; y < i; y++) {
						if (oDestination.options[x].text == oSource.options[y].text) {
							kam = x + 1;
						}
					}
				}
				
				oDestination.options[len] = new Option(oSource.options[i].text, oSource.options[i].value)
				// poposuvame vsetko od 'kam'
				for (var x = len; x > kam; x--) {
					oDestination.options[x].text = oDestination.options[x-1].text;
					oDestination.options[x].value = oDestination.options[x-1].value;
				}
				oDestination.options[kam].text = oSource.options[i].text;
				oDestination.options[kam].value = oSource.options[i].value;
				oSource.options[i].selected = false;
				len++;
			}
		}
	}

	// Zmaze polozky z vyberu
	function Remove(sSource) {
		oSource = document.getElementById(sSource);
		for (var i = oSource.length-1; i >= 0; i--) {
			if (oSource.options[i].selected) {
				oSource.options[i] = null;
			}
		}
	}

	function SubmitFilter(form) {
		var result = null;
		for (var i = 0; i < form.nazvy_right.length; i++) {
			hodnota = form.nazvy_right.options[i].value - 0;
			if (result) result += "|"+hodnota;
			else result = hodnota;
		}
		form.statistiky_nazvy.value=result;

		result = null;
		for (var i = 0; i < form.obdobie_right.length; i++) {
			hodnota = form.obdobie_right.options[i].value - 0;
			if (result) result += "|"+hodnota;
			else result = hodnota;
		}
		form.statistiky_obdobie.value=result;
		form.submit();
	}
	//]]>
	</script>
	
