  (function() {
    function check() {
      var iframes = top.document.getElementsByTagName('iframe');
      for(var i = 0; i < iframes.length; i++) {
        var ifr = iframes[i];
        if(!ifr.id || ifr.id.indexOf('sas_') < 0) {
          continue;
        }
        try {
          var ifrs = ifr.contentWindow.document.getElementsByTagName('iframe');
          var subIfr = ifrs[0];
          if(subIfr && subIfr.id && subIfr.id.indexOf('google_ads_iframe_') === 0) {
            var width = subIfr.getAttribute('width');
            var height = subIfr.getAttribute('height');
            if(width !== ifr.getAttribute('width')) {
              ifr.setAttribute('width', width);
            }
            if(height !== ifr.getAttribute('height')) {
              ifr.setAttribute('height', height);
            }
          }
        } catch (e) {}
      }
    };
    setInterval(check, 1000);
  })();
