<script type="text/plain" data-usercentrics="INFOnline">
        var sendBasicIVWTracking = function(pageProps, IVWcode) {
            var iam_data = {
                "st": pageProps.advertID,
                "cp": IVWcode,
                "sv": "i2",
                "co": pageProps.siteID + pageProps.contentLabel,
                "sc": "yes"
            };
            if (pageProps.advertID !== '') {
                iom.c(iam_data, 1);
            }
        };

        var initialiseIVWTracking = function (pageProps, IVWcode) {
            
            if (pageProps.advertID !== null) {
                var pageHeader = document.getElementsByTagName('head')[0];
                var ivwScript = document.createElement('script');
                ivwScript.src = location.protocol + '//script.ioam.de/iam.js?m=1';
                pageHeader.appendChild(ivwScript);

                if (ivwScript.readyState) {
                    
                    ivwScript.onreadystatechange = function () {
                        if (ivwScript.readyState === 'loaded' || ivwScript.readyState === 'complete') {
                            ivwScript.onreadystatechange = null;
                            sendBasicIVWTracking(pageProps,IVWcode);
                        }
                    };
                } else {
                    
                    ivwScript.onload = function () {
                        sendBasicIVWTracking(pageProps,IVWcode);
                    };
                }
            }
        };

        var IVWcode = null;
        var pageProps = {
            siteID: 'haufe.de',
            contentLabel: 'Fehlerseite/404',
            advertID: '',
            averageRating: '',
            publicationDate: '',
            documentType: 'Overview'
        };

        var slashIndex = pageProps.contentLabel.indexOf('/');
        if (slashIndex >= 0) {
            IVWcode = pageProps.contentLabel.substring(0, Math.min(12, slashIndex));
        } else {
            IVWcode = pageProps.contentLabel.substring(0, 12);
        }
        initialiseIVWTracking(pageProps, IVWcode);
    </script>
