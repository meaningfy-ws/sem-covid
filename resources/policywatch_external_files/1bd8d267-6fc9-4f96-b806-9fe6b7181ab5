<script>
  $(function () {
    $('#searchComunicados .searchsubmit').click(function () {
      var target ='comunicados' ;
        if (typeof __searchpresstargeturl !== 'undefined')
          target = __searchpresstargeturl;
        DoClientSearch(GetSearchParameters("#searchComunicados", target));
      });

      $('#searchDocs .searchsubmit').click(function () {

        var target ='documentos' ;
        if (typeof __searchdocumentstargeturl !== 'undefined')
          target = __searchdocumentstargeturl;
        DoClientSearch(GetSearchParameters("#searchDocs", target));
      });

      $('#searchIntervencoes .searchsubmit').click(function () {
        var target ='intervencoes' ;
        if (typeof __searchinterventionstargeturl !== 'undefined')
          target = __searchinterventionstargeturl;
        DoClientSearch(GetSearchParameters("#searchIntervencoes", target));
      });

      $('#searchNoticias .searchsubmit').click(function () {
        var target ='noticias' ;
        if (typeof __searchnewstargeturl !== 'undefined')
          target = __searchnewstargeturl;
        DoClientSearch(GetSearchParameters("#searchNoticias", target));
      });

      $('#searchAlbuns .searchsubmit').click(function () {
        var target = '';
        if (typeof __searchgallerytargeturl !== 'undefined')
          target = __searchgallerytargeturl;
        DoClientSearch(GetSearchParameters("#searchAlbuns", target));
      });

      $('#searchGeneral .searchsubmit').click(function () {
        var target = 'https://www.portugal.gov.pt/pt/gc22/pesquisar';
      DoClientSearch(GetSearchParameters("#searchGeneral", target));
    });
  });

  function GetSearchParameters(parentid, target) {

    return {
      "f": $(parentid + " .searchText").val()
      , "b": $(parentid + " .searchBeginDate").val()
      , "e": $(parentid + " .searchEndDate").val()
      , 'target': target
    };
  }
</script>


<script>
    $(function () {
        //$.extend($.fn.datepicker.defaults, { format: 'dd-mm-yyyy', language: 'pt' });
        $.extend($.fn.datepicker.defaults, { format: 'yyyy-mm-dd'});
    });
</script>

						
				</div>
			</div>
			
<script>
  function textIputPerformClick(e, target) {
    if (e.keyCode == 13) {
      $(target).click();
    }
  }
</script>
