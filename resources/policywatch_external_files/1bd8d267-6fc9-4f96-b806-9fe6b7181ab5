<script>
    $(function () {   
        if (Galleria.get().length > 0) {
            for (var i = 0; i < Galleria.get().length; i++) {
                Galleria.get(0).destroy();
            }
        }

        if ($(".galleria").length) {
            Galleria.run('.galleria');
        }

        SetMetaOg(undefined, 'Comunicado do Conselho de Ministros de 1 de outubro de 2020', undefined, $('#regText').text());
    });
</script>


	</div>

<script >
    function onSuccess() {
        $("#breadCrumbItem").text($("#itemtitle").text());
        
    }

    function onError() {
        window.location = '';
    }


    $(function () {
        var getparams = (function () {
            var id = GetUrlHashValue();
            return {
                "containerId": 'PageSelector__ctl0_SimpleRecordPageLauncher_content'
                 , "controlEnum": 77912
                 , "id": id
                 , 'onSuccess': onSuccess
                 , 'onError': onError
             }
        });

        if (GetUrlHashValue()) {
            LoadAsyncControl(getparams);
        }
    });
</script>



    </div>

