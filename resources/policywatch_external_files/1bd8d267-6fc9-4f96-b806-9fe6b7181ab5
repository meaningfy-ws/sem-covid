    $(function () {
        $("#globalsimplesearchbt").click(function () {
            var filter = $("#globalsimplesearchpinput").val();
            var searchparams = {
                "f": filter = '' ? undefined : filter
                , "target": 'https://www.portugal.gov.pt/pt/gc22/pesquisar'
            };
            DoClientSearch(searchparams);
        }
        );

      window.textIputPerformClick = function (e, target) {
        if (e.keyCode == 13) {
          $(target).click();
        }
      };

    });
</script>

