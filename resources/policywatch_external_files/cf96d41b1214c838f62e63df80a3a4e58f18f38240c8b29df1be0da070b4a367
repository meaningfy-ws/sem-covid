                            .addSize([1023, 599], [['fluid'], [2, 2]])
                            .addSize([800, 480], [['fluid'], [2, 2]])
                            .build()
                };

                $('#inread').insertAfter('.blocTexteType1:first p:nth-child(3)');

                // Make Mapping and slots
                $('div[data-ad-sizes]').each(function() {
                    var targeting = $(this).data('ad-targeting'),
                            mapping = $(this).data('ad-mapping'),
                            slotSizes = $(this).data('ad-sizes');

                    var slot = googletag
                            .defineSlot(adPath, slotSizes, $(this)[0].id)
                            .addService(googletag.pubads());

                    if (sizeMappings[mapping] !== undefined && sizeMappings[mapping] !== null) {
                        slot.defineSizeMapping(sizeMappings[mapping]);
                    }

                    if (targeting) {
                        $.each(targeting, function(position, value) {
                            slot.setTargeting(position, value);
                        });
                    }
                });

                //Make targeting
