 <script>
  function validateForm(token) {
    const check = document.getElementById("pf_Question").value;
    
    if ( check === "sort" ) {
      document.getElementByClassName("c-footer__form").submit();
    } else if  ( check === "Sort" ) {
      document.getElementByClassName("c-footer__form").submit();
    } else if ( check === "svart" ){
      document.getElementByClassName("c-footer__form").submit();
    } else if ( check === "Svart" ){
      document.getElementByClassName("c-footer__form").submit();
    } else {
      alert("Skriv fargen av zebra");
      event.preventDefault()
    }
  }
