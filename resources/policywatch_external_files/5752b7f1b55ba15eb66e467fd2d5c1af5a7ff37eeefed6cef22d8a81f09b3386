    <script>

            DOMReady(function () {



                // Initialize τη μπάρα με το sharing - print κλπ.
                setTimeout(function(){
                    CAPITALMVC.StoryShareAndPrintActions.Initialize({
                        articleId: 3444819,
                        addToReadingListPageUrl: "/api/article/readinglist/3444819/add",
                        loginPageUrl: "/account?returnUrl=https%3A%2F%2Fwww.capital.gr%2Foikonomia%2F3444819",
                        registerPageUrl: "/account/register?returnUrl=%2Foikonomia%2F3444819",
                        passwordResetPageUrl: "/account/passwordreset?returnUrl=https%3A%2F%2Fwww.capital.gr%2Foikonomia%2F3444819",
                        usernameReminderPageUrl: "/account/usernamereminder?returnUrl=https%3A%2F%2Fwww.capital.gr%2Foikonomia%2F3444819",
                        emailArticlePageUrl: "/story/3444819/email",
                        printArticlePageUrl: "/story/3444819/print",
                        rateArticleUrl: "/api/article/rate/3444819",
                        enableShareButtons: true       });
                }, 99);

                // Comments του άρθρου

                setTimeout(function(){
                    $(".dismiss-promoted-article").on("click", function(){});
                    $.get("/forum/api/threads/articlethreaddata/3444819").done(function (data, textStatus) {
                            if (textStatus === "success") {
                                var html;
                                if (data.ResultData && data.ResultData.threadId && data.ResultData.threadId > 0 && data.ResultData.totalMessages && data.ResultData.totalMessages > 0) {
                                    html = "<h3><a  href=\"" + data.ResultData.threadUrl + "\" rel=\"ugc\">Το άρθρο έχει " + data.ResultData.totalMessages + " σχόλια</a></h3>";
                                    html += "<a class=\"buttonS2 condensed bold\" href=\"" + data.ResultData.threadUrl + "\" rel=\"ugc\">ΓΡΑΨΤΕ ΤΟ ΣΧΟΛΙΟ ΣΑΣ</a>";
                                } else {
                                    if (data.ResultData && data.ResultData.threadUrl) {
                                        html = "<a class=\"buttonS2 condensed bold button__center \" href=\"" + data.ResultData.threadUrl + "\" rel=\"ugc\">ΣΧΟΛΙΑΣΤΕ ΤΟ ΑΡΘΡΟ</a>";
                                    } else {
                                        html = "<a class=\"buttonS2 condensed bold button__center\" href=\"/Forum/Api/Threads/CreateArticleThread/3444819\" rel=\"ugc\">ΣΧΟΛΙΑΣΤΕ ΤΟ ΑΡΘΡΟ</a>";
                                }

                                }
                            $("#article-comments-container").html(html).show();
                            }
                        });
                    }, 15);
                
                // Τα στατιστικά του άρθρου.
                $.get("/api/statistics/articles/logview/3444819/ine-gsee");


                if (!$("html").hasClass("bIE9"))
                {
                    // Ο slider με τα προτεινόμενα άρθρα.
                    var readArticleTimeoutInMs = 1000; // Μετά από τόσο χρόνο θα εμφανίζεται το popup.
                    var minWordsCount = 300; // Πόσες λέξεις πρέπει να έχει το άρθρο προκειμένου να εμφανίζεται το popup.
                    var beaconSelector = "#readarticlemarker";
                    var beaconOffset = 600; // Σε πόσα pixels πάνω απο το beacon element θα εμφανίζει το popup.
                    var staticScrollOffset = 0; // Το offset από την αρχή το document απο το οποίο και μετά θα εμφανίσει το popup. Αν οριστεί αυτό τότε δεν ισχύουν οι υπόλοιπες παράμετροι.
                    var autoCloseIntervalInMs = 300000; // Αν είναι > 0 είναι ο χρόνος που κλείνει μόνο του το popup αν ο χρήστης δεν πατήσει το X
                    var articleWordsCount = $(".article__content").text().match(/([\wα-ωΑ-Ωά-ώΆ-Ώ]{2,})|([\r\n]+)/ig).length;
                    var windowHeight = $(window).height();
                    var beaconObj = $(beaconSelector);
                    var snapContainerObj = $("#snap-for-promoted-article");
                    var scrollHandler = function(){
                        var scrollPos = $(this).scrollTop();
                        var beaconPos = beaconObj.offset().top;
                        var activationScrollPos = staticScrollOffset || beaconPos - windowHeight - beaconOffset;
                        if (scrollPos >= activationScrollPos) {
                            showSnappedContent();
                        }
                    };


                    function showSnappedContent() {
                        $(document).off("scroll", scrollHandler);
                        $.get("/api/promotedstory?rnd=" + parseInt(Math.random() * 10000000))
                            .done(function (data, textStatus){
                                if (textStatus === "success")
                                {
                                    var model = ko.mapping.fromJS(data.ResultData);
                                    var imageUrl = model.imageUrl();
                                    if (imageUrl) {
                                        imageUrl = imageUrl.replace(/[?&](width=\d+|height=\d+)/img, "");
                                        if (imageUrl.indexOf("?") < 0) {
                                            imageUrl += "?v=1";
                                        }
                                        imageUrl += "&width=120&height=90";
                                        model.imageUrl(imageUrl);
                                    }
                                    console.log(snapContainerObj.get(0));
                                    ko.applyBindings(model, snapContainerObj.get(0));
                                    snapContainerObj.find(".snap-close").click(function(eventInfo){
                                        eventInfo.preventDefault();
                                        $.ajax({
                                            url: "/api/promotedstory/dismiss",
                                            method: "POST",
                                            data: JSON.stringify({
                                                articleId: data.ResultData.id
                                            }),
                                            contentType: 'application/json; charset=utf-8',
                                            dataType: 'json'
                                        });
                                        snapContainerObj.animate( { bottom: "-100%" }, { queue: false, duration: 1000 });
                                    });
                                    snapContainerObj.animate( { bottom: 0 }, { queue: false, duration: 1000 });
                                    if (autoCloseIntervalInMs > 0) {
                                        setTimeout(function(){
                                            snapContainerObj.animate( { bottom: "-100%" }, { queue: false, duration: 1000 });
                                        }, autoCloseIntervalInMs);
                                    }
                                }
                            });
                    }

                    // Ενεργοποιώ το προτεινόμενο άρθρο αφού έχει περάσει <readArticleTimeoutInMs> milisecond ο χρήστης στη σελίδα
                    // και εφόσον το άρθρο έχει περισσότερες απο <minWordsCount> λέξεις.
                    if (articleWordsCount > minWordsCount)
                    {
                        setTimeout(function(){
                            scrollHandler(document);
                            $(document).scroll(scrollHandler);
                        }, readArticleTimeoutInMs);
                    }
                }
                

            });

            // Initialize το Addthis sharing service
    </script>
    <script>
        if (window.addEventListener) {
            window.addEventListener("message", onMessage, false);
        }
        else if (window.attachEvent) {
            window.attachEvent("onmessage", onMessage, false);
        }

        function onMessage(event) {
            // Check sender origin to be trusted
            //if (event.origin !== "http://example.com") return;

            var data = event.data;

            if (typeof (window[data.func]) == "function") {
                window[data.func].call(null, data.message);
            }
        }

