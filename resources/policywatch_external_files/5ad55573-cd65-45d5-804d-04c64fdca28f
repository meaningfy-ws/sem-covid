                                                                                            <script>
   svDocReady(function() {
   // Fallback if browser doesn't support the html5 placeholder attribute
   if (!sv.ClientUtil.supportsPlaceholder) {
      // Ensure initial value
      if ($svjq("#search12_33f2d2211574b50350a23bef").val() == "") {
         $svjq("#search12_33f2d2211574b50350a23bef").val("Ange sökord");
      }
      // Ensures 'search here' text is properly cleared when entering search field
      $svjq("#search12_33f2d2211574b50350a23bef").on('focusin', function() {
         if ($svjq("#search12_33f2d2211574b50350a23bef").val() == "Ange sökord") {
            $svjq("#search12_33f2d2211574b50350a23bef").val("");
            return false;
         }
      });
      // Ensures 'search here' text is properly set when leaving search field
      $svjq("#search12_33f2d2211574b50350a23bef").on('focusout', function() {
         if ($svjq("#search12_33f2d2211574b50350a23bef").val() == "") {
            $svjq("#search12_33f2d2211574b50350a23bef").val("Ange sökord");
            return false;
         }
      });
   }
      // Submit, blocks empty search and 'search here' search
      $svjq("#search12_33f2d2211574b50350a23befbutton").on('click', function() {
         var qry = $svjq("#search12_33f2d2211574b50350a23bef").val();
         if (qry == "") {
            $svjq("#search12_33f2d2211574b50350a23bef").val("Ange sökord");
            return false;
         } else if (qry == "Ange sökord") {
            return false;
         }
      });
      // Invokes the search button if user presses the enter key (needed in IE)
      $svjq("#search12_33f2d2211574b50350a23bef").on('keypress', function(e) {
         if (e.which == 13) {
            $svjq("#search12_33f2d2211574b50350a23befbutton").trigger('focus').trigger('click');
            return false;
         }
      });
   });
</script>

