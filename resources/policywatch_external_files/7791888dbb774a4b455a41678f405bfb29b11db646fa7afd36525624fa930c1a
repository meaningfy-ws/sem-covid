<script>
$(function(){

  $(document).on('click', '.comment-vote', function() {
    var action = $(this).data('action'),
        comment = $(this).data('cid'),
        thisObj = $(this),
        counter = $(this).find('.counter').html();


    if ($.cookie('comment' + comment) == undefined) {
      $.get( action, { comment : comment }, function( data ) {
        if (data) {
          if (counter) {
            counter++;
            $(thisObj).find('.counter').html(counter);
          } else {
            $(thisObj).addClass('active');
            $(thisObj).find('.counter').html(1);
          }

          $.cookie('comment' + comment, 1);
        }
      });
    }

  });

});
</script>
