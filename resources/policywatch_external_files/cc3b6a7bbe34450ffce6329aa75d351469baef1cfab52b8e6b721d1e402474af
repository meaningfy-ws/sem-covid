                            'action': 'Latest in News',
                            'label': ' Clicked: '+(jq(this).attr('href'))+' from ' + window.location.pathname + ' From: '+whichPosition( jq(this) )
                        });
					});		
				  }
				});
				
				var refreshId = setInterval( function() 
			    {
			    	
					var timestamp = new Date().getTime();
					jq.ajax({
						  url: '/news/ireland/span-12-column-splitter-8-4-7.2411566/latest-ireland-7.2411568?ot=example.AjaxPageLayout.ot&ts='+timestamp,
						  timeout: 10000,
						  
						  complete: function(data, statusMsg){
							if(statusMsg == 'success'){
								var responseText = jq.trim( data.responseText );
								var inpageText = jq.trim( jq('#ajax-placeholder-64281496').text() );
								
								if(responseText != inpageText){
									jq('.spinner').show();
									jq('#ajax-placeholder-64281496' ).animate({
										opacity: 0
										}, 400, 'linear', function() {
											jq('#ajax-placeholder-64281496').html(responseText);
											jq('#ajax-placeholder-64281496' ).animate({
											opacity: 1
											}, 400, 'linear', function() {
												jq('.spinner').hide();
												removeSplitterGaps();
												jq("abbr.timeago").timeago();
												/*Analytics*/
												jq('.latestin .story a').click(function(e) {		
                                                    dataLayer.push({
                                                        'event': 'data-layer-event',
                                                        'category': 'Latest',
                                                        'action': 'Latest in News',
                                                        'label': ' Clicked: '+(jq(this).attr('href'))+' from ' + window.location.pathname + ' From: '+whichPosition( jq(this) )
                                                    });
												});	
										});						
									});
								}
							}else{
								return;
							}
						  }						  
						});	
	
			    }, 120000);
			});
	    </script>
	    						
						
