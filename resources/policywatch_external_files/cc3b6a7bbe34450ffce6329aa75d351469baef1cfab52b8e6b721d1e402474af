    .lt-ie9 .notifyjs-bootstrap-base{ background-color:rgb(246,246,246);}

    @media only screen and (max-width: 768px) {

    .notifyjs-bootstrap-base{}
    .notifyjs-bootstrap-base .notify-msg{font-size:100%;}
    .notifyjs-bootstrap-base .notify-prompt{}
    .notifyjs-bootstrap-base .notify-prompt span{display:none;}
}

.fw.bg-dark.features {
    padding: 0 0 30px 0;
}
</style>
      
    
    
