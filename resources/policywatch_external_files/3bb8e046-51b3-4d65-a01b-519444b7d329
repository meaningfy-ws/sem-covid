                function checkMenuItems(){
                    var elmnt = document.getElementById("nav-bar-filter");

                    // if (elmnt.offsetHeight < elmnt.scrollHeight ||
                        // elmnt.offsetWidth < elmnt.scrollWidth) {

                        // if (elmnt.children[i].offsetTop + elmnt.children[i].offsetHeight >
                        //       elmnt.offsetTop + elmnt.offsetHeight ||
                        //       elmnt.children[i].offsetLeft + elmnt.children[i].offsetWidth >
                        //       elmnt.offsetLeft + elmnt.offsetWidth )

                        var invisibleItems = [];
                        var visibleItems = [];
                        for(var i=0; i<elmnt.childElementCount; i++){
                            if (elmnt.children[i].offsetTop + elmnt.children[i].offsetHeight >
                              elmnt.offsetTop + elmnt.offsetHeight ){

                                invisibleItems.push(elmnt.children[i]);
                                elmnt.children[i].classList.add("invisible");
                                elmnt.children[i].classList.remove("visible");
                            }else{
                                visibleItems.push(elmnt.children[i]);
                                elmnt.children[i].classList.add("visible");
                                elmnt.children[i].classList.remove("invisible");
                            }

                        }
                        elmnt.classList.remove("overflow-hidden");

                        // console.log(elmnt.clientWidth);

                        setTimeout(function(){ 
                        // alert("Hello"); 
                            var firstelmnt = document.getElementsByClassName('invisible')[0];
                            var elmntNew = document.getElementById("nav-bar-filter");

                            if (typeof(firstelmnt) != 'undefined' && firstelmnt != null){

                                if (!(firstelmnt.offsetTop + firstelmnt.offsetHeight > elmntNew.offsetTop + elmntNew.offsetHeight)){
                                    firstelmnt.classList.add("visible");
                                    firstelmnt.classList.remove("invisible");
                                    
                                }
                            }
                      
                        }, 200);
                        
                    // } else {
    
                    //     // your element doesn't have overflow
                    //     elmnt.classList.remove("overflow-hidden");
                    // }
                    return;
                }

                if($('.makthes-hero-menu').length > 0){

                    checkMenuItems();


                    setTimeout(function(){ 
                        // alert("Hello"); 
                        var firstelmnt = document.getElementsByClassName('invisible')[0];
                        var elmntNew = document.getElementById("nav-bar-filter");

                        if (typeof(firstelmnt) != 'undefined' && firstelmnt != null){

                            if (!(firstelmnt.offsetTop + firstelmnt.offsetHeight > elmntNew.offsetTop + elmntNew.offsetHeight)){
                                firstelmnt.classList.add("visible");
                                firstelmnt.classList.remove("invisible");
                                
                            }
                        }
                  
                    }, 200);
                    

