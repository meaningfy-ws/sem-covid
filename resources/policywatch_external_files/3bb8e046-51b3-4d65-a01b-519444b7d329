        <script>



function notDone(msg) {
        iziToast.error({
            title: 'Error',
            message: msg,
            position: 'topRight',
        });
}

function isDone(msg) {
        iziToast.show({
            title: 'Success',
            message: msg,
            position: 'topRight',
            backgroundColor: '#14161a',
            color: '#fff',
            messageColor: '#fff',
            theme:'dark',
            titleColor: '#fff',
            progressBarColor: '#e10d2c',
        });
}

</script>

                    
        
        
    </body>
</html>
