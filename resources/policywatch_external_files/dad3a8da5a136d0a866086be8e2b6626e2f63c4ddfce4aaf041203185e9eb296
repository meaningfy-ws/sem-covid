    <script>
        $(document).ready(function () {
            $('#txtSearch').on('input', function (e) {
                if ($("#txtSearch").val().length < 1) {
                    $("#buttonSearch").prop('disabled', true);
                }
                else {
                    $("#buttonSearch").prop('disabled', false);
                }
            });

            $(".validation-summary-errors").append("<span class='closemessage' title='Close'>x</span>")

            $(".closemessage").on("click", function () {
                $(this).parent().fadeOut(500);
            });
        });

        $(".language-current").on("click", function () {
            var languageList = $(".language ul").get();
            if ($(languageList).is(":visible")) {
                $(languageList).hide();
            }
            else {
                $(languageList).show();
            }
        });

        $(document).ajaxError(function (event, jqxhr, settings, thrownError) {
            try {
                var jsonResult = $.parseJSON(jqxhr.responseText);
                if (jsonResult.errorMessage == "expired") {
                    alert("Kindly note that your session has expired. Please login again to continue using Jobsplus' personalised services.");
                    window.location = snapPrimaryUrl;
                }
            }
            catch (e) {
                alert("An error occured, please try again.");
            }
        });
    </script>
