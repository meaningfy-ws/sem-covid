var sp_preloader = '0';

var sp_gotop = '1';

var sp_offanimation = 'default';

jQuery(document).ready(function() {
    function close_accordion_section() {
        jQuery('#jeAccordion372 .jeAcc-title').removeClass('active');
        jQuery('#jeAccordion372 .jeAcc-content').slideUp(300).removeClass('open');
    }
    jQuery('#jeAccordion372 .jeAcc-title').click(function(e) {
        // Grab current anchor value
        var currentAttrValue = jQuery(this).attr('href');
 
        if(jQuery(e.target).is('.active')) {
            close_accordion_section();
        }else {
            close_accordion_section();
 
            // Add active class to section title
            jQuery(this).addClass('active');
            // Open up the hidden content panel
            jQuery('#jeAccordion372 ' + currentAttrValue).slideDown(300).addClass('open');
        }
        e.preventDefault();
    });
});

