<script>
(function() {
var options = { weekday: 'long', year: 'numeric', month: 'long', day: 'numeric' };
var today  = new Date();
var pad = function(num, size) {
    var s = num+"";
    while (s.length < size) s = "0" + s;
    return s;
};
jQuery('.jumi-dynamic-time').html(today.toLocaleDateString("el-GR", options) + ', <i class="icon-time"></i>' + pad(today.getHours(), 2) + ':' + pad(today.getMinutes(), 2));
})();
</script></p></div>
