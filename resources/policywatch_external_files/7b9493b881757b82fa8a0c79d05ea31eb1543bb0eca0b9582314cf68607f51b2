        <script type="text/javascript">
            var routesObj = {
                baseUrl : 'https://www.makthes.gr/'
            };

            $(document).ready(function(){
                // $.ajax({
                //     type:'GET',
                //     url:'/gen-csrf',
                //     success:function(data) {
                //         if(data.success) {
                //             //alert(data.csrf);
                //             var input = $("<input>")
                //                .attr("type", "hidden")
                //                .attr("name", "_token").val(data.csrf);
                //             $('form').append($(input));
                //             $('meta[name="csrf-token"]').attr('content', data.csrf);
                //         }
                //     },
                //     cache: false
                // });

                $.ajaxSetup({headers:{'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')}});
            });

            $(document).ready(function () {

                $("#newsletter-email").on("click", function (e) {

                   if (!$("input[name='ms-email']").val() == '') {
                        e.preventDefault();
                         if (!$("input[name='newsletterTerms']").is(':checked')) {
                            //$('.footer_terms').addClass('ns_error');
                            alert('Πρέπει να αποδεχτείτε τους όρους χρήσης');
                        }
                        else{
                            $('.newsletter-form .ajax-loading').show();
                            // handleNewsletterSubmit();
                            $('form#newsletter_form').submit();
                        }
                    }
                });

                $('.newsletter-form input[name="ms-email"]').keypress(function (evt) {
                    if (evt.which == 13) {
                        evt.preventDefault();
                        //$('.newsletter-footer-form .ajax-loading').show();
                        if (!$("input[name='newsletterTerms']").is(':checked')) {
                            //$('.footer_terms').addClass('ns_error');
                            alert('Πρέπει να αποδεχτείτε τους όρους χρήσης');
                        }
                        else{
                            $('.newsletter-form .ajax-loading').show();
                            // handleNewsletterSubmit();
                            $('form#newsletter_form').submit();
                        }
                        //handleNewsletterSubmit();
                    }
                });
            });

            function handleNewsletterSubmit() {
                $.ajax({ url: routesObj.baseUrl+"newsletter/subscribe", type: "post",
                    data: $("#newsletter_form").serialize(),
                    success: function(data) {

                        switch (data.opstatus) {
                            case 0:
                                if (data.opmessage.length > 0) {
                                    $('.newsletterReponse').text(data.opmessage);

                                } else {
                                    $('.newsletterReponse').text(data.errors[0]);
                                }
                                break;
                            case 1:

                                $('.newsletterReponse').html(data.opmessage);
                                $('#newsletter_form').hide();
                                //fbq('track', 'CompleteRegistration');
                                break;
                        }
                        $('.newsletter-form .ajax-loading').slideUp(100);
                    }
                });
            }
        </script>
        <script>
        $(document).ready(function () {

            $('.str_wrap').css('display','block');
            $('.str1').liMarquee({circular:true, startShow:true});

            $('#boxLoading').hide();


                var header = document.getElementById("sticky-header");
                var sticky = header.offsetTop;

                window.onscroll = function() {
                    stickyMenuFunction();
                };



                function stickyMenuFunction() {
                  if (window.pageYOffset > sticky) {
                    header.classList.add("sticky","small-menu");
                    $('#content-box').show();
                    $('#go-top:hidden').stop(true, true).fadeIn();
                  } else {
                    header.classList.remove("sticky","small-menu");
                    $('#content-box').hide();
                    $('#go-top').stop(true, true).fadeOut();
                  }
                }



                $('#go-top').click(function() {
                    $('html, body').animate({ scrollTop: 0 }, 600);
                    return false;
                });

                $('.menu-switch').click(function(){
                    $('.side-menu').stop(true,true).fadeIn(200).addClass('opened');
                    $('html, body').addClass('no-overflow');
                });

                $('.close-smenu').click(function(){
                    $('.side-menu').removeClass('opened').stop(true,true).delay(300).fadeOut(200, function(){$('html, body').removeClass('no-overflow');});
                });

                $('.close-cmenu').click(function(){
                    $('.cookiew').removeClass('opened');
                });

                $('.side-menu').on('click', function(){
                    $('.close-smenu').trigger('click');
                });

                $('.smenu-w').on('click', function(evt){
                    evt.stopPropagation();
                });

                var heroslider = $('#hero-slider');

                
                heroslider.on('initialize.owl.carousel', function(event) {
                    $('#top-news').find(".owl-dot").eq(0).addClass("active");
                    heroslider.find(".slider-item").removeClass('d-none');
                });

                heroslider.owlCarousel({
                    loop: true,
                    // rewind: true,
                    responsiveClass: true,
                    margin: 0,
                    dots: true,
                    smartSpeed: 900,
                    autoHeight: true,
                    autoplay: true,
                    autoplayTimeout: 9000,
                    autoplayHoverPause: true,
                    responsive:{
                        0:{
                            items:1,
                            nav:false
                        },
                        600:{
                            items:1,
                            nav:false
                        },
                        1000:{
                            items:1,
                            nav:false
                        }
                    }
                })
                .on('changed.owl.carousel', function(event) {
                    if (event.item) {
                        var current = event.item.index;
                        var currentThumb = $(event.target).find('.owl-item').eq(current).find('.slider-item').data('thumb');

                        $('#top-news').find(".owl-dot").eq(currentThumb).addClass("active").siblings().removeClass("active");

                    }
                   
                });


                    /*                afterMove: moved,
                    startDragging: pauseOnDragging,*/


                $('#top-news .owl-dot').click(function () {
                    var dataThumb = $(this).data('thumb');
                    heroslider.trigger('to.owl.carousel', [dataThumb, 500]);
                    
                    heroslider.trigger('stop.owl.autoplay');
                    heroslider.trigger('play.owl.autoplay');
                });

                var gallery_slider = $('#gallery-slider').owlCarousel({
                    loop: true,
