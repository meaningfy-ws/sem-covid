                            $(this).find('.sub-dropdown').stop(true, true).delay(200).slideUp(300);
                        });
                    }

                    
                    var mm_width = $(window).outerWidth();

                    $(window).on('resize', function() {
                        // if ($(this).outerWidth() !== mm_width) {
                            // mm_width = $(this).outerWidth();
                            // console.log(mm_width);
                            checkMenuItems();

                        // }

                        if(this.resizeTO) clearTimeout(this.resizeTO);
                        this.resizeTO = setTimeout(function() {
                            // console.log('test');
                            checkMenuItems();
                            
                        }, 1000);
                    });
                                    
