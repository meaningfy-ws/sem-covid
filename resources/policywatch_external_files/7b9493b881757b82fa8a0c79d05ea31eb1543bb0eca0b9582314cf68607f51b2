                        },
                        600:{
                            items:1,
                            nav:false,
                            loop:true
                        },
                        1000:{
                            items:1,
                            nav:false,
                            loop:true
                        }
                    }
                }).on('changed.owl.carousel', function(event) {
                    //moved();
