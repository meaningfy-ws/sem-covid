<v-data v-slot="state" :data="{ showReceipt: false, permission: false, showForm: true}">
    <div v-show="!state.showForm">
        <v-stack size="small">
            <v-text><p>Som medlem af 3F skal du logge p&aring; Mit3F for at kontakte din afdeling.</p></v-text>
            <v-inline>


