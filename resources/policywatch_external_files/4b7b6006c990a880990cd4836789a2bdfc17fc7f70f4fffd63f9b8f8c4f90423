    <v-data v-slot="state" :data="{ activeModal: null, modtager: null, modtagerkategorier: null, key: 1, touched: false }">
        <div>

            <v-modal :open="state.activeModal === 'Oprethenvendelse'" v-on:close="state.activeModal = null;state.touched=false;">
                <div class="row">
                    <v-stack size="small" class="col">
                        <v-text heading size="small" tag="h2">
                            Opret henvendelse
                        </v-text>

                        <v-fetch v-slot="response" url="/api/3F/MinPost/HentModtagerKategorier">
                            <div v-if="response.loading">
                                <v-loading></v-loading>
                            </div>
                            <div v-else-if="response.ok">
                                <v-form v-slot="form"
                                        method="POST"
                                        action="/api/3F/MinPost/opret"
                                        enctype="multipart/form-data"
                                        @success="if('' != ''){toast('success','');state.activeModal = null}else{location.href='?' + $event.data}"
                                        @fail="toast('error','Fejl. Henvendelsen kunne ikke sendes')"
                                        @error="toast('error', 'Something went wrong')"
                                        async>
                                    <v-stack size="medium">
                                        <input name="HenvendelsesentTekst" type="hidden" value="Henvendelsen er sendt" />
                                            <v-select name="modtager" required 
                                                      @input="state.modtager = $event.target.value; state.key += 1; state.touched=true;" 
                                                      placeholder="Modtager"
                                                      error="Modtager skal v&#230;lges">
                                                <v-option v-for="(value,name) in response.data" :key="name" :value="name" :disabled="response.data[name].length == 0">{{name}}</v-option>
                                            </v-select>
                                            <div style="margin-top:0px">
                                                <v-text size="small">
                                                    3F&#39;s fagforening og a-kasse m&#229; ikke udveksle dine oplysninger. Derfor skal du f&#248;rst v&#230;lge en modtager.
                                                    <a :href="''">Læs mere</a>
                                                </v-text>
                                            </div>
                                            <v-select name="kategori" required 
