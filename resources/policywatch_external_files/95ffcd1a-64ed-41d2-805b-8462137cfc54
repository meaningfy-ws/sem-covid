<script type="text/javascript">
    var addthis_config = addthis_config||{};
    addthis_config.data_track_addressbar = false;
    addthis_config.data_track_clickback = false;

    (function () {
        if (typeof frontend.HeaderRegionalisation !== 'undefined') {
            frontend.HeaderRegionalisation.setup('3qmlhh7c41dk23gc', 'https://www.noen.at/in-ausland');
        }
    })();

    $(document).ready(function () {
        $('#linkToMobile').on('click', function (e) {
            e.preventDefault();
            $.removeCookie('desktop', { path: '/', domain: '.noen.at' });
            window.location.replace($(this).attr('href'));

        });

        if ($.cookie('desktop')) {
            $('#linkToMobile').removeClass('hidden');
        }
        $('#user-logout-link').on('click', function (e) {
            e.preventDefault();
            $.removeCookie('spunQ_User', { path: '/', domain: '.noen.at'});
            setTimeout(customRedirect, 100, $(this).attr('href'));
        });
    });

    $(document).on(frontend.events.SESSION_LOADED, function (event, userData) {
        if (userData.loggedIn) {
            // needed for update on login
            if (!User.loggedIn) {
                User.loggedIn = true;
                User.sessionLoaded = true;
            }
            $('.user-login-link').replaceWith('<a class="user-control-link icon-user-male" href="javascript:">Mein NÖN.at</a>');
        }
    });

    function customRedirect(url) {
        window.location.replace(url);
    }

</script>                        <script type="text/javascript">                $(document).trigger(frontend.events.HEADER_LOADED);
            
</script>
            <div class="maincontainer">
                                    <div id="bigsizebanner-helper">
                        <div class="additionAd" id="div-bigsizebanner_1">
            <script>
            googletag.cmd.push(function() {
                                googletag.display('div-bigsizebanner_1');
            });
        </script>
    </div>

                    </div>
                                <div class="col-12 header-sub box">
    <ul class="breadcrumbs">
        <li class="breadcrumb">
            <a href="https://www.noen.at/">
                NÖN.at
