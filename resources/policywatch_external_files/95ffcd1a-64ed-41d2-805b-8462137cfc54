<script type="text/javascript">
      window.addEventListener('message', receiveMessage, false);

      function receiveMessage(event) {
          if (typeof event.data !== 'undefined') {
              if ((typeof event.data === 'string' || event.data instanceof String) && event.data == 'form-submitted-successfully') {
                  piGoogleAnalytics('Testleser-Popup', 'Article', window.location.href);
                  $.cookie('testleser-form-submitted', true, {
                    expires: 365,
                    domain: User.options.issueCookieDomain,
                    path: '/'
                });
              }
              else if ((typeof event.data === 'string' || event.data instanceof String) && event.data.indexOf('iframe-newsletter-success') > (-1)) {
                  $.cookie('ouibounce-newsletter', null);
                  $.cookie('ouibounce-newsletter', true, {
                    expires: 365,
                    domain: User.options.issueCookieDomain,
                    path: '/'
                  });
                  piGoogleAnalytics('Newsletter-Popup: Formular', 'Abgeschickt', '1');
              }
              else if ((typeof event.data === 'string' || event.data instanceof String) && event.data.indexOf('iframe-newsletter-close') > (-1)) {
                  $('#ouibounce-modal-newsletter').hide();
              }
              else if ((typeof event.data === 'string' || event.data instanceof String) && event.data.indexOf('iframe-newsletter-decline') > (-1)) {
                  $('#ouibounce-modal-newsletter').hide();
                  $.cookie('ouibounce-newsletter', null);
                  $.cookie('ouibounce-newsletter', true, {
                    expires: 365,
                    domain: User.options.issueCookieDomain,
                    path: '/'
                  });
              }
              else if ((typeof event.data === 'string' || event.data instanceof String) && event.data.indexOf('iframe-newsletter-') > (-1)) {
                  var height = event.data.replace('iframe-newsletter-', '');
                  $('iframe.frame-newsletter').attr('height', height);

                  $('#ouibounce-modal-newsletter .exit-intent__modal').height(height);
                  $('#ouibounce-modal-newsletter .exit-intent__modal').css('top', $(window).scrollTop() + 'px');
                  $('#ouibounce-modal-newsletter iframe').height(height);

              }
          }
      }

      function enableExitIntent() {
          if ($.cookie('testleser-form-count') >= 1 && !$.cookie('testleser-form-submitted')) {
              var _ouibounce = ouibounce(document.getElementById('ouibounce-modal'), {
                aggressive: false,
                timer: 0,
                cookieExpire: 365,
                sitewide: true,
                cookieName: 'testleser-form-2',
                callback: function () {
                  $('.exit-intent__modal').css('top', ($(window).scrollTop() + 20) + 'px');
                  piGoogleAnalytics('Testleser-Popup', 'Angezeigt', '1');
                  console.log('Testlester 2. Mal angezeigt');
                }
              });
          }
          else if (!$.cookie('testleser-form-submitted')) {
              var _ouibounce = ouibounce(document.getElementById('ouibounce-modal'), {
                  aggressive: true,
                  timer: 0,
                  cookieExpire: 365,
                  sitewide: true,
                  cookieName: 'testleser-form-1',
                  callback: function () {
                    $('.exit-intent__modal').css('top', ($(window).scrollTop() + 20) + 'px');
                    piGoogleAnalytics('Testleser-Popup', 'Angezeigt', '1');
                    console.log('Testlester 1. Mal angezeigt');
                  }
              });

              $.cookie('testleser-form-count', 1, {
                  expires: 365,
                  domain: User.options.issueCookieDomain,
                  path: '/'
              });
          }

          if (($.cookie('testleser-form-2') || $.cookie('testleser-form-submitted')) && !$.cookie('ouibounce-newsletter')) {

              var type = Math.floor(Math.random() * Math.floor(2));
              if (type == 0) {
                var _ouibounce = ouibounce(document.getElementById('ouibounce-modal-newsletter-banner'), {
                  aggressive: false,
                  timer: 0,
                  cookieExpire: 14,
                  sitewide: true,
                  cookieName: 'ouibounce-newsletter',
                  callback: function () {
                    $('.exit-intent__modal').css('top', ($(window).scrollTop() + 20) + 'px');
                    piGoogleAnalytics('Newsletter-Popup: Banner', 'Angezeigt', '1');
                  }
                });
              }
              else if (type == 1) {
                var _ouibounce = ouibounce(document.getElementById('ouibounce-modal-newsletter'), {
                  aggressive: false,
                  timer: 0,
                  cookieExpire: 14,
                  sitewide: true,
                  cookieName: 'ouibounce-newsletter',
                  callback: function () {
                    $('.exit-intent__modal').css('top', ($(window).scrollTop() + 20) + 'px');
                    piGoogleAnalytics('Newsletter-Popup: Formular', 'Angezeigt', '1');
                  }
                });
              }
          }
      }
      $('#ouibounce-modal a.no').on('click', function() {
        $('#ouibounce-modal').hide();
      });

      $('#ouibounce-modal-newsletter a.no').on('click', function(e) {
        e.preventDefault();
        $('#ouibounce-modal-newsletter').hide();
      });

      $('#ouibounce-modal-newsletter-banner a.no, #ouibounce-modal-newsletter-banner a.exit-intent__discard').on('click', function(e) {
        e.preventDefault();
        $('#ouibounce-modal-newsletter-banner').hide();
      });


      $('#ouibounce-modal .modal').on('click', function(e) {
        e.stopPropagation();
      });


</script>
