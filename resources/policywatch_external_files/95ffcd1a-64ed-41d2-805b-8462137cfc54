<article
    class="col-4 box teaser "
    itemscope=""
    itemtype="http://schema.org/Article"
    data-linkwrapper=""
    data-content="245744318"
    data-publishdate="1611661320">
                <div class="teaser__image-container">
        <figure class="teaser__image gradient">
                                                <img itemprop="thumbnailUrl" class="lazyload"
                    src="/images/frontend/blank.gif"
                    data-src="https://images02.noen.at/63670011558613254_BLD_Online.jpg/teaser-col-4/245.742.343"
                    alt="">
