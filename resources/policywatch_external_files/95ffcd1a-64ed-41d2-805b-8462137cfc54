<script type="text/javascript">

    function CookiebotCallback_OnAccept() {

        if (Cookiebot.consent.marketing) { CookieBot_accept_marketing(); }
        else { CookieBot_decline_marketing(); }

        if (Cookiebot.consent.statistics)
        {
            enableGoogleAnalytics();
            enableGTM();
        }
    }
    function CookieBotCallback_OnDecline() {
        if (Cookiebot.consent.marketing) { CookieBot_accept_marketing(); }
        else { CookieBot_decline_marketing(); }
    }

    function CookieBot_accept_marketing() {
        if (typeof enableExitIntent === 'function') {
            enableExitIntent();
        }
        enableAddThis();
        enableFB();
        enableFbPixel();
        _ad_dfp_consent = 'true';
        _ad_dfp_nonpersonalized = 0;
        _ad_inRead = true;
        // load script for YOC ads after user action to provide correct consent
        console.log('accept marketing');
        $('div[data-visx]').each(function (i) {
            // $.getScript('https://s.visx.net/tag.js');
            $.ajax({
              url: 'https://s.visx.net/tag.js',
              dataType: "script",
              cache: true,
              success: function() {}
            });
        });
    }
    function CookieBot_decline_marketing() {
        _ad_dfp_consent = 'false';
        _ad_dfp_nonpersonalized = 1;
        _ad_inRead = false;
        // load script for YOC ads after user action to provide correct consent
        console.log('decline marketing');
        $('div[data-visx]').each(function (i) {
            // $.getScript('https://s.visx.net/tag.js');
            $.ajax({
              url: 'https://s.visx.net/tag.js',
              dataType: "script",
              cache: true,
              success: function() {}
            });
        });
    }
</script>

