<script type="text/javascript">    $(document).on(frontend.events.SESSION_LOADED, function (event, userData) {
        if(userData.isAdmin) {
            $('.adminEditArea').html('<a href="/community/redirect/backend/208.890.718" target="_blank" title="Edit in Backend"><img src="/images/frontend/pix/16x16/link_go.png" /></a>');
        }
    });
function getNewTextSections(clickEvent) {
    var isNewsticker = parseInt($('.refreshTicker').length);
    if(isNewsticker < 1) {
        return;
    }
    // alle vorhandenen IDs aus dem DOM auslesen
    var sectionIds = '';
    $('.article-section').each(function( index ) {
    // jedes Element durchgehen das die Klasse .article-section hat
        var tmp = $(this).attr("data-section");
        if(tmp) {
            // wenn gültig, splitten und in einen String kopieren
            var arr = tmp.split("section-");
            if(arr.length > 1)
                sectionIds += arr[1] + ',';
        }
    });

    // eine andere Page mittels AJAX aufrufen und ein Template generieren lassen
    $.ajax({
        type: "GET",
        url: "https://www.noen.at/liveticker/208.890.718//0" + "/ids/" + sectionIds,
        success: function(data) {
            if(data) {
                // es gibt einen neuen Eintrag - hinzufügen
                var newHtml = '';
                // wenn clickEvent ist 1 => es wurde der Button zum gleich aktualisieren gedrückt
                if(clickEvent != 1) {
                    // in diesem Falle ist der Timer abgelaufen - daher um den Block ein display-none - wird nicht gleich angezeigt
                    newHtml += '<span class="newsticker__hiddenEntries">';
                }
                newHtml += data;
                if(clickEvent != 1) {
                    newHtml += '</span>';
                    // aus dem Button die Anzahl der Einträge auslesen die noch nicht aktualisiert wurden und um 1 erhöhen
                    var count = parseInt($('.newsticker__sticky-button').attr("data-count")) + 1;
                    $('.newsticker__sticky-button').attr("data-count", count);
                    var text = "";
                    if(count == 1)
                        text = count + " neuer Eintrag verfügbar - Jetzt aktualisieren";
                    else if (count > 1)
                        text = count + " neue Einträge verfügbar - Jetzt aktualisieren";
                    $('.newsticker__sticky-button').css("visibility", "visible");
                    $('.newsticker__sticky-button').text(text);
                }
                var html = $('.newsticker__area').html();
                newHtml += html;
                $('.newsticker__area').html(newHtml);
            }
        },
        error: function(e) {
            // console.log(e);
        }
    });
}

(function () {

    var cookieName = 'readIds';
    var readIds = $.cookie(cookieName);
    var articleId = "208890718";
    if (readIds) {
        var readIds = readIds.split(",");
        if ($.inArray(articleId,readIds) == '-1') {
            readIds.push(articleId);
        }
        readIds.sort();
    } else {
        readIds = [];
        readIds.push(articleId);
    }
    $.cookie(cookieName, readIds, { path: '/'});

    $('.refreshTicker').on('click', function(e){
        $('.newsticker__hiddenEntries').show();
        // wenn der fixe Button oben gedrückt wurde - auf neue Sections überprüfen
        getNewTextSections(1);
        // Anzahl der neuen Einträge am Sticky Button zurücksetzen
        $('.newsticker__sticky-button').attr("data-count", 0);
        // danach den Sticky Button zurücksetzen - da keine neuen Einträge
        $('.newsticker__sticky-button').text("keine neuen Einträge verfügbar");
    });

    window.setInterval(getNewTextSections,  30000);

    $('.ad-column.twyn .additionAd').hide();

})();

$('.article-image').on('click', function (e) {
    $target = $(e.target);
    if (!($target.is('a'))) {
        frontend.InlineImage.toggleImageFullScreen($(this));
    }
});

$('.wohin-sommer__map-container .overlay').on('click', function() {
    $(this).hide();
});

$(document).on('click','.newsticker__sticky-button', function(e){
    // wenn Sticky Button gedrückt wurde - alle hidden-entries anzeigen
    $('.newsticker__hiddenEntries').show();
    // nach oben scrollen zu den neuen Einträgen
    $('html, body').animate({
        scrollTop: $(".refreshTicker").offset().top - 70
    }, 2000);
    $('.newsticker__sticky-button').attr("data-count", 0);
    // danach den Sticky Button ausblenden
    $('.newsticker__sticky-button').css("visibility", "hidden");
});

$(window).scroll(function(){
    var isNewsticker = parseInt($('.refreshTicker').length);
    if(isNewsticker > 0) {
        // Scroll Funktion - wenn eine gewisse höhe erreicht wurde - Sticky Button ein - oder ausblenden
        var refreshTickerButton = $('.refreshTicker');
        var refreshTickerButton_height = refreshTickerButton[0].getBoundingClientRect().top;
        var social_print = $('.article-social-print');
        var social_print_height = social_print[0].getBoundingClientRect().top;
        var sticky_button = $('.newsticker__sticky-button');
        var sticky_button_height = sticky_button[0].getBoundingClientRect().top + 30;

        if (refreshTickerButton_height > 50)
            $('.newsticker__sticky-button').css("visibility", "hidden");
        if (refreshTickerButton_height < 50) {
            if (sticky_button_height > social_print_height)
                $('.newsticker__sticky-button').css("visibility", "hidden");
            else {
                var count = parseInt($('.newsticker__sticky-button').attr("data-count"));
                if(count > 0) {
                    $('.newsticker__sticky-button').css("visibility", "visible");
                }
            }
        }
    }
});

window.addEventListener('message', receiveMessage, false);
function receiveMessage(event) {
    if (typeof event.data !== 'undefined') {
        if ((typeof event.data === 'string' || event.data instanceof String) && event.data.indexOf('iframe-rezepte-login') > (-1)) {
                            $('.header-main .user-login-link').trigger('click');
                    }
        else if ((typeof event.data === 'string' || event.data instanceof String) && event.data.indexOf('iframe-rezepte-') > (-1)) {
            var height = event.data.replace('iframe-rezepte-', '');
            $('iframe.formular--rezept').attr('height', height);
            console.log('Got height: ' + height);
        }
    }
}

</script>                <button id="topButton"><div id="topButtonArrow"><div></div></div><div id="topButtonLabel">nach oben</div></button>
