    console.log('social icons delo was loaded')
    var getWindowOptions = function () {
        var width = 500;
        var height = 350;
        var left = (window.innerWidth / 2) - (width / 2);
        var top = (window.innerHeight / 2) - (height / 2);

        return [
            'resizable,scrollbars,status',
            'height=' + height,
            'width=' + width,
            'left=' + left,
            'top=' + top,
        ].join();
    };
    var url = 'https://www.delo.si/novice/slovenija/za-193-milijonov-evrov-kriznih-dodatkov/'

    var fbShare = function () {
        FB.ui({
            method: 'share',
            href: url
        }, function (response) {
        });
    };

    var fbMessenger = function () {
        FB.ui({
            method: 'send',
            link: url,
        }, function (response) {
        });
    };

    var twitter = function (e) {
        var win = window.open('https://twitter.com/intent/tweet?url=' + url,
            '',
            getWindowOptions());
        win.opener = null;
    };
</script>
