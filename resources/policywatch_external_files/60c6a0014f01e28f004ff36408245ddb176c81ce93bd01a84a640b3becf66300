    <script>
     
        $(document).ready(structureArticle());

        $(function () {
            shareButtons();


            var tagLists = 'Covid-19';

            var data = {
                tags: tagLists.split("|")
            };

            if (data.tags != '') {
                var url = "~/Handlers/SiteContent.ashx";

                if (url.indexOf("~/") === 0) {
                    url = baseUrl + url.substring(2);
                }

                $.ajax({
                    url: url,
                    type: "POST",
                    async: true,
                    data: { action: "get_business_categories", query: tagLists },
                    dataType: "json",
                    success: function (msg) {

                        if (msg !== null && msg.length >= 2) {
                            var html = "<div class='b2b-boxes packed' style='margin-left: -5px; margin-right: -5px;'>";

                            var loopLimit = 4;

                            var imageWidth = 25;

                            if (msg.length < 4) {
                                loopLimit = msg.length;

                                imageWidth = 100 / msg.length;
                            }

                            for (var i = 0; i < loopLimit; i++) {
                                var val = msg[i];

                                html += "<div class='b2b-box'><a href='"
                                    + val.CategoryUrl + "'><div class='wrapper'><img src='"
                                    + val.ImageUrl + "/275/275' class='srctobgd' /><div class='overlay'></div><div class='title'><span class='box'>"
                                    + val.Name + "</span></div></div></a></div>";

                            }

                            html += "</div>";

                            $('#b2btags_thread').append(html);

                            //var link = document.domain + "/en/b2b-directory";
                            //var from = "<p>FROM <a href='" + link + "'>B2B DIRECTORY</a></p>";
                            $('#b2btags_from').append("<p>FROM <a href='b2b-directory'>B2B DIRECTORY</a></p>");

                            $(".srctobgd").each(function () {
                                var image = $(this);

                                if (image.attr("src").length > 0) {
                                    $(image).parent().css('background-image', "url(" + image.attr("src") + ")");
                                    $(image).remove();
                                }
                            });
                        }

                        b2bBoxes();
                    },
                    error: function (jqXHR, textStatus, errorThrown) {

                    }
                });
            }

            if (window.location.href.indexOf("&preview=") <= -1) {
                $('.fb_comments-wrapper').append('<div class="fb-comments" data-href="' + window.location.href + '" data-width="100%" data-numposts="5" data-order-by="reverse_time"></div>')
            }

            midContentAdvert();
        });

        function structureArticle() {
            $('.article-container').first().unwrap();
            $('.article-container:eq(1)').before($('.article-container').first());
        }

        function midContentAdvert() {
            const index = Math.ceil($('.article-container .content > *, .article-container-v2 .content > *').length / 2);
            $('#midContentAdvert .advert').insertBefore($('.content > *:eq(' + index + ')'));
        }
    </script>


