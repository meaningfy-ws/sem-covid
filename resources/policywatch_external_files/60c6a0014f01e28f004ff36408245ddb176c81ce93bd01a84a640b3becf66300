    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.11.3/jquery.min.js" type="text/javascript"></script>
    <script src='/assets/bundles/js/main.min.js'></script>
    <script src='/assets/js/custom.es5.min.js' type="text/javascript"></script>

    <script src="/assets/js/stickUp.min.js" type="text/javascript"></script>

    <script type="text/javascript">
        var isMobilePlatform = (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) ? true : false;
        var baseUrl = "/";

        $(function () {

            $("#mm-canvas").mmenu({
                offCanvas: {
                    position: "right"
                }
            }, {
                offCanvas: {
                    pageSelector: "#mm-wrapper",
                    pageNodetype: "section"
                },
            });

            var API = $("#mm-canvas").data("mmenu");

            $("#mm-open").click(function () {
                API.open();
            });

            masterFunctions();
            menuFunctions();

            $(".mm-listview > li.ww > a").click(function (e) {
                if($('#pageName').text() != "The Malta Chamber: The Voice of Business"){
                    window.location.replace("https://www.maltachamber.org.mt/#ww-widget")
                }else{
                    API.closeAllPanels();
                    setTimeout(function(){ $(window).scrollTop($('#ww-widget').offset().top - 90); }, 700)
                }
            });

            $("#mm-canvas .mm-panel#mm-0 .mm-listview").append($(".header .social-link-cells.desktop .link-cell").clone()); 


            $('.lazy').Lazy({
                scrollDirection: 'vertical',
                effect: 'fadeIn',
                visibleOnly: true
            });

            if (!isMobilePlatform) {
                $('.sticky-ad-wrapper').stickUp({

                    keepInWrapper: true,
                    topMargin: '70px'

                });
            }

            $('#btnNewsLetterSubscribe').on('click', function (e) {

                var url = "~/Handlers/newsletterSubscribe.ashx";

                if (url.indexOf("~/") === 0) {
                    url = baseUrl + url.substring(2);
                }

                var subscribe = {
                    email: $('#txtNewsLetterEmail').val(),
                    name: $('#txtNewsLetterName').val(),
                    surname: $('#txtNewsLetterSurname').val()
                };

                $.ajax({
                    url: url,
                    type: "POST",
                    async: true,
                    data: { action: "newsletter_subscribe", query: JSON.stringify(subscribe) },
                    dataType: "json",
                    success: function (msg) {

                        $('#txtNewsLetterEmail').val('');
                        $('#txtNewsLetterName').val('');
                        $('#txtNewsLetterSurname').val('');

                        popupTrigger('close', 'popup-chamberlink-join')
                    },
                    error: function (jqXHR, textStatus, errorThrown) {

                    }
                })

            });

            //temporary, only show 6 items in dropdown menu
            $('#menu nav > ul > li').each(function () {
                $(this).find('> ul > li').slice(5).hide();
            });
        });
    </script>

