<script>

	jQuery(document).ready(function($) {

		// Add post content margin top
		stickySocialIcons();

		// Display main content
		setTimeout(function(){
			// $(".wrapper-singlepost").css({ paddingTop: $("#sticky").outerHeight() + 'px' });
			// $(".wrapper-singlepost, .header-title").css({opacity: 1});
		}, 50);

		// Bind click on comment in header
		$(document).on( "click", ".post-comments-no, .social-copied .total-comments", function() {
			$('html,body').animate({
				scrollTop: $("#coral_thread").offset().top},
				'slow');
		});

	});

	jQuery(function($){

		newc = ""; // New Clipboard
	});

	// Copy social icons to sticky header
	function stickySocialIcons(){
		// Make comments badge

		// TODO: uncomment when implementing comments
		var commentsNumberDOM = "";

		if( $(".post-comments-no").length ){
			commentsNumberDOM = "<span class='total-comments'>" + jQuery(".main-head .post-comments-no").text().replace(/[A-Za-z/ ]+/gm, '') + "</span>";
		}

		// Copy social icons to the header with new comments badge
		var link = jQuery("div[heateor-sss-data-href]").attr("heateor-sss-data-href");
		var t = jQuery(".heateor_sss_sharing_ul").html();
		jQuery('<div class="social-copied">'+commentsNumberDOM+'<ul class="heateor_sss_sharing_ul" heateor-sss-data-href="'+link+'"><li class="share-text heateorSssSharingRound"> Podijeli :</li>' + t + '</ul></div>').insertAfter(".entry-title");

		// Init new Clipboard for new social icon
		newc = new ClipboardJS('.heateorSssCopyLinkSvg', {
			text: function(trigger) {
			var element = trigger.parentElement.parentElement.parentElement;
			var url = jQuery(element).attr("heateor-sss-data-href") || "";
				return url;
			}
		});

		// On Success COPY link
		newc.on('success', function(e) {
			alert(heateorSssCopyLinkMessage);
		});
		stickySocialInitialized = true;
	}

</script>
<!-- end Sticky  header -->

