    <p id="ctl00_CookieInfo_pCookieText">This website uses its own and third-party cookies to maintain the session, offer a better user experience and obtain statistical data on user navigation. For more information see <a title='Cookies policy' target='_self' href='/lang/en/Paginas/cookie-policy.aspx'>Cookies policy</a></p>
    <a href="/" id="ctl00_CookieInfo_btnLink" class="button" target="_self" onclick="btnCookieClick(this, 365, &#39;ON&#39;); return false;" title="Accept">Accept</a>
    <a href="/" id="ctl00_CookieInfo_btnLinkNO" class="button buttonred" target="_self" onclick="btnCookieClick(this, 0, &#39;OFF&#39;); return false;" title="Reject">Reject</a>
</div>
    
<script>
    // Aumentar versión si es necesario que se vuelvan a aceptar las cookies por parte de los usuarios
    var version = "v2";
    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        var f = d.toUTCString();
        var expires = '';
        if (exdays > 0) {
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            expires = "expires=" + d.toUTCString();
        }
        document.cookie = cname + "=" + cvalue + " - " + f + ";" + expires + ";secure" + ";path=/";
    }    

    function showCookie(cname) {
        var cookie = getCookie(cname);
        if (cookie != "" && cookie.indexOf(version) !== -1) {
            return false;
        } else {
            return true;
        }
    }

    function btnCookieClick(elem, days, message)
    {
        setCookie('CONSENT', version + " - " + message + " - en", days);
        elem.parentElement.classList.add('hidden');
    }

    if (showCookie('CONSENT')) {
        document.getElementById('divCookieInfo').classList.remove('hidden');
    }
    

</script>
<!-- CookieInfo_Fin -->

