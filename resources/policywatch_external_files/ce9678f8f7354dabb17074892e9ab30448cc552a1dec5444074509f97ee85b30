var sync_leaderboard_id = undefined;
(function() {
function createPbar(id) {
var pDiv = document.createElement('div');
pDiv.setAttribute('id', id);
pDiv.setAttribute('class', 'ad_progressbar');
pDiv.setAttribute('style', 'visibility: visible;');
setTimeout(function() {
if(document.getElementById(id)) {
document.getElementById(id).remove();
}
}, 2000);
return pDiv;
};
function getSyncJSON(category) {
return JSON.parse('{"mtvuutiset_fi":{"leaderboard-1":{"breakpoints":[{"breakpoint":1440,"id":"521871"}]}},"default":{"leaderboard-1":{"breakpoints":[{"breakpoint":1440,"id":"600970"}]}},"haku":{"leaderboard-1":{"breakpoints":[{"breakpoint":1440,"id":"600944"}]}},"video":{"leaderboard-1":{"breakpoints":[{"breakpoint":1440,"id":"521870"}]}},"suosituimmat":{"leaderboard-1":{"breakpoints":[{"breakpoint":1440,"id":"521870"}]}},"uusimmat":{"leaderboard-1":{"breakpoints":[{"breakpoint":1440,"id":"521870"}]}},"poiminnat":{"leaderboard-1":{"breakpoints":[{"breakpoint":1440,"id":"521870"}]}},"palaute":{"leaderboard-1":{"breakpoints":[{"breakpoint":1440,"id":"521870"}]}},"uutiset":{"leaderboard-1":{"breakpoints":[{"breakpoint":1440,"id":"521870"}]}},"sport":{"leaderboard-1":{"breakpoints":[{"breakpoint":1440,"id":"521867"}]}},"urheilu":{"leaderboard-1":{"breakpoints":[{"breakpoint":1440,"id":"521867"}]}},"viihde":{"leaderboard-1":{"breakpoints":[{"breakpoint":1440,"id":"511341"}]}},"lifestyle":{"leaderboard-1":{"breakpoints":[{"breakpoint":1440,"id":"521869"}]}},"autot":{"leaderboard-1":{"breakpoints":[{"breakpoint":1440,"id":"521872"}]}},"digi":{"leaderboard-1":{"breakpoints":[{"breakpoint":1440,"id":"521873"}]}},"koti":{"leaderboard-1":{"breakpoints":[{"breakpoint":1440,"id":"521885"}]}}}')[category];
};

function getSyncIdFromJson(slot, category) {
var json = getSyncJSON(category) || getSyncJSON('default');
try {
return json[slot]["breakpoints"][0].id;
} catch(err){
return undefined;
}
};

var getCategoryAndTagMapping = function(category, microTags, rules) {
var matchingCategory = rules.filter(function(rule) { return rule.category === category }) // pick only rules that match the current page category

var matchingMicroTag = microTags
  .map(function(tag) { return rules.filter(function(rule) { return rule.matches === tag })[0] })
  .filter(Boolean); // pick rules which match a specific microtag
  
var matchingMicrotagAndCategory = matchingCategory
  .filter(function(rule) { return rule.matches.indexOf(':') > -1 }) // pick only rules which have a microtag attached to them ('lifestyle:digi')
  .filter(function(rule) { return microTags.indexOf(rule.matches.split(':')[1]) > -1 }); // pick only rules that have corresponding microTag on the page

var onlyCategory = matchingCategory.filter(function(rule) { return rule.matches.indexOf(':') === -1 }); // pick only rules with category, e.g. 'lifestyle'

var rule = matchingMicroTag[0] || matchingMicrotagAndCategory[0] || onlyCategory[0];
// If negatives within rule match the site microtags, we should not use the rule
var matchesNegatives = rule && rule.negatives !== '' && microtags.filter(function() { return new RegExp(rule.negatives).test(t) }).length > 0;
var ruleName = rule && !matchesNegatives ? rule.matches : 'default';

return ruleName;
};

var getCategoryRules = function() {
/*
* matches: what to match
* negatives: skip these
* category: which key to search in data = returns value (JSON)
*/
return [
{matches: 'mtvuutiset_fi', negatives: '', category: 'mtvuutiset_fi'},
{matches: 'saa', negatives: '', category: 'saa'},
{matches: 'uutiset', negatives: '', category: 'uutiset'},
{matches: 'sport', negatives: '', category: 'sport'},
{matches: 'sport', negatives: '', category: 'urheilu'},
{matches: 'viihde', negatives: '', category: 'viihde'},
{matches: '404', negatives: '', category: '404'},
{matches: 'haku', negatives: '', category: 'haku'},
{matches: 'lifestyle', negatives: '', category: 'lifestyle'},
{matches: 'koti', negatives: '', category: ''},
{matches: 'autot', negatives: '', category: ''},
{matches: 'digi', negatives: '', category: ''},
{matches: 'uutiset', negatives: '', category: 'suosituimmat'},
{matches: 'uutiset', negatives: '', category: 'poiminnat'},
{matches: 'uutiset', negatives: '', category: 'video'},
{matches: 'uutiset', negatives: '', category: 'palaute'},
{matches: 'uutiset', negatives: '', category: 'uusimmat'},
{matches: 'teema', negatives: '', category: 'teema'}
];
}

function getCategoryTargeting() {
var microtags = window.digitalData.mtv.cm.microtags.split(':');
var c = window.digitalData.mtv.cm.category.indexOf(':') > -1 ? window.digitalData.mtv.cm.category.split(':')[0] : window.digitalData.mtv.cm.category === '' ? 'default': window.digitalData.mtv.cm.category;
c = getCategoryAndTagMapping(c, microtags, getCategoryRules());
c = getSyncJSON(c) ? c : 'default';
return c;
}

function setSyncTargeting() {
var c = getCategoryTargeting();
var q_id = window.digitalData.mtv.cm.id;
var microtags = window.digitalData.mtv.cm.microtags.split(':');
var q_microtags = '';
var q_category = 'mtv.cm.category:' + c;
for(var i = 0, len = microtags.length ; i < len; ++i) {
q_microtags += ',mtv.cm.microtag:' + microtags[i];
}

// adform enreachresp_a targeting
var q_ecIDs = '';
var ecIDs = (window.enreachresp_a || '').split(',');
if ( ecIDs && ecIDs.length ) {
for(var i = 0 ; i < ecIDs.length ; i++) {
q_ecIDs += ',ecid:' + ecIDs[i].replace('ecid:','');
}
}

var q_type = ',mtv.cm.type:' + window.digitalData.mtv.cm.type;
var q_hasvideo = ',mtv.cm.hasvideo:' + window.digitalData.mtv.cm.hasvideo || '';
var q_autoplay = ',mtv.cm.autoplay:' + window.digitalData.mtv.cm.autoplay || '';

return '&mkw=' + q_id + '&mkv=' + q_category + q_microtags  + q_ecIDs;
};
var w = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
if(document.location.href.indexOf('/blueprint/') == -1 && w >= 1440) {
var category = getCategoryTargeting();

window._adform = window._adform || [];
_adform.push(['on.draw', function(settings){
var type = this.options.BANNER_TYPE;

var limitWidth = function(elem) {
if(!elem) return;
elem.style.width = '100%';
elem.style.margin = 0;
}
if(type === 'Floating Banner') {
window.setTimeout(function() {
document.querySelector('#leaderboard-1').parentNode.style.display = 'none'
limitWidth(document.querySelector('header nav'));
limitWidth(document.querySelector('.release-dropdown'));
var cookieNote = document.querySelector('.mtvcookienotificationscontainer');

var footerMutation = new MutationObserver(function(mut, observer) {
document.querySelector('footer').style = '';
observer.disconnect();
});

footerMutation.observe(document.querySelector('footer'), { attributeFilter: ['style'], attributes: true })

if(cookieNote) {
cookieNote.style.maxWidth = '1220px';
cookieNote.style.margin = '0 auto';
}
}, 100)

}
}]);

sync_leaderboard_id = getSyncIdFromJson("leaderboard-1", category);
var noAds = window.black_tags && window.black_tags.length > 0 ? anyMatchInArray(window.black_tags, window.digitalData.mtv.cm.microtags.split(':')) : false;
if(noAds === true) {
  document.getElementsByClassName('leaderboard-1')[0].remove();
}
if(!noAds) {
var lb_1 = document.getElementById('leaderboard-1');
if(sync_leaderboard_id && lb_1) {
lb_1.appendChild(createPbar('lb_progress_1'));
document.write('<script id="sync-leaderboard-1" src="//adx.adform.net/adx/?mid=' + sync_leaderboard_id+ setSyncTargeting() + '"><\/script>');
}
}
}
})();
</script>
</div>
</div><div class="component newsticker" data-timestamp="2021-01-29T17:23:35.000+02:00">
