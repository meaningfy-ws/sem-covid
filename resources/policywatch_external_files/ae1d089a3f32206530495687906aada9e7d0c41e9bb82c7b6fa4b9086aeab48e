    $("a[href^='http://www.revisorforeningen.no/?did=']")
   .each(function () {
       console.log("hit");
       this.href = this.href.replace("http://www.revisorforeningen.no/?did=", "http://www.revisorforeningen.no/");
   });
    $("a[href^='default.aspx?did=']")
    .each(function () {
        var id = getURLParameter(this.href, "did");
        console.log(id);
        this.href = "http://www.revisorforeningen.no/" + id;
    });

    function getURLParameter(url, name) {
        return (RegExp(name + '=' + '(.+?)(&|$)').exec(url) || [, null])[1];
    }
