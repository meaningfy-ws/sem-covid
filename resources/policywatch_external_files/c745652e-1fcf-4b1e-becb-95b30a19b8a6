    <script>
        jQuery(window).scroll(function() {
            var scroll = jQuery(window).scrollTop();

            if (scroll > 120) {
                jQuery(".language-switcher-locale-url").addClass("switch_language");
                jQuery(".en").addClass("switch_language_float");
                jQuery("li.it.first").addClass("switch_it_float");
              if ($(window).width() < 769)
              {
                jQuery(".home_it").addClass("home_but_translate");
                jQuery(".home_en").addClass("home_but_translate");
                jQuery(".home_multilanguage").addClass("home_multilanguage_switch");
              }
            }
            else {
                jQuery(".language-switcher-locale-url").removeClass("switch_language");
                jQuery(".en").removeClass("switch_language_float");
                jQuery("li.it.first").removeClass("switch_it_float");
              if ($(window).width() < 769)
              {
                jQuery(".home_it").removeClass("home_but_translate");
                jQuery(".home_en").removeClass("home_but_translate");
                jQuery(".home_multilanguage").removeClass("home_multilanguage_switch");
              }
            }
        });
    </script>
