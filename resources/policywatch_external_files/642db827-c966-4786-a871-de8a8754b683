<script type="text/javascript">
	function showCitat(){
		document.getElementById('osnovniCitat').style.display="";
		document.getElementById('strokAnaliZgodovina').style.display="none";
		document.getElementById('zgodovinaLink').style.textDecoration="underline"; 
		document.getElementById('zgodovinaLink').style.fontWeight="normal";
		document.getElementById('citatLink').style.textDecoration="none";
		document.getElementById('citatLink').style.fontWeight="bolder";
	}

	function showZgodovina(){
		document.getElementById('osnovniCitat').style.display="none";
		document.getElementById('strokAnaliZgodovina').style.display="";
		document.getElementById('zgodovinaLink').style.textDecoration="none"; 
		document.getElementById('zgodovinaLink').style.fontWeight="bolder";
		document.getElementById('citatLink').style.textDecoration="underline";
		document.getElementById('citatLink').style.fontWeight="normal";
	}
</script>
		<div id="osnovni" class="mainBody" style="float:left; position:relative; padding-top: 0px; width:100%;">
			<table id="naslovZgodovina" cellspacing="0" border="0" style="width:100%;border-collapse:collapse; ">
				<tr>
					<td>
                        <h1 style="color:#529bb9 !important; font-size:23px !important; font-weight:bold !important; padding:12px 0 20px 0; line-height:25px; font-family: Arial,Helvetica,sans-serif;">
							Zakon o pokojninskem in invalidskem zavarovanju (ZPIZ-2)
							
								&#8225;
							
						</h1>
						<!--[if lte IE 9]>
						<div style="color:red">
						Verzija brskalnika IE, ki ga uporabljate, je nižja od 9. Če želite uporabljati vse funkcionalnosti spletne strani, si namestite novejšo verzijo brskalnika ali uporabite brskalnik Firefox oziroma Chrome. </div>
						<![endif]-->
						
							<h2 class="grey">
								
									Tretji in sedmi odstavek novega 40.a člena se začneta uporabljati 1. maja 2020 (glej 140. člen ZPIZ-2G).<br/>
								
							</h2>
						
					</td>
				</tr>
				<tr>
					<td colspan="2">   
				        <table width="100%">
