	<script>
		$(document).ready(function(){
			$("#f2-2").click(function(){
				if ($(this).is(':checked')) {
					$('input[name="s"]').attr('name', 'f1');
					$('input[name="wpcfs"]').val('preset-1');
				} else {
					$('input[name="f1"]').attr('name', 's');
					$('input[name="wpcfs"]').val('0');
				}
			});
		});
	</script>

