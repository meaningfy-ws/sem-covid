<script>

$(document).ready(function () {


    $("#CB_1").click(function(){
        if ($(this).is(':checked')) {
            $("#hidden_CB_1").prop( "disabled", true );
        } else {
            $("#hidden_CB_1").prop( "disabled", false );
        }
    });

    $("#CB_2").click(function(){
        if ($(this).is(':checked')) {
            $("#hidden_CB_2").prop( "disabled", true );
        } else {
            $("#hidden_CB_2").prop( "disabled", false );
        }
    });

    $("#CB_3").click(function(){
        if ($(this).is(':checked')) {
            $("#hidden_CB_3").prop( "disabled", true );
        } else {
            $("#hidden_CB_3").prop( "disabled", false );
        }
    });

    $("#CB_4").click(function(){
        if ($(this).is(':checked')) {
            $("#hidden_CB_4").prop( "disabled", true );
        } else {
            $("#hidden_CB_4").prop( "disabled", false );
        }
    });

    $("#CB_5").click(function(){
        if ($(this).is(':checked')) {
            $("#hidden_CB_5").prop( "disabled", true );
        } else {
            $("#hidden_CB_5").prop( "disabled", false );
        }
    });

    $("#CB_6").click(function(){
        if ($(this).is(':checked')) {
            $("#hidden_CB_6").prop( "disabled", true );
        } else {
            $("#hidden_CB_6").prop( "disabled", false );
        }
    });

    $("#CB_7").click(function(){
        if ($(this).is(':checked')) {
            $("#hidden_CB_7").prop( "disabled", true );
        } else {
            $("#hidden_CB_7").prop( "disabled", false );
        }
    });



    $('#feliratkozomFormSubBtn').click(function() {
      checked = $("input[type=checkbox].requiredCheckBox:checked").length;

      if(!checked) {
        $("#kotelezoHirlevel").modal('show');
        return false;
      }

    });
});

//    $(document).ready(function()
//    {
        grecaptcha.ready(function()
        {
            grecaptcha.execute('6LeT8JMUAAAAAE8tpfPe8JCZVP8wItziFcNzFocd', {action: 'newsletter'}).
            then(function(gtoken)
            {
                $('#form4493').validator().on('submit', function (e)
                {
                    if (!e.isDefaultPrevented())
                    {
                        $("#feliratkozomFormSubBtn").attr("disabled", true);
                        e.preventDefault();
                        $.ajax(
                        {
                            url:'wp-admin/admin-ajax.php',//'https://s1364398973.t.eloqua.com/e/f2',
                            type:'post',
                            data:
                            {
                                action : 'google_recaptcha_v3',
                                formdata:$('#form4493').serialize(),
                                token: gtoken
                            },
                            success:function(resp)
                            {
                                //console.log(resp);
                                $('#eloqua-form').modal('hide');
                                $("#form4493").trigger('reset');
                                $("#feliratkozomFormSubBtn").attr("disabled", false);
                                $("#form4493").validator('update');
                                if(resp.success=="true")
                                {
                                    $('#feedbackModal').modal('show');
                                }
                                else
                                {
                                    $('#captchaSpamErrorModal').modal('show');
                                }
                            }
                        });
                    }
                });
            })
        })
//    });
</script>
