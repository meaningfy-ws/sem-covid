 $(document).ready(function () {
 var bNascondiDivCookie = leggiCookie("nascondiDivCookie");
 var bDisattivaDivCookie = 'true';
 if (bNascondiDivCookie != 'true' && bDisattivaDivCookie == 'true') {
 $("#cookie_directive_container").appendTo("#cookie_directive");
 $('#cookie_directive_container').show();

 var distance = $(window).scrollTop();
 $(window).scroll(function () {
 if ($(this).scrollTop() != distance) {
 $('#cookie_directive_container').hide();
 }
 });
 }
 });
 $("#cookie_accept").click(function () {
 $('#cookie_directive_container').hide();
 scriviCookie("nascondiDivCookie", "true", 7200)
 });
 $("#cookie_directive_container .close").click(function () {
 $('#cookie_directive_container').hide();
 scriviCookie("nascondiDivCookie", "true", 7200)
 });

