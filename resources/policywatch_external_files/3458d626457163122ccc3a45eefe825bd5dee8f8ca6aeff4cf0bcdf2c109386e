 $("#modal-servizio-accesso").on('show.bs.modal', function () {

 CentraModali("servizio-accesso");
 });

 $('#modal-servizio-accesso').on('hidden.bs.modal', function () {
 window.location = window.location.href.split("?")[0];
 });


