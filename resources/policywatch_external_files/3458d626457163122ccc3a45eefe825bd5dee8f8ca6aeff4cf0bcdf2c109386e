 CentraModali("servizio-accesso");

 });

 /* Skiplink */
 $(function () {
 //Quando un link dentro il div skiplink ha il fous, il div contenitore appare
 $('.skiplink a').on({
 'focus': function () {
 $('.skiplink').removeClass('sr-only').addClass('skiplink--focused');
 },
 'blur': function () {
 $('.skiplink').addClass('sr-only').removeClass('skiplink--focused');
 }
 });
 });
</script>
<script type="text/javascript">$(document).ready(function () {   $('.service-list a').click(function () {       if (this.href != 'javascript:void(0)') {            var btnDisabled = this;            var oldHref = this.href;           var oldClick = this.attributes["onclick"].nodeValue;     var tmpFunc = new Function(oldClick);    tmpFunc();      this.href = 'javascript:void(0)';           this.removeAttribute('onclick');           setTimeout(function () { btnDisabled.href = oldHref; btnDisabled.setAttribute('onclick', oldClick); }, 5000);           window.location.href = oldHref;       }   });});</script>
