<script type="text/javascript">
 //rimensionamento della modale con iframe al clic del link per farla apparire
 $("#modal-iframe").on('shown.bs.modal', function () {

 $("#modal-iframe .modal-body").height($("#modal-iframe .modal-dialog").height() - 155);

 });

 //rimensionamento della modale con iframe al ridimensionamento della finestra
 $(window).resize(function () {
 $("#modal-iframe .modal-body").height($("#modal-iframe .modal-dialog").height() - 155);

 });
