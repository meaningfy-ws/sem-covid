 function spinAttendi()
 {
 // visualizzo lo spin di attesa se la chat operatore è chiusa o non definita
 if (typeof b_ChatOperatoreIsOpen === 'undefined' || b_ChatOperatoreIsOpen === null || !b_ChatOperatoreIsOpen)
 setTimeout("$('#divSpinAlCentro').css('visibility', 'visible');$('#overlay').show()", 1000);
 }


