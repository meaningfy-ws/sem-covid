<script type="text/javascript">
 

 $("#card-menu-utente").click(function () {
 $('#dropdown-menu-utente').css('display', 'block');
 });
 $("#card-menu-utente").mouseover(function () {
 $('#dropdown-menu-utente').css('display', 'block');
 });
 $("#card-menu-utente").blur(function () {
 $('#dropdown-menu-utente').css('display', 'none');
 });
 $(".header-global__personal-card").mouseleave(function () {
 $('#dropdown-menu-utente').css('display', 'none');
 });

 $(function () {
 findModalScrollableHeight();
 });
 $(window).resize(function () {
 findModalScrollableHeight();
 });

 //Funzione per definire l'area scrollabile della modale
 var findModalScrollableHeight = function () {
 //verifico se la grandezza della finestra è superiore a 500px
 if ($(window).height() > 500) {
 //alla grandezza della finestra tolgo 300px di ingonmbro della modale, uso il risultato per definire il corpo della modale che è scrollabile 
 var altezza = ($(window).height() - 300);
 $(".modal.scrollable .modal-body").height(altezza);
 } else {
 $(".modal.scrollable .modal-body").height(250);
 }
 };
</script>


<script type="text/javascript">
 $('body').on('click', function (e) {
 //elimino le popover al clic tranne se clicco nella popover stessa
 if ($(e.target).data('toggle') !== 'popover'
 && $(e.target).parents('[data-toggle="popover"]').length === 0
 && $(e.target).parents('.popover.in').length === 0) {
 $('[data-toggle="popover"]').popover('hide');
 }
 });

 $('[data-toggle="popover"]').popover({
 trigger: 'hover',
 placement: 'top',
 html: true,
 delay: {
 show: 100,
 hide: 20000
 },
 content: function () {
 //recupero il div di riferimento e lo inserisco come contentuto della popover
 return $('#' + $(this).attr("id") + '-content').html();
 }
 });


 //Andando sul link di una popover disabilito le altre
 $('[data-toggle="popover"]').on('mouseover', function (e) {
 $('[data-toggle="popover"]').not(this).popover('hide');
 });
 
 $(window).scroll(function () {
 //faccio sparire le popover allo scroll
 $('[data-toggle="popover"]').popover('hide');
 });

 


</script>

