<script>
    $(document).ready(function(){
        $(".viewmore a").click(function(){
            $(this).parent().next('.more-items').slideToggle(1200);
            $(this).toggleClass('active');
            if( $(this).hasClass('active') )
                $(this).text('View less...');
            else
                $(this).text('View more...');
            return false;
        });
    });
</script>
