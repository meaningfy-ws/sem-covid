<script>
    $(document).ready(function(){
        $('iframe').each(function(){
            var url = $(this).attr("src");
            var char = "?";
            if(url.indexOf("?") != -1){ //is there a ? in the URL ?
                char = "&";
            }
            $(this).attr("src",url+char+"wmode=transparent");
        });
    });
</script>









<script>

    //GOOGLE MAPS POINTER EVENTS

$('.map-frame').addClass('pointer'); // set the pointer events to none on doc ready
$('.map').on('click', function() {
    $('.map-frame').removeClass('pointer'); // set the pointer events true on click
});

// disable pointer events when the mouse leave the canvas area;

$( ".map" ).mouseleave(function() {
    $('.map-frame').addClass('pointer'); // set the pointer events to none when mouse leaves the map area
});

//END GOOGLE MAPS POINTER EVENTS//
</script>



