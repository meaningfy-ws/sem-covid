 <script>
     $(function()    {
         /*
         var ga = function(args)    {console.log(arguments);};*/
         var url = document.location.href.replace(/\/+$/, "");
         var lastSegment = url.substr(url.lastIndexOf('/') + 1);
         if($('.js-ican-debt-recovery-thank-you').length > 0)   {
             ga('send', 'event', 'Download', 'pageLoad', 'September Burst', 1);
         }
         $('.email .ga-email-tracking').click(function()  {
             if(['debt-recovery', 'financial-sector-debt-recovery', 'commercial-debt-recovery', 'public-sector-debt-recovery', 'international-debt-recovery'].indexOf(lastSegment) !== -1)    {
                 ga('send', 'event', 'Email', 'click', 'September Burst', 1);
             }
         });
         $('.v-card-download').click(function()  {
             if(['debt-recovery', 'financial-sector-debt-recovery', 'commercial-debt-recovery', 'public-sector-debt-recovery', 'international-debt-recovery'].indexOf(lastSegment) !== -1)    {
                 ga('send', 'event', 'VCard', 'click', 'September Burst', 1);
             }
         });
     });
 </script>

 <!-- End ICAN September 2016 -->

