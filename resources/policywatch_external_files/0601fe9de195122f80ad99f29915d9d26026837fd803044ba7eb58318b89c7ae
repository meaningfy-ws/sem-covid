      "use strict";
      if (!window.nhst.cookieMessageShown) {
        $("#cookieInfo").removeClass("is-hidden");
        $("#cookieInfo").parent("body").addClass("is-cookieInfo");
      }
      $("#closeCookie").click(function () {
        try {
          localStorage.setItem('cookieInfoMessage', 'confirmed');
        } catch (error) {
          document.cookie = "cookieUsageMessage=confirmed";
        }
        $("#cookieInfo").addClass("is-hidden");
        $("#cookieInfo").parent("body").removeClass("is-cookieInfo");
      });
    })();
  </script>
</div>
