<script type="text/javascript">
	window.urlParams = function() {
		var match,
			pl = /\+/g,
			search = /([^&=]+)=?([^&]*)/g,
			decode = function(s) {
				return decodeURIComponent(s.replace(pl, " "));
			},
			query = [window.location.search.substring(1), window.location.hash.substring(2)];
		var o = {};
		for (var i = 0; i < query.length; i++)
		{
			while (match = search.exec(query[i])) {
				var v1 = decode(match[1]);
				var v2 = decode(match[2]);
				if (v2 === 'true' || v2 === 'false') {
					v2 = JSON.parse(v2);
				}
				o[v1] = v2;
			}
		}
		return o;
	}
	var options = urlParams();
	options.logged = SessionSingleSale.length > 0;	// Prisijungus vz
	options.standalone = 1032 != 1872;	// Podcasts profile - ið tiesø reiktø podcasts page, nes bus bëdø. ðiuo metu workaround per '.main-article p'
	options.premium = SessionSingleSale.toLowerCase().indexOf('premium') >= 0;
	options.threshold = options.premium ? -1 : 120;
	options.highlight = 'ial9qeN1';
	$('.main-article p, #marketing-wr p').each(function(index){
		var match = $(this).text().match(/podcast::(\S+)\s*.*$/m);
		if (match)
		{
			options.play = match[1];
			if ($(this).text() === 'podcast::' + match[1])
			{
				$(this).remove();
			}
			else
			{
				$(this).text($(this).text().replace(new RegExp('podcast::' + match[1] + '\s*'), ''));
			}
			VZPlayer.ready(function(){
				$('.main-article .img .podcast-overlay').show();
				$('.main-article .img').on('click', function(){
					S.toggle('play');
				});
				S.on('play', function(){
					$('.main-article .img .play').hide();
					$('.main-article .img .pause').show();
				});
				S.on('pause', function(){
					$('.main-article .img .play').show();
					$('.main-article .img .pause').hide();
				});
			});
		}
		options.standalone = true;
	});
	$('#appLoginLink').each(function(index, e){
		options.appLoginLink = $(e).text();
	});
	options.events = {
		dtvAccount: {
			album: 'SXHj5a3m'
		}
	};
	VZPlayer.player.ui(options);
	new JWReader(['k5NH88Y2', 'rCfhoIMy', 'BAQgMy37', 'ial9qeN1']).start();
</script>

