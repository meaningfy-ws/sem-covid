	$(document).ready(function(){
		var target = $('#popular-list-onthe');
		$.ajax({
			method: "GET",
			url: MANO.host + "/test/top-read",
			type: "json",
			success: function(data) {
				$.each($(data), function(index,rowdata){
					var number = index+1;
					var premium = (rowdata.category == 'PREMIUM') ? '<span class="premium">Premium</span>' : '';
					var comments = (rowdata.comments > 0) ? '<span class="comments"><a href="'+rowdata.url+'&amp;template=comments">'+rowdata.comments+'</a></span>' : '';
					var html = 
					'<div class="article clearfix">'+
						'<div class="article-nr">'+number+'</div>'+
						'<h2>'+
							'<a href="'+rowdata.url+'" onclick="ga(\'send\',\'event\',\'InternalLinks\',\'MostRead\',\'\');">'+rowdata.title+premium+'</a>'
							+comments+
						'</h2>'+
					'</div>';
					$(target).append(html);
				});
			}
		});
	});
