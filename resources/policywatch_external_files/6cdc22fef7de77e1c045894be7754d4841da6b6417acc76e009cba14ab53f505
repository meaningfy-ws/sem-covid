  <script type="text/javascript">
    function WarningClose() {
      jQuery.ajax({
        url: '/Handlers/SessionHandler.ashx?isCookieWarningAgreed=true',
        type: 'POST',
        success: function (data) {
          if (data == 'true') {
            jQuery("#cookie").hide();
          }
        },
        async: false
      });
    }
  </script>
  

   

