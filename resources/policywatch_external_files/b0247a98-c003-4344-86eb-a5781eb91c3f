ASPx.createControl(ASPxClientLabel,'ctl00_ctl00_ASPxSplitter1_Content_ContentSplitter_MainContent_MouseY','MouseY',{'uniqueID':'ctl00$ctl00$ASPxSplitter1$Content$ContentSplitter$MainContent$MouseY','scStates':4,'scPostfix':'DevEx'});

