         <div class="portlet"> 
          <div class="portlet-boundary portlet-boundary_fi_yja_journaldisplay_AssetDisplay_ portlet-static portlet-static-end portlet-barebone " id="p_p_id_fi_yja_journaldisplay_AssetDisplay_INSTANCE_qwbh_"> 
           <span id="p_fi_yja_journaldisplay_AssetDisplay_INSTANCE_qwbh"></span> 
           <section class="portlet" id="portlet_fi_yja_journaldisplay_AssetDisplay_INSTANCE_qwbh"> 
            <a id="aineisto-fi_yja_journaldisplay_AssetDisplay_INSTANCE_qwbh" name="aineisto-fi_yja_journaldisplay_AssetDisplay_INSTANCE_qwbh" class="yja-anchor-elements"></a> 
            <div class="portlet-content"> 
             <div class="autofit-float autofit-row portlet-header "> 
              <div class="autofit-col autofit-col-end"> 
               <div class="autofit-section"> 
               </div> 
              </div> 
