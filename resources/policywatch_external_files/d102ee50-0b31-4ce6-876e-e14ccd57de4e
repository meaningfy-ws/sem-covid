        'loadedlists': loadedlists.join('|'),
        'payflowSplittest': (win.payflowGlobal || {}).payflowSplittest
      }
    };

    var splitTest = document.querySelector('.fnSplitTest');
    if (splitTest !== null) {
      pageObj.page.splittestname = splitTest.dataset.trackingname;
      pageObj.page.splittestversion = splitTest.dataset.choice;
    }

    var personalizationElements = document.querySelectorAll('.fnPersonalized');
    if (personalizationElements.length !== 0) {
      var personalizationArray = [];
      for (i = 0; i < personalizationElements.length; i++) {
        var personalizationEl = personalizationElements[i];
        var personalizationData = {
