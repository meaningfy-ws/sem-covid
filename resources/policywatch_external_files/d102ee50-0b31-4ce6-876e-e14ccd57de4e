        };
        personalizationArray.push(personalizationData);
      }
      pageObj.page.personalization = personalizationArray;
    }

    //
      pageObj.page['page_lvl1'] = 'ekstrabladet';
    
      pageObj.page['page_lvl2'] = 'nyheder';
    
      pageObj.page['page_lvl3'] = 'politik';
    

    tempDataLayer.push(pageObj);

    //
    var userObj = {
      'user': {
        'id': userData.SsoId || '',
        
        'roles': '',
        'customer': false,
        
        'loggedin': (userData.SsoId) ? 'registered' : 'anonymous',
        'auth': !!(userData.SsoId),
        'segments': window.eb_segments || ''
      }
    };

    tempDataLayer.push(userObj);

    //
    function pxLength () {
      var returnVal;
      var articletop = document.querySelector('.headercontainer') || document.querySelector('.ekstra-header-container');
      var articlebottom = document.querySelector('.articlefooter') || document.querySelector('.bodytext--ekstra');

      if (articletop !== null && articlebottom !== null) {
        var topdiff = articletop.offsetTop;
        var bottomline = (articlebottom.offsetTop + articlebottom.clientHeight);

        returnVal = bottomline - topdiff;
      } else {
        returnVal = 'overlay'
      }
      return returnVal
    }

    var articleObj = {
      'article': {
        'cid': '8045648',
        'breaking': 'true',
        'type': 'default',
        'authors': '',
        
        'prem_redak': '',
        'pubdate': '2020-03-12 16:16:10.0',
        'pxlength': pxLength(),
        
      }
    };

    if (typeof ebTracking.ebintids !== 'undefined') {
      articleObj.article.ebintids = ebTracking.ebintids.join('|')
    }
    if (typeof ebTracking.videoplayer !== 'undefined') {
      var vidplayer = '' + ebTracking.videoplayer.join('|'),
        ebIdx = vidplayer.indexOf('EB'),
        mtgIdx = vidplayer.indexOf('MTG');

      if (ebIdx != -1 && mtgIdx != -1) {
        vidplayer = 'EB|MTG'
      } else if (ebIdx != -1) {
        vidplayer = 'EB'
      } else if (mtgIdx != -1) {
        vidplayer = 'MTG'
      }
      articleObj.article.videoplayer = vidplayer
    }

    tempDataLayer.push(articleObj);
    

    //
    for (i = tempDataLayer.length; i--;) {
      ebDataLayer.push(tempDataLayer[i]);
    }

    ebTracking.trackClick = function (eventLabel, eventAction, eventCategory) {
      ebDataLayer.push({
        'event': 'GAEvent',
        'eventCategory': eventCategory,
        'eventAction': eventAction,
        'eventLabel': eventLabel,
        'eventValue': 0,
        'eventNint': 0
      });
    }
  };

  //
  function bodytextClickTrack () {
    var eventCategory = 'UX';
    var eventAction = 'click - relaterede links';
    if (this.href.indexOf('ekstrabladet.dk') !== -1) {
      var eventLabel = (this.dataset['articleid']) ? this.dataset['articleid'] : this.href.split('/').pop().split('?')[0];
      ebTracking.trackClick(eventLabel, eventAction, eventCategory);
    }
  }

  var fnBodytextTracking = document.getElementById('fnBodytextTracking');
  if (fnBodytextTracking !== null) {
    var bodytextLinks = fnBodytextTracking.getElementsByTagName('a');
    bodytextLinksLength = bodytextLinks.length;
    for (var i = 0; i < bodytextLinksLength; i++) {
      var el = bodytextLinks[i];
      if (el.className.indexOf('link-gallery-item') === -1) {
        el.addEventListener('mousedown', bodytextClickTrack); // 
      }
    }
  }
  //

  // 
  const fronts = document.querySelectorAll('.front');
  for (var frontIdx = 0; frontIdx < fronts.length; frontIdx++) {
    const front = fronts[frontIdx];
    const childrenLinks = front.querySelectorAll('article[data-articleid]');
    const trackingObj = {
      event: 'trackAction',
      eventAction: 'click',
      dreProductId: front.dataset.product,
      dreEditionId: front.dataset.edition,
      dreDeckIdr: front.dataset.decknumber
    };
    for (var linkIdx = 0; linkIdx < childrenLinks.length; linkIdx++) {
      const link = childrenLinks[linkIdx];
      link.addEventListener('click', function(ev) {
        const artEl = ev.currentTarget;
        const trackArtObj = trackingObj;
        const spmElement = artEl.closest('div.fnSpaceManagement');
        trackArtObj.areaId=(spmElement ? spmElement.id : '');
        trackArtObj.articleUrl = artEl.querySelector('a').href;
        trackArtObj.contentId = artEl.dataset.articleid;
        window.ebDataLayer.push(trackArtObj);
      });
    }
  }
  // 

  function getGAcid () {
    try {
      var trackingIdEnv = 'UA-2135460-1';
      var trackers = ga.getAll();
      var i, len;
      for (i = 0, len = trackers.length; i < len; i += 1) {
        if (trackers[i].get('trackingId') === trackingIdEnv) { return trackers[i].get('clientId') }
      }
    } catch (err) {

      return 'false';
    }
  }

  //
  win.addEventListener('message', function (e) {
    try {
      if ((e.origin.indexOf('interactive.ekstrabladet.dk') !== -1 || e.origin.indexOf('static.ekstrabladet.dk') !== -1)  && typeof e.data !== 'undefined') {
        if (typeof e.data.eventCategory !== 'undefined') {
          var trackObject = e.data;
          trackObject.event = 'GAEvent';
          ebDataLayer.push(trackObject)
        }
      }
    } catch (err) {
      console.error('GA message handler error', err)
    }
  });

  ebTracking.options = ebTracking.options || {};
  ebTracking.options.gaDataSent = false;

  function handlePlusBtns() {
    var plusButtons = document.querySelectorAll('.fnPlusBuyBtn');
    if (plusButtons.length > 0) {
      var gaClientId = getGAcid();
      while (plusButtons.length > 0) {
        var el = plusButtons[0];
        el.classList.remove('fnPlusBuyBtn');
        var queryCharacter = (el.href.match(/\?/) !== null) ? '&' : '?';
        el.href = el.href + queryCharacter + 'gaClientId=' + gaClientId;
      }
    }
  }

  ebTracking.options.gaDataSent = false;
  win.addEventListener('DOMContentLoaded', function () {
    if (!ebTracking.options.gaDataSent) {
      sendGAData();
    }
    ebTracking.options.gaDataSent = true
  });
  win.addEventListener('load', function () {
    if (!ebTracking.options.gaDataSent) {
      sendGAData();
    }
    ebTracking.options.gaDataSent = true;
    handlePlusBtns();
  });
  win.getGAcid = getGAcid;
}(window.ebDataLayer = window.ebDataLayer || [], window.ebTracking = window.ebTracking || {}, window));
</script>
