;(function googleDataLayer (ebDataLayer, ebTracking, win) {

  var sendGAData = function () {
    var i;
    var userData = (function () {
      var data = {};
      if (typeof win.eblogin !== 'undefined') {
        var user = win.eblogin.currentUser() || false;
        if (user) {
          data.SsoId = user.id
        } else {
          data.cookie = false
        }
      } else {
        data.cookie = false
      }
      return data
    }());

    var tempDataLayer = [];

    //
    var pagetitle = document.title.replace(/['"]/g, '');
    var pageLists = document.querySelectorAll('[data-trackname]');
    var loadedlists = [];

    for (i = pageLists.length; i--;) {
      loadedlists.push(pageLists[i].dataset['trackname'])
    }
    var pageObj = {
      'event': 'datalayerready',
      'page': {
        'brand': 'ebmedier',
        'subbrand': 'ekstrabladetdk',
