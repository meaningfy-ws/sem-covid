<link rel="stylesheet" type="text/css" href="//cdnjs.cloudflare.com/ajax/libs/cookieconsent2/3.0.3/cookieconsent.min.css" />
<script src="//cdnjs.cloudflare.com/ajax/libs/cookieconsent2/3.0.3/cookieconsent.min.js"></script>
<script>
    window.addEventListener("load", function () {
        window.cookieconsent.initialise({
            "palette": {
                "popup": {
                    "background": "#efefef",
                    "text": "#404040"
                },
                "button": {
                    "background": "#9bcd41",
                    "text": "#ffffff"
                }
            },
            "onInitialise": function (status) {
                $.fn['gdpr-popin'].updatePreferences(status, this.hasConsented());
            },
            "onPopupOpen": function () {
                $.fn['gdpr-popin'].refreshElements();
            },
            "onStatusChange": function (status, chosenBefore) {
                $.fn['gdpr-popin'].updatePreferences(status, this.hasConsented(), chosenBefore);
            },
            "content": {
                "dismiss": "OK",
                "link": " En savoir plus",
                "href": "/fr-fr/conditions-generales-d-utilisation"
,"message": "En poursuivant votre navigation sur ce site, vous acceptez l’utilisation de cookies pour des contenus et services adaptés à vos centres d’intérêts"            }
        })
    });
</script>
