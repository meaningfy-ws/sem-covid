        <div data-portal-component-type="part" id="signup" class="l-container l-mt-3 l-mb-5">
  <section class="b-box b-box--square b-box--blue">
    <form id="86eadeff-b6a8-4945-abe8-b1a098e5da24" onsubmit="signup();" class="row">
      <div class="col-md-5 col-xl-4">
        <h2 class="h3">Abonnere på nyheter</h2>
        <label for="email-subscription" class="b-input-text__label">
          Fyll inn e-postadressen din for å motta varsler om: <br>
          <strong>
            
              Nyheter
            

            
          </strong>
        </label>
      </div>
      <div class="col-md-6 l-mb-1">
        <div class="l-flex b-input-text">
          <input type="text" id="email86eadeff-b6a8-4945-abe8-b1a098e5da24"
            class="b-input-text__input b-input-text__input--mt-1 col-md-7 l-mt-1 l-mr-1" />
          <div class="l-mt-1">
            <button class="b-button">Abonnér</button>
          </div>
        </div>
        <div id="error86eadeff-b6a8-4945-abe8-b1a098e5da24" class="b-input-text__error" style="display: none;">
          Skriv inn en gyldig epost-adresse.
        </div>
      </div>
    </form>
    <div id="success86eadeff-b6a8-4945-abe8-b1a098e5da24" class="b-input-text__description l-mt-1" style="display: none;">
      <h2 class="h3">Bekreftelse</h2>
      Abonnementet ditt er registrert. Du får straks e-post fra oss.
    </div>
  </section>
</div>

<script>
  var id = '86eadeff-b6a8-4945-abe8-b1a098e5da24'
  var serviceUrl = '/nyheter/_/service/helsedirektoratet/callSubscription'
  var type = 'tree'
  var value = '86eadeff-b6a8-4945-abe8-b1a098e5da24'
  var site = 'helsedirektoratet'
  var codes = '[]'

  $(document).ready(function () {
    $('#error' + id).hide()
    $('#success' + id).hide()

    $(document).on('submit', '#' + id, function (event) {
      event.preventDefault();
    });
  })

  function validateEmail(email) {
    var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/
    return re.test(email)
  }

  function showError() {
    $('#error' + id).show()
  }

  function showSuccess() {
    $('#' + id).hide()
    $('#success' + id).show()
  }

  var codeString = ''
  if (codes) {
    codes.substring(1, codes.length - 1).split(', ').forEach(function (code) {
      codeString += ('&code=' + code)
    })
  }

  function signup() {
    var email = $('#email' + id).val()
    var valid = validateEmail(email)
    if (valid) {
      // Call subscription
      var url = serviceUrl + '?email=' + email + '&type=' + type + '&value=' + value + '&site=' + site

      if (type === 'code') {
        url += codeString
      }

      $.ajax({
        type: 'GET',
        url: url,
        dataType: 'json',
        success: function () {
          showSuccess()
        }, error: function (jqXHR, textStatus, errorThrown) {
          showError()
        }
      })
    } else {
      showError()
    }
  }

  var hasParentWithClass = ($("#signup").parents().hasClass('l-container'))
  if (hasParentWithClass) {
    $("#signup").removeClass('l-container')
  }

</script>

      
      
    </div>

    
  </main>

  

  <div data-portal-region="beforeFooter">
    
  </div>

  <!-- importing footer from footer.html file -->
  <section class="b-box b-box--square b-box--blue-dark">
  <div class="l-container l-mt-2">
    <footer class="b-footer">
      <div class="row" role="navigation">
        <div class="col-lg-3 col-xs-12">
          <div class="">
            
              <a href="https://www.helsedirektoratet.no/om-oss/kontakt-oss" class="b-footer__link">Kontakt oss</a>
            
