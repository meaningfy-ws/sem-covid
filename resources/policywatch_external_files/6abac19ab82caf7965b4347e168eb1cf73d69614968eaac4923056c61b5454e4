<div class="slider-wrap clear">
<script type="text/javascript">
jQuery(window).load(function(){
var vimeoPlayers = jQuery("#slider-22026").find("iframe");
jQuery(vimeoPlayers).each(function(){
Froogaloop(this).addEvent("ready", ready);
});
function ready(player_id) {
Froogaloop(player_id).addEvent("play", function(data) {
jQuery("#slider-22026").flexslider("pause");
});
Froogaloop(player_id).addEvent("pause", function(data) {
jQuery("#slider-22026").flexslider("play");
});
}
jQuery("#slider-22026").flexslider({
animation: "fade",
easing: "swing",
animationSpeed:800,
slideshowSpeed:4000,
selector: ".slides > .slide",
pauseOnAction: true,
smoothHeight: false,
controlNav: true,
directionNav: false,
useCSS: false,
prevText: "Prev",
nextText: "Next",
controlsContainer: "#slider-22026-controls",
animationLoop: true,
start: function(slider) {
jQuery(slider).removeClass("flex-loading");
},
before: function(slider) {
if (slider.slides.eq(slider.currentSlide).find("iframe").length !== 0)
Froogaloop( slider.slides.eq(slider.currentSlide).find("iframe").attr("id") ).api("pause");
},
after: function(slider) {
}
});
})
</script></p>
