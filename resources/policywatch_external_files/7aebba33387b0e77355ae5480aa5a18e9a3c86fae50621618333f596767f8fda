	<script type="text/javascript">
	//<![CDATA[
	$(document).ready(function(){
		var searchText = 'Vyhľadajte'
		var defaultText = 'Vyhľadajte'
	
    	$("#searchWordsTop").focus(function () {
        	var text = $(this).val();
        	if(text == defaultText){
            	$(this).val("");
        	}else{
            	$(this).val(text);
        	}
    	});
    	$("#searchWordsTop").blur(function () {
        	var text = $(this).val();
        	if(text == ""){
            	$(this).val('Vyhľadajte');
        	}else{
            	$(this).val(text);
        	}
    	});
	});
	//]]>
	</script>


