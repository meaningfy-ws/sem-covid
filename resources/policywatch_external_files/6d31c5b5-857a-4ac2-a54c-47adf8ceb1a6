        <script>
  window.onload = function() { gouvernementMessenger() };

  function gouvernementMessenger () {
    var data = {
      'default': {
        'title': 'SITE CORONAVIRUS (COVID-19)',
        'callToAction': {
            'link': 'http://www.covid19.lu',
            'text': 'www.covid19.lu',
            'title': 'covid19.lu'
        },
        'content': '<p>Retrouvez des informations officielles sur les mesures et recommandations sanitaires, des informations pour voyageurs, des gestes de protection, informations sectorielles et des FAQ thématiques.</p>',
        'img': {
          'src': '//cdn.public.lu/gov-insert/coronavirus/decoration.png',
          'alt': '',
          'width': 189,
          'height': 127
        }
      },
      'en': {
        'title': 'WEBSITE CORONAVIRUS (COVID-19)',
        'callToAction': {
            'link': 'http://www.covid19.lu',
            'text': 'www.covid19.lu',
            'title': 'covid19.lu'
        },
        'content': '<p>Find official information on sanitary measures and recommendations, information for travellers, protective measures, sectoral information and thematic FAQ.</p>',
          'img': {
              'src': '//cdn.public.lu/gov-insert/coronavirus/decoration.png',
              'alt': '',
              'width': 189,
              'height': 127
          }
      },
      'de': {
        'title': 'WEBSEITE CORONAVIRUS (COVID-19)',
        'callToAction': {
            'link': 'http://www.covid19.lu',
            'text': 'www.covid19.lu',
            'title': 'covid19.lu'
        },
        'content': '<p>Finden Sie hier offizielle Informationen über Hygienemaßnahmen und -empfehlungen, Informationen für Reisende, Schutzgesten, Informationen zu verschiedenen Bereichen und thematische FAQs.</p>',
          'img': {
              'src': '//cdn.public.lu/gov-insert/coronavirus/decoration.png',
              'alt': '',
              'width': 189,
              'height': 127
          }
      },
      'lb': {
        'title': 'WEBSÄIT CORONAVIRUS (COVID-19)',
        'callToAction': {
            'link': 'http://www.covid19.lu',
            'text': 'www.covid19.lu',
            'title': 'covid19.lu'
        },
        'content': '<p>Hei fannt Dir offiziell Informatiounen iwwert sanitär Mesuren a Recommandatiounen, Informatiounen zu Reesen, Geste-barrièren, Informatiounen zu verschiddene Beräicher an thematesch FAQen.</p>',
          'img': {
              'src': '//cdn.public.lu/gov-insert/coronavirus/decoration.png',
              'alt': '',
              'width': 189,
              'height': 127
          }
      }
    };
    var lang = document.querySelector('html').getAttribute('lang').toLowerCase();
    var head = document.querySelector('head');
    var govbar = document.querySelector('.govbar');
    var message = document.createElement('div');
    var style = document.createElement('style');

    function getStyle () {
        return '.gouvernemental_messenger{background:url(//cdn.public.lu/gov-insert/coronavirus/border.png) repeat-x bottom left #fff;color:#000;font-family:Calibri,sans-serif;hyphens:none;line-height:1.4;margin:0 auto;padding:15px 20px;position:relative;overflow:hidden;z-index:30000}.gouvernemental_messenger-content-title{color:#e30613;font-size:24px;font-weight:600;font-style:normal;margin:0 0 15px;text-transform:uppercase}.gouvernemental_messenger-content-call-to-action{margin:0 0 15px 0}.gouvernemental_messenger-btn{background:#274891;color:#fff;display:inline-block;font-weight:700;font-size:16px;padding:10px 20px;text-align:center;text-decoration:none}.gouvernemental_messenger-btn:visited{background:#274891;color:#fff;text-decoration:none}.gouvernemental_messenger-btn:focus,.gouvernemental_messenger-btn:hover{background:#1f3a73;color:#fff;text-decoration:none}.gouvernemental_messenger-content-description{color:#000;font-size:16px}.gouvernemental_messenger-content-description p{color:#000;font-family:Calibri,sans-serif;font-size:16px;font-weight:400;font-style:normal;hyphens:none;line-height:1.4;margin:0 0 15px}.gouvernemental_messenger-content-description a{color:#274891;font-family:Calibri,sans-serif;text-decoration:underline}.gouvernemental_messenger-content-description a:focus,.gouvernemental_messenger-content-description a:hover{color:#274891;text-decoration:none}.gouvernemental_messenger-figure{display:none}.gouvernemental_messenger-figure img{border:none;display:block;margin:0 auto;width:189px}@media (min-width:45em){.gouvernemental_messenger-content-title{font-size:28px;margin-bottom:30px}.gouvernemental_messenger{padding:15px 229px 15px 20px;position:relative}.gouvernemental_messenger-figure{display:block;margin:0;padding:0 0 15px 20px;position:absolute;top:15px;right:20px;width:189px}}@media (min-width:61.5em){.gouvernemental_messenger-content{overflow:hidden}.gouvernemental_messenger-content-call-to-action{float:left;position:relative;z-index:1}.gouvernemental_messenger-content-description{box-sizing:border-box;float:left;margin-left:-150px;padding-left:170px;width:100%}}';
    }

    function createMessage () {
        var data = getContent();
        var template = '' +
            '<div class="gouvernemental_messenger-content">' +
                '<p class="gouvernemental_messenger-content-title">${title}</p>' +
                '<p class="gouvernemental_messenger-content-call-to-action">' +
                    '<a href="${call-to-action-link}" title="${call-to-action-title}" class="gouvernemental_messenger-btn" aria-hidden="true" target="_blank">${call-to-action-text}</a>' +
                '</p>' +
                '<div class="gouvernemental_messenger-content-description">' +
                    '${content}' +
                '</div>' +
            '</div>' +
            '<div class="gouvernemental_messenger-figure">' +
                '<img src="${img-src}" alt="${img-alt}" width="${img-width}" height="${img-height}">' +
            '</div>';

        return template.replace('${title}', data.title)
          .replace('${call-to-action-link}', data.callToAction.link)
          .replace('${call-to-action-text}', data.callToAction.text)
          .replace('${call-to-action-title}', data.callToAction.title)
          .replace('${content}', data.content)
          .replace('${img-src}', data.img.src)
          .replace('${img-alt}', data.img.alt)
          .replace('${img-height}', data.img.height)
          .replace('${img-width}', data.img.width)
    }

    function getContent () {
      return data[lang] !== undefined ? data[lang] : data['default'];
    }

    style.innerHTML = getStyle();
    message.setAttribute('class', 'gouvernemental_messenger');
    message.innerHTML = createMessage();

    // Insert style
    head.appendChild(style);

    // Insert message
    if (govbar !== null) {
      govbar.parentNode.insertBefore(message, govbar.nextSibling);
    } else {
      var body = document.querySelector('body');
      body.insertBefore(message, body.children[0]);
    }
  }
</script>
