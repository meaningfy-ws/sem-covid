<script>
  (function () {
    window.onload = function() {
      if (isNotExclude()) {
        gouvernementMessenger()
      }
    };

    function isNotExclude () {
        var run = true;
        var url = window.location.hostname;
        // Add here the name of the website to exclude
        // If there is two ore more websites sharing the same name (even partially), use the whole hostname.
        // Example: guichet.public.lu and helloguichet.internet.lu will be both excluded.
        var exclude = [
            'guichet'
        ];

        exclude.forEach(function (item) {
            if (url.indexOf(item) !== -1) {
                run = false;
            }
        });
        return run;
    }

    function gouvernementMessenger () {
      var data = {
        'default': {
          'title': 'SITE CORONAVIRUS (COVID-19)',
          'callToAction': {
              'link': 'http://www.covid19.lu',
              'text': 'www.covid19.lu',
              'title': 'covid19.lu - Nouvelle fenêtre'
          },
          'content': '<p>Recommandations par domaine, formulaires, actualités, informations pour les entreprises et frontaliers, un espace pour les professionnels de la santé, des FAQ, ainsi qu’une médiathèque.</p>',
          'img': {
            'src': '//cdn.public.lu/gov-insert/coronavirus/decoration.png',
            'alt': '',
            'width': 189,
            'height': 127
          }
        },
        'en': {
          'title': 'WEBSITE CORONAVIRUS (COVID-19)',
          'callToAction': {
              'link': 'http://www.covid19.lu',
              'text': 'www.covid19.lu',
              'title': 'covid19.lu - new window'
          },
          'content': '<p>Recommendations by domain, forms, news, information for companies and border workers, a space for health professionals, FAQ, as well as a media library.</p>',
            'img': {
                'src': '//cdn.public.lu/gov-insert/coronavirus/decoration.png',
                'alt': '',
                'width': 189,
                'height': 127
            }
        },
        'de': {
          'title': 'WEBSEITE CORONAVIRUS (COVID-19)',
          'callToAction': {
              'link': 'http://www.covid19.lu',
              'text': 'www.covid19.lu',
              'title': 'covid19.lu - Neues Fenster'
          },
          'content': '<p>Empfehlungen nach Bereichen, Formulare, Nachrichten, Informationen für Unternehmen und Grenzgänger, einen Raum für Gesundheitsexperten, FAQ sowie eine Mediathek.</p>',
            'img': {
                'src': '//cdn.public.lu/gov-insert/coronavirus/decoration.png',
                'alt': '',
                'width': 189,
                'height': 127
            }
        },
        'lb': {
          'title': 'WEBSÄIT CORONAVIRUS (COVID-19)',
          'callToAction': {
              'link': 'http://www.covid19.lu',
              'text': 'www.covid19.lu',
              'title': 'covid19.lu - nei Fënster'
          },
          'content': '<p>Recommandatiounen no Beräicher, Formulairen, Neiegkeeten, Informatiounen fir Betriber an Frontalieren, en Espace fir Professioneller aus der Santé, en FAQ an eng Mediathéik.</p>',
            'img': {
                'src': '//cdn.public.lu/gov-insert/coronavirus/decoration.png',
                'alt': '',
                'width': 189,
                'height': 127
            }
        }
      };
      var lang = document.querySelector('html').getAttribute('lang').toLowerCase();
      var head = document.querySelector('head');
      var govbar = document.querySelector('.govbar');
      var message = document.createElement('div');
      var style = document.createElement('style');

      function getStyle () {
          return '.gouvernemental_messenger{background:url(//cdn.public.lu/gov-insert/coronavirus/border.png) repeat-x bottom left #fff;color:#000;font-family:Calibri,sans-serif;hyphens:none;line-height:1.4;margin:0 auto;padding:15px 20px;position:relative;overflow:hidden;z-index:30000}.gouvernemental_messenger-content-title{border:none;color:#e30613;font-size:24px;font-weight:600;font-style:normal;margin:0 0 15px;padding:0;text-transform:uppercase}.gouvernemental_messenger-content-title::after,.gouvernemental_messenger-content-title::before{display:none}.gouvernemental_messenger-content-call-to-action{margin:0 0 15px 0}.gouvernemental_messenger-btn{background:#274891;color:#fff;display:inline-block;font-weight:700;font-size:16px;padding:10px 20px;text-align:center;text-decoration:none}.gouvernemental_messenger-btn:visited{background:#274891;color:#fff;text-decoration:none}.gouvernemental_messenger-btn:focus,.gouvernemental_messenger-btn:hover{background:#001951;color:#fff;text-decoration:none}.gouvernemental_messenger-content-description{display:none;color:#000;font-size:16px}.gouvernemental_messenger-content-description p{color:#000;font-family:Calibri,sans-serif;font-size:16px;font-weight:400;font-style:normal;hyphens:none;line-height:1.4;margin:0 0 15px}.gouvernemental_messenger-content-description a{color:#274891;font-family:Calibri,sans-serif;text-decoration:underline}.gouvernemental_messenger-content-description a:focus,.gouvernemental_messenger-content-description a:hover{color:#274891;text-decoration:none}.gouvernemental_messenger-figure{display:none}.gouvernemental_messenger-figure img{border:none;display:block;margin:0 auto;width:189px}@media (min-width:45em){.gouvernemental_messenger-content-title{font-size:28px;margin-bottom:30px}.gouvernemental_messenger{padding:15px 229px 15px 20px;position:relative}.gouvernemental_messenger-figure{display:block;margin:0;padding:0 0 15px 20px;position:absolute;top:15px;right:20px;width:189px}.gouvernemental_messenger-content-description{display:block}}@media (min-width:61.5em){.gouvernemental_messenger-content{overflow:hidden}.gouvernemental_messenger-content-call-to-action{float:left;position:relative;z-index:1}.gouvernemental_messenger-content-description{box-sizing:border-box;float:left;margin-left:-150px;padding-left:170px;width:100%}}';
      }

      function createMessage () {
          var data = getContent();
          var template = '' +
              '<div class="gouvernemental_messenger-content" role="complementary">' +
                  '<h1 class="gouvernemental_messenger-content-title">${title}</h1>' +
                  '<p class="gouvernemental_messenger-content-call-to-action">' +
                      '<a href="${call-to-action-link}" title="${call-to-action-title}" class="gouvernemental_messenger-btn" target="_blank">${call-to-action-text}</a>' +
                  '</p>' +
                  '<div class="gouvernemental_messenger-content-description">' +
                      '${content}' +
                  '</div>' +
              '</div>' +
              '<div class="gouvernemental_messenger-figure">' +
                  '<img src="${img-src}" alt="${img-alt}" width="${img-width}" height="${img-height}">' +
              '</div>';

          return template.replace('${title}', data.title)
            .replace('${call-to-action-link}', data.callToAction.link)
            .replace('${call-to-action-text}', data.callToAction.text)
            .replace('${call-to-action-title}', data.callToAction.title)
            .replace('${content}', data.content)
            .replace('${img-src}', data.img.src)
            .replace('${img-alt}', data.img.alt)
            .replace('${img-height}', data.img.height)
            .replace('${img-width}', data.img.width)
      }

      function getContent () {
        return data[lang] !== undefined ? data[lang] : data['default'];
      }

      style.innerHTML = getStyle();
      message.setAttribute('class', 'gouvernemental_messenger');
      message.innerHTML = createMessage();

      // Insert style
      head.appendChild(style);

      // Insert message
      if (govbar !== null) {
        govbar.parentNode.insertBefore(message, govbar.nextSibling);
      } else {
        var body = document.querySelector('body');
        body.insertBefore(message, body.children[0]);
      }
    }
  })();
</script> 
