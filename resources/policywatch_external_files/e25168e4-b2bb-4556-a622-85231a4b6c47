        <script type="text/javascript">
            function showAlertOldBrowserDialog() {
                var $dialog = $('#alert-old-browser-dialog');
                if ($dialog.length === 0) {
                    return;
                }
                $dialog.elladialog(true);
            }

            $(function() {
                showAlertOldBrowserDialog();
            });
        </script>
