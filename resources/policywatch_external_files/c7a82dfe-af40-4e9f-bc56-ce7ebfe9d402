            jQuery('.header-submenu.my-scroll').smint({
                'scrollSpeed': 1000
            });

            jQuery('.carousel').carousel();

            jQuery(".screen-watch").each(function () {
                var item  = jQuery(this);
                var shift = 0;

                if (jQuery(item).attr("data-watch-shift")) {
                    shift = jQuery(item).attr("data-watch-shift");
                }

                if (jQuery(item).attr("data-watch") == 'bottom') {
                    var item_position = parseInt(jQuery(item).height()) + parseInt(jQuery(item).offset().top) + parseInt(shift);
                } else {
                    var item_position = parseInt(jQuery(item).offset().top) + parseInt(shift);
                }

                var top_position    = $(window).scrollTop();
                var window_height   = jQuery(window).height();
                var screen_position = top_position + window_height;
                var repeat          = false;
                var first           = true;
                if (jQuery(this).attr("data-watch-repeat") == 'true') {
                    repeat = true;
                }

                var watchClasses = function (screen_position) {
                    if (screen_position >= item_position && top_position <= item_position) {
                        jQuery(item).addClass('screen-watch-active');
                        jQuery(item).removeClass('screen-watch-before').removeClass('screen-watch-after');
                        first = false;
                    } else if (screen_position < item_position && (repeat == true || first == true)) {
                        jQuery(item).addClass('screen-watch-before');
                        jQuery(item).removeClass('screen-watch-active');
                        first = false;
                    } else if (repeat) {
                        jQuery(item).addClass('screen-watch-after');
                        jQuery(item).removeClass('screen-watch-active');
                    }
                }

                watchClasses(screen_position);

                jQuery(window).scroll(function () {
                    var top_position    = $(window).scrollTop();
                    var screen_position = top_position + window_height;

                    screen_position = top_position + window_height;
                    watchClasses(screen_position);
                });
            });

                        skrollr.init({
                forceHeight:     false,
                smoothScrolling: false
            });
                    });
    </script>

