    <script type="text/javascript">
        // notifications - show message bar
        var message = '';
        if (message != '') {
            var image = '';
            var type = '' + 'MessageBar';
            var duration = '';

            // hack for IE lower versions
            if (isIE() != false && isIE() < 9) {
                var msg = "";
                msg += "<div id='" + type + "'>";
                msg += image + " " + message;
                msg += "</div>";
                var fragment = create(msg);
                document.body.insertBefore(fragment, document.body.childNodes[0]);
            }
            else {
                ShowMessageBar(message, type, image, duration);
            }
        }

        // create div - hack for IE lower versions
        function create(htmlStr) {
            var frag = document.createDocumentFragment(),
                temp = document.createElement('div');
            temp.innerHTML = htmlStr;
            while (temp.firstChild) {
                frag.appendChild(temp.firstChild);
            }
            return frag;
        }

        // check browser - hack for IE lower versions
        function isIE() {
            var myNav = navigator.userAgent.toLowerCase();
            return (myNav.indexOf('msie') != -1) ? parseInt(myNav.split('msie')[1]) : false;
        }

        // show simultaneous login message bar
        function ShowMessageBar(message, type, image, delay) {
            $("<div />", { id: type, html: image + ' ' + message }).hide().prependTo("body")
            .slideDown('fast').delay(delay).slideUp(function () { $(this).remove(); });
        }
    </script>

