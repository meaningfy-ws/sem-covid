<script type="text/javascript">
// here is the magic
function addMessage()
{
    jQuery.ajax({'url':'/contact/message/create','data':$(this).serialize(),'type':'post','dataType':'json','success':function(data)
            {
                if (data.status == 'failure')
                {
                    $('#dialogMessage div.divForForm').html(data.div);
                          // Here is the trick: on submit-> once again this function!
                    $('#dialogMessage div.divForForm form').submit(addMessage);
                }
                else
                {
                    $('#dialogMessage div.divForForm').html(data.div);
                    setTimeout("$('#dialogMessage').dialog('close') ",10000);
                }
 
            } ,'cache':false});;
    return false; 
 
}

 
</script>


          
 
</li>
                            <li><a onclick="{addSubscribe(); $(&#039;#dialogSubscribe&#039;).dialog(&#039;open&#039;);}" class="js-subscribe-link" href="#">Odoberať novinky</a> 
