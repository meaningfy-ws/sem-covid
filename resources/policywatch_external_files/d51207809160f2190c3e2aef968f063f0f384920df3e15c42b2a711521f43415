<div class="divForForm"></div>
 
</div>
<script type="text/javascript">
// here is the magic
function addSubscribe()
{
    jQuery.ajax({'url':'/subscribe/user/create','data':$(this).serialize(),'type':'post','dataType':'json','success':function(data)
            {
                if (data.status == 'failure')
                {
                    $('#dialogSubscribe div.divForForm').html(data.div);
                          // Here is the trick: on submit-> once again this function!
                    $('#dialogSubscribe div.divForForm form').submit(addSubscribe);
                }
                else
                {
                    $('#dialogSubscribe div.divForForm').html(data.div);
                    setTimeout("$('#dialogSubscribe').dialog('close') ",10000);
                }
 
            } ,'cache':false});;
    return false; 
 
}


    function subscribe_heslo_keypress(heslo_element){ 
        if(heslo_element.value.trim().length > 0){
           $('#ohlasenie_noviniek').show(); 
           $('#odober_noviniek').hide(); 
        }else{
            $('#odober_noviniek').show(); 
            $('#ohlasenie_noviniek').hide(); 
        }
    }
 
</script>


          
 
