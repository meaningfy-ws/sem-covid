<!-- Suggest preko SOLR -->
<script>
$(function() {
	
  $( "#searchPravniRedRS" ).autocomplete({

   source: function( request, response ) {
    $.ajax({
     url: "http://pisrs.si/solr/select/?wt=json&json.wrf=?",
     dataType: "jsonp",
     data: {
      q: "{!q.op=AND}(userquery:\"" + request.term + "\")",
      sort: "score desc, count desc",
      fq: "userquerytype:0",
      start: 0,
      rows: 5
     },
     
     success: function( data ) {
      response( $.map(data.response.docs, function(doc) {
         return {
           label: doc.userquery,
    	   value: doc.userquery,
         };
       }));
     }
     
    });
   },
   
   minLength: 2,
   
   select: function( event, ui ) {
   },
   
   open: function() {
    $( this ).removeClass( "ui-corner-all" ).addClass( "ui-corner-top" );
   },
   
   close: function() {
    $( this ).removeClass( "ui-corner-top" ).addClass( "ui-corner-all" );
   }
   
  });
  
});
 
$(document).ready(function() {
  $('#searchPravniRedRS').focus();
  
  $('#form').submit(function(){
		var a = document.getElementById('searchPravniRedRS');
		bool = false;
		if (a.value == ""){
			bool = false;
		} else {
			bool = true;
		}
	    return  bool;                                         
	});
});

</script>
<!-- End Suggest -->

