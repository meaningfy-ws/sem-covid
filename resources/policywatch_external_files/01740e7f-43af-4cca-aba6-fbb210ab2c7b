                <path fill-rule="evenodd" d="M86.14 78.383H65.628c-3.999.006-7.95 1.349-11.128 3.782l-1.57 1.204v-1.981c0-7.084 5.697-12.792 12.698-12.792h3.907c1.615 0 2.93-1.318 2.93-2.936V25.532H86.14v52.851zM52.93 22.596c0-7.016 5.697-12.724 12.698-12.724h.977V62.72l-.974.003c-4.003.012-7.955 1.366-11.13 3.81l-1.57 1.21V22.596zm-5.86 60.772l-1.57-1.202c-3.178-2.432-7.131-3.775-11.13-3.783H13.86V25.532h20.512c7.001 0 12.698 5.708 12.698 12.723v45.113zm42-63.708H72.465V6.936C72.465 5.317 71.15 4 69.535 4h-3.907C55.395 4 47.07 12.342 47.07 22.596v2.084l-1.572-1.21c-3.174-2.445-7.126-3.799-11.129-3.81H10.93C9.315 19.66 8 20.977 8 22.596v58.723c0 1.619 1.315 2.936 2.93 2.936h23.442c5.767.028 10.797 3.952 12.237 9.543.334 1.295 1.499 2.2 2.835 2.202h1.113c1.335-.002 2.5-.907 2.835-2.202 1.439-5.59 6.47-9.515 12.231-9.543H89.07c1.615 0 2.93-1.317 2.93-2.936V22.596c0-1.62-1.315-2.936-2.93-2.936z" />
