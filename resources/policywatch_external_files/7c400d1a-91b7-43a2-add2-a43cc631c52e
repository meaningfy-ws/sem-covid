<script>
// production
window.addEventListener('load', async() => {
  var l = window.location.pathname;
  if ((l != '/status') && (l != '/status/obvestila')) {
    if ('serviceWorker' in navigator) {
      const registration = await navigator.serviceWorker.register('/firebase-messaging-sw.js', {
        updateViaCache: 'none'
      });
      
      var firebase_config = {
        apiKey: "AIzaSyBV4DLsfjGSsRbr9uNhkZ3PQdqYHVvXaFA",
        authDomain: "primorske-si.firebaseapp.com",
        databaseURL: "https://primorske-si.firebaseio.com",
        projectId: "primorske-si",
        storageBucket: "primorske-si.appspot.com",
        messagingSenderId: "411264579330",
        appId: "1:411264579330:web:b4958762a566dd22"
      };
  
      firebase.initializeApp(firebase_config);
  
      const messaging = firebase.messaging();
  
      messaging.useServiceWorker(registration);
  
      messaging.usePublicVapidKey("BKdJ4_PFUtQEfJD17YvVnztkzGl9NLn1cL-4v5GIsYdPeiaYOIatAr7Sz2H-66BbJ2dXtrpYDocRnvP8AsVIJGY");
  
      Notification.requestPermission().then((permission) => {
        if (permission === 'granted') {
          messaging.getToken()
          .then(function(currentToken) {
            if (currentToken) {
              sendTokenToServer(currentToken, current_user);
            }
            else {
              setTokenSentToServer(false);
            }
          })
          .catch(function(err) {
            setTokenSentToServer(false);
          })
        }
        else {
        }
      });
  
      messaging.onTokenRefresh(function() {
        messaging.getToken()
        .then(function(refreshedToken) {
          // token refreshed, send it to the backend
          setTokenSentToServer(false);
          sendTokenToServer(refreshedToken);
        })
        .catch(function(err) {
        });
      });
  
      function sendTokenToServer(currentToken, current_user) {
        if (!isTokenSentToServer()) {
          var xhr = new XMLHttpRequest();
          xhr.open("POST", 'https://api.primorske.si/pn.svc/notification_token');
          xhr.setRequestHeader("Content-Type", "application/json;charset=UTF-8");
          xhr.onreadystatechange = function(){
            if (xhr.readyState === 4 && xhr.status === 200) {
              setTokenSentToServer(true);
            }
          };
          var json = {
            "token":currentToken,
            "user_name":current_user,
            "topic":"pn_all"
          };
          xhr.send(JSON.stringify(json));
        }
        else {
          // already sent to server
        }
      }
  
      function isTokenSentToServer() {
        return window.localStorage.getItem('sentToServer') == 1;
      }
  
      function setTokenSentToServer(sent) {
        window.localStorage.setItem('sentToServer', sent ? 1 : 0);
      }

      messaging.onMessage((payload) => {
        VanillaToasts.create({
          title: payload.notification.title,
          text: payload.notification.body,
          icon: 'https://www.primorske.si/icons/android-chrome-96x96.png',
          type: 'info',
          timeout: 5000,
          callback: function() {
            window.open(payload.notification.click_action, '_blank');
          }
        });
      });
    }
  }
});
</script>
    
      <input type="hidden" name="__VIEWSTATE" id="__VIEWSTATE" value="" />
