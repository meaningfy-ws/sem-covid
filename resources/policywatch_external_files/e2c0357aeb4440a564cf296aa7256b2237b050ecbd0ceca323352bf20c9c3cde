        <script>
            var EDKTranslations = {
                fi: {
                    resxMobileSearchClose: "Piilota hakulaatikko",
                    resxMobileSearchOpen: "Näytä hakulaatikko",
                    resxMobileNavigationOpen: "Avaa navigaatio",
                    resxMobileNavigationClose: "Sulje navigaatio",
                    resxMobileNavigationAriaLabel: "Hampurilaisnavigaatio",
                    resxTopNavigationbAriaLabel: "Päänavigaatio",
                    resxBreadcrumbAriaLabel: "Murupolku",
                    resxLanguageLabel: "Sivuston kieli:&nbsp;"
                },
                en: {
                    resxMobileSearchClose: "Hide search box",
                    resxMobileSearchOpen: "Show search box",
                    resxMobileNavigationOpen: "Open navigation",
                    resxMobileNavigationClose: "Close navigation",
                    resxMobileNavigationAriaLabel: "Hamburger navigation bar",
                    resxTopNavigationbAriaLabel: "Main navigation",
                    resxBreadcrumbAriaLabel: "Bread crumb trail",
                    resxLanguageLabel: "Site language:&nbsp;"
                },
                sv: {
                    resxMobileSearchClose: "Dölj sökfältet",
                    resxMobileSearchOpen: "Visa sökfältet",
                    resxMobileNavigationOpen: "Öpna navigering",
                    resxMobileNavigationClose: "Stäng navigering",
                    resxMobileNavigationAriaLabel: "Hamburgernavigering",
                    resxTopNavigationbAriaLabel: "Huvudnavigering",
                    resxBreadcrumbAriaLabel: "Länkstig",
                    resxLanguageLabel: "Webbplatsens språk:&nbsp;"
                }
            }

            $(document).ready(function () {
                var current_url = window.location.href.toLowerCase();
                if (current_url.indexOf("/fi/") >= 0) {
                    EDKResx = EDKTranslations.fi;
                }
                else if (current_url.indexOf("/sv/") >= 0) {
                    EDKResx = EDKTranslations.sv;
                }
                else {
                    EDKResx = EDKTranslations.en;
                }
                var x = document.getElementById("mobile-search-title");
                x.innerHTML = EDKResx.resxMobileSearchOpen;
                var y = document.getElementById("NavigationButtonText");
                y.innerHTML = EDKResx.resxMobileNavigationOpen;

                $("#hamburgerNavigation").attr("aria-label", EDKResx.resxMobileNavigationAriaLabel);
                $("#TopNavigation").attr("aria-label", EDKResx.resxTopNavigationbAriaLabel);
                $("#breadcrumb").attr("aria-label", EDKResx.resxBreadcrumbAriaLabel);

                $("#languageLabel").html(EDKResx.resxLanguageLabel);
            });

            function ToggleNavigation() {
                var current_url = window.location.href.toLowerCase();
                if (current_url.indexOf("/fi/") >= 0) {
                    EDKResx = EDKTranslations.fi;
                }
                else if (current_url.indexOf("/sv/") >= 0) {
                    EDKResx = EDKTranslations.sv;
                }
                else {
                    EDKResx = EDKTranslations.en;
                }

                var x = document.getElementById("NavigationButtonText");
                if (x.innerHTML === EDKResx.resxMobileNavigationOpen) {
                    x.innerHTML = EDKResx.resxMobileNavigationClose;
                } else {
                    x.innerHTML = EDKResx.resxMobileNavigationOpen;
                }
            }

            function ToggleSearchForm() {
                var current_url = window.location.href.toLowerCase();
                if (current_url.indexOf("/fi/") >= 0) {
                    EDKResx = EDKTranslations.fi;
                }
                else if (current_url.indexOf("/sv/") >= 0) {
                    EDKResx = EDKTranslations.sv;
                }
                else {
                    EDKResx = EDKTranslations.en;
                }

                var x = document.getElementById("mobile-search-title");
                if (x.innerHTML === EDKResx.resxMobileSearchOpen) {
                    x.innerHTML = EDKResx.resxMobileSearchClose;
                } else {
                    x.innerHTML = EDKResx.resxMobileSearchOpen;
                }

                var x = document.getElementById("mobile-search-button").getAttribute("aria-expanded");
                if (x == "true") {
                    x = "false"
                } else {
                    x = "true"
                }
                document.getElementById("mobile-search-button").setAttribute("aria-expanded", x);
            }
        </script>

