<script type="text/javascript">
 function refreshCaptcha() {
 $('#refreshcaptcha')[0].setAttribute('value', 'true');
 }

 $(document).ready(function () {
 $('#modal-feedback').on('hidden.bs.modal', function () {
 $("#testoFeedback").val("");

 $('#chiaro').val(0);
 $('#gruppo-chiaro').ratingcontrol('cancel', false); 

 $('#completo').val(0);
 $('#gruppo-completo').ratingcontrol('cancel', false); 

 $('#aggiornato').val(0);
 $('#gruppo-aggiornato').ratingcontrol('cancel', false); 

 $('#facile').val(0);
 $('#gruppo-facile').ratingcontrol('cancel', false); 

 $("#trovato1").prop("checked", false);
 $("#trovato2").prop("checked", false);
 $("#trovato3").prop("checked", false);

 $("#divMessageAiutaMigliorarePagina").css("display", "none");
 });
 });

</script>

