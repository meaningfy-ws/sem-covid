<script type="text/javascript">
	var pkDocumentTitle = document.title;
    var pkBarPathDelPortale = "";
    //C2
    if (typeof (sPathBarDelPortaleNuova) !== 'undefined' && sPathBarDelPortaleNuova.length > 0) {
		sPathBarDelPortale = sPathBarDelPortaleNuova;
    }

	if (typeof (sPathBarDelPortale) !== 'undefined') {
		if (sPathBarDelPortale.substring(0, 1) != "/") {
			pkBarPathDelPortale = "/";
		}
		pkBarPathDelPortale += sPathBarDelPortale;
	}
	else {
		pkBarPathDelPortale = window.location.href;
	}

	var _paq = _paq || [];
	/* C3 */
	//_paq.push(["setCookieDomain", "*.inps.it"]);
	_paq.push(["setDocumentTitle", sPathBarDelPortaleNuova]);
	_paq.push(['setCustomUrl', location.href.toLowerCase()]);	
	//C8
	_paq.push(['trackPageView']);
	_paq.push(['enableLinkTracking']);
	(function() {
		//var u = "//**/stat3/"; //C4
		//var u = "//**/stat3/"; //C5
		var u = "//www.inps.it/stat3/"; //C6
		_paq.push(['setTrackerUrl', u+'piwik.php']);
		_paq.push(['setSiteId', '2']);
		var d=document, g=d.createElement('script'), s=d.getElementsByTagName('script')[0];
		g.type='text/javascript'; g.async=true; g.defer=true; g.src=u+'piwik.js'; s.parentNode.insertBefore(g,s);
	})();

	window.piwikAsyncInit = function () {
		//C7
		trackBigData();
	};

	function trackPiwikScrollPageView() {
		_paq.push(["trackPageView"]);
		//C7
		trackBigData();
	}

	//C7

	var trackBigData = function (){
		var piwikBigData = Piwik.getTracker("//www.inps.it/stat3/bd.php", 11);
		//piwikBigData.setDocumentTitle(pkBarPathDelPortale);
		piwikBigData.setDocumentTitle(window.location.origin.toLowerCase() + pkBarPathDelPortale);
		
		try {
			var cookieByName = getCookieByName("newSCCSP");
			if (cookieByName){
				piwikBigData.setCustomVariable( 1, 'userId', getCookieByName("newSCCSP"), 'page');
			}

			if (document.head.querySelector("meta[name=IdContenuto]")) {
				piwikBigData.setCustomVariable( 2, 'contentId', document.head.querySelector("meta[name=IdContenuto]").content, 'page');
			}

			if (document.head.querySelector("meta[name=IdTipologiaContenuto]")) {
				piwikBigData.setCustomVariable( 3, 'contentTypeId', document.head.querySelector("meta[name=IdTipologiaContenuto]").content, 'page');
			}
		}
		catch (err) {
		} 		
		
		piwikBigData.enableLinkTracking(true);
		piwikBigData.trackPageView();
	}
	
	getCookieByName = function (name) {
		match = document.cookie.match(new RegExp(name + '=([^;]+)'));
		if (match) return match[1];
		else return "";
	}
</script>
