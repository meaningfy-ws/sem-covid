        function initTalk() {
            var fnArtCommentCount = document.getElementById('fnArtCommentCount');
            if (fnArtCommentCount){
                var commentsCount = '56';
                if (commentsCount > 0) {
                    fnArtCommentCount.innerHTML = commentsCount;
                }
