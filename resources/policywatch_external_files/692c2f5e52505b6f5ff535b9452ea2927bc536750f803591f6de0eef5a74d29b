            var fnTalkComments = document.querySelector('#fnTalkComments');

            fnfetchTalk.addEventListener('click', function () {
                ebDataLayer.push({ 'event' : 'GAEvent', 'eventCategory' : 'UX', 'eventAction' : 'nationen-vis-kom', 'eventLabel' : '', 'eventValue' : 0, 'eventNint' : 1 });
                fnfetchTalk.classList.add('hidden');
                fnTalkComments.classList.remove('hidden');
            });

            document.getElementById('fnTalkContainer').style.opacity = '1';
            Coral.Talk.render(document.getElementById('talkStream'), {
                lazy: true,
                asset_id: '8045648',
                asset_url: 'https://ekstrabladet.dk/nyheder/politik/coronavirus-regeringen-vil-give-virksomheder-refusion-fra-dag-et/8045648',
                auth_token: '',
                talk: 'https://nationen.ebcomments.dk/',
            });
        }

        if (ebJS.options.isEBJSReady) {
            initTalk();
        } else {
        window.addEventListener('isEBJSReady', function() {
            initTalk();
        });
