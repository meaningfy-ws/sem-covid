<script type="text/javascript">
// SET IFRAME TO FULL CONTENT HEIGHT TO ELIMINATE SCROLL
// THE FUNCTION IS CALLED ON READY: window.onresize = function(){iframe_full_height('IFRAME_ID', 1);}
// IT RUNS ONCE MORE AFTER 500ms
function iframe_full_height($id, $retry)
	{
	// RESET
	$('#' + $id).css('height', '');
	// SET
	$('#' + $id).css('height', $('#' + $id).contents().height());
	// console.log($id +' = '+ $('#' + $id).contents().height());
	// DO ANOTHER FOR LAG FIX
	if ( $retry == 1 )
		{
		setTimeout("iframe_full_height('"+ $id +"', 0)", 100);
		setTimeout("iframe_full_height('"+ $id +"', 0)", 200);
		setTimeout("iframe_full_height('"+ $id +"', 0)", 400);
		setTimeout("iframe_full_height('"+ $id +"', 0)", 600);
		setTimeout("iframe_full_height('"+ $id +"', 0)", 800);
		setTimeout("iframe_full_height('"+ $id +"', 0)", 1000);
		setTimeout("iframe_full_height('"+ $id +"', 0)", 2000);
		setTimeout("iframe_full_height('"+ $id +"', 0)", 3000);
		setTimeout("iframe_full_height('"+ $id +"', 0)", 4000);
		setTimeout("iframe_full_height('"+ $id +"', 0)", 5000);
		}
	}
document.addEventListener("DOMContentLoaded", function(event)
	{
	window.onresize = function()
		{
		iframe_full_height('latest_news', 1);
		iframe_full_height('top_news', 1);
		// iframe_full_height('top_slider', 1);
		}
	});
</script><footer class="main-footer black">
