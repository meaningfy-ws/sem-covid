	window.addEventListener("message", receiveMessage, false);
	function receiveMessage(event)
		{
		// d('iframe msg received');
		if ( event.origin != 'https://googleads.g.doubleclick.net' )
			{
			// d('not google ads');
			return 0;
			}
		try
			{
			var obj = JSON.parse(event.data);
			}
		catch (error)
			{
			var obj = 0;
			// d('NON JSON MSG');
			return 0;
			}
		if ( obj == 0 )
			{
			// d('iframe msg empty');
			return 0;
			}
		if ( obj.msg_type != 'resize-me' )
			{
			// d('iframe msg not resize');
			return 0;
			}
		d('IFRAME MSG RESIZE');
		d(obj);
		setTimeout("fix_gframe('"+ obj.key_value[0]['value'] +"', '"+ obj.key_value[1]['value'] +"')", 200);
		setTimeout("fix_gframe('"+ obj.key_value[0]['value'] +"', '"+ obj.key_value[1]['value'] +"')", 500);
		setTimeout("fix_gframe('"+ obj.key_value[0]['value'] +"', '"+ obj.key_value[1]['value'] +"')", 1000);
		setTimeout("fix_gframe('"+ obj.key_value[0]['value'] +"', '"+ obj.key_value[1]['value'] +"')", 2000);
		}
	function fix_gframe($h, $qui)
		{
		d('SET G ADS Iframe '+ $qui +' Height to ' + $h);
		var $frame = $("iframe[data-google-query-id='" + $qui +"']");

		$($frame).css('height', $h);
		$($frame).parent().css('height', $h);
		$($frame).parent().parent().css('height', $h);
		$($frame).parent().parent().parent().css('height', $h);
		}
