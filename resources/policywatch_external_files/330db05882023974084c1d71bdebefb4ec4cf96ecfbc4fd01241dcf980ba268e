<script>
function number_clean($number)
	{
	if ( $number === true )
		{
		return 1;
		}
	$number = String($number);
	if ( $number == '' || $number == undefined )
		{ return 0; }
	$number = pstr_replace(' ', '', $number);
	$number = pstr_replace(',', '.', $number);
	// check if negative and save
	var $negative = $number.substr(0, 1);
	$number = $number.replace(/[^\d.]/g, '');
	$number = parseFloat($number);
	if ( $number == '' || $number == undefined || isNaN($number) )
		{ return 0; }
	// if was negative fix it
	if ( $negative == '-' )
		{
		$number = $number * -1;
		}
	return $number;
	}
function pfloat($number){return number_clean($number);}
function pround($number, $decimal){if($decimal==undefined){$decimal=0;}$number=pfloat($number);return Math.round($number*Math.pow(10,$decimal))/Math.pow(10,$decimal);}
function pint($number){return $number=pround($number);}
function pstr_replace($search, $replace, $string)
	{
	$string = String($string);

	if ( $search == '' || $search == undefined || $string == '' || $string == undefined )
		{ return $string; }

	if ( $search == '\\' )
		{ $string = $string.replace(/\\/g, $replace); }
	else if ( $search == '/' )
		{ $string = $string.replace(/\//g, $replace); }
	else
		{
		while ( $string.search($search) != -1 )
			{ $string = $string.replace($search, $replace); }
		}
	return $string;
	}
var $trud_get_id = "5074706";
var $article_id = "5074706";
function rate_comment($comment_id, $like)
	{
	// HANDLE LIKE
	if ($like == 1)
		{
		// check block
		if ( $('#like_' + $comment_id).attr('done') == '1' )
			{
			return 0;
			}
		// GET LIKE COUNT++
		var $count = pint( $('#like_' + $comment_id).html() ) + 1;
		// block like
		$('#like_' + $comment_id).attr('done', 1);
		}

	// HANDLE LIKE
	else
		{
		// check block
		if ( $('#dislike_' + $comment_id).attr('done') == '1' )
			{
			return 0;
			}
		// GET LIKE COUNT++
		var $count = pint( $('#dislike_' + $comment_id).html() ) + 1;
		// block like
		$('#dislike_' + $comment_id).attr('done', 1);
		}

	var $request = new Array();
	$request['url'] = '/index.php?p=trud_ajax&article_id=5074706&ajax_rate=1&like=' + $like + '&comment_id=' + $comment_id;
	$request['on_success'] = 'rate_comment_success';
	$request['on_success_data'] = {'comment_id' : $comment_id, 'like' : $like, 'count': $count};

	ajax_request($request);
	}

function rate_comment_success($answer, $data)
	{
	if ($answer == '1')
		{
		if ($data.like == 1)
			{
			$('#like_' + $data.comment_id).html( $data.count );
			}
		else
			{
			$('#dislike_' + $data.comment_id).html( $data.count );
			}
		}
	}

document.addEventListener("DOMContentLoaded", function(event)
	{
	// ARTICLE VIEW++
	if ( null )
		{
		setTimeout(function()
			{
			$("#autoopen_comments").trigger('click');
			},1);
		}
	var $request = new Array();
	$request['url'] = '/index.php?p=trud_ajax&view=' + $article_id;
	ajax_request($request);
	document.getElementById('test').style.display = 'none';
	});

</script>
