						function archive_datepicker()
							{
							var $to_load = '/public/_core/jquery.ui/base/jquery-ui.min.css';
							$('head').append($('<link rel="stylesheet" type="text/css" />').attr('href', $to_load));

							$to_load = '/public/_core/jquery.ui/jquery-ui.min.js';
							$.getScript($to_load, function()
								{
								$to_load = '/public/_core/datepicker/datepicker-bg.js';
								$.getScript($to_load, function()
									{
									$to_load = '/public/js/archive_calendar.js';
									$.getScript($to_load, function(){});
									});
								});
							}
