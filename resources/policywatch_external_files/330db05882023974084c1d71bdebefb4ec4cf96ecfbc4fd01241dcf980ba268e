			<script type="text/javascript">
			// DAYS A WEEK
			var days = ['Неделя','Понеделник','Вторник','Сряда','Четвъртък','Петък','Събота'];
			var currentdate = new Date();

			//	GET THE DAY OF THE MONTH, 2 DIGITS WITH LEADING ZEROS
			var day = (currentdate.getDate() < 10	? '0' : '') + currentdate.getDate();
			var month = (currentdate.getMonth() < 9	? '0' : '') + (currentdate.getMonth() + 1 ) ;
			var $minutes =(currentdate.getMinutes() < 10	? '0' : '') + (currentdate.getMinutes());
			var $hour =(currentdate.getHours() < 10	? '0' : '') + (currentdate.getHours());
			var datetime = days[currentdate.getDay()] + " / "
							+ day + "."
							+ month	+ "."
							+ currentdate.getFullYear() + " / "
							+ $hour + ":"
							+ $minutes;

			document.getElementById("time-item").innerHTML = datetime ;
			</script>
