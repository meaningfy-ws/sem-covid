<script type="text/javascript">	
	function displayPdfIcon() {
		document.write('<img align="absmiddle" border="0" src="/wps/PA_RoleDesAffaires/images/acrobat.gif" hspace="3" vspace="3">');
	};
	function displayFolderIcon() {
		document.write('<img align="absmiddle" border="0" src="/wps/PA_RoleDesAffaires/images/folder_into.gif" hspace="3" vspace="3">');
	};
	function displayWordIcon() {
		document.write('<img align="absmiddle" border="0" src="/wps/PA_RoleDesAffaires/images/word.gif" hspace="3" vspace="3">');
	};
	function displayVideoIcon() {
		document.write('<img align="absmiddle" border="0" src="/wps/PA_RoleDesAffaires/images/video_link.gif" hspace="3" vspace="3">');
	};
	function downloadResumeFile(resumePath) {
		window.open("/wps/PA_RoleDesAffaires/FTSServletImpl/?path=" + resumePath); ///export/docpa2/1704_res_0.doc
	}
	function displayDocumentFile(docPath) {
		window.open('/wps/PA_RoleDesAffaires/FTSByteServingServletImpl?path=' + docPath,'download','resizable=1, scrollbars=1, status=1');
	}
	function searchInArchives(description) {
		var indice = description.slice(-2);
		var displayWindow = window.open("https://www.chd.lu/wps/portal/public/Accueil/TravailALaChambre/Recherche/RechercheArchives?lqs_fmid=&lqs_dpid=7545_" + indice);
	}
	function openReportFileByUpdf(docUpdf) {
		var displayWindow = window.open("/wps/PA_RoleDesAffaires/FTSShowAttachment?id=" + docUpdf);
	}
	function viewVideo(fichierRef, sequenceRef) {  
		var param = "resizable = yes, scrollbars=yes, menubar = no, toolbar = yes, status = no, width = 1065, height = 900px, screenX = 10px, screenY = 10px, top = 10px, left = 10px";
		crpglPopUp.newPopUp('https://www.chd.lu/ArchivePlayer', fichierRef, sequenceRef);
	}

	//Function permitting to call the servlet permitting to compile to full dossier
	function compileCompleteDossier(url, userName, width, height, scroll) {
		var windowWidth = (screen.width - width) / 2;
		var windowHeigth = (screen.height - height) / 2;
		winprops = 'height=' + height + ', width=' + width + ', top=' + windowHeigth + ', left=' + windowWidth + ', scrollbars=' + scroll + ', resizable=yes'
		win = window.open("/wps/PA_RoleDesAffaires/" + url, userName, winprops)
		if (parseInt(navigator.appVersion) >= 4) {
			win.window.focus();
		}
	};
	
	// Print function
	function printList(tagid) {
		 var doc = document.getElementById(tagid).innerHTML;
		 var pos = 1;
		 while(pos != 0){
		  doc = doc.replace('<a','<c');
		  pos = doc.indexOf('<a', pos) + 1;
		 }

		var pos = 1;
		while(pos != 0){
		 doc = doc.replace('<A','<c');
		 pos = doc.indexOf('<A', pos) + 1;
		}

		 var NewWindow = window.open('' ,'MyWindow');
		 var print = '\<script\> window.print();\</script\>';
		 var debatDetailsStyle=
			'table.table td {padding:5px;}'+
			'.table {width:100%;background-color:black;}'+
			'.header {background-color:#EFC663;width:100%;padding:5px;margin-top:5px;clear:both;}'+
			'.summary {background-color:#D9D9D9;width:100%;padding:5px;margin-top:5px;}'+
			'.property {background-color:#D9D9D9;width:100%;padding:5px;margin-top:5px;}'+
			'.panel_div {margin-top:5px;}'+
			'.panel {width:100%;padding:5px;}'+
			'.footer {width:100%;background-color:#D9D9D9;}'+
			'.table_column_title {background-color:#EBEBEB;font-weight:bold;padding:5px;}'+
			'.table_column_content {background-color:#F7F7F7;padding:5px;}'+
			'.table_debat {width:100%;}'+
			'.panel_title {text-decoration:underline;font-weight:bold;}'+
			'.subject_header {font-weight:bold;}'+
			'.document_link {font-weight:bold;padding-right:5;}'+
			'.property_name {font-weight:bold;}'+
			'.button_goback {width:16px;height:16px;}'+
			'.previous_page {float: left;margin-bottom:5px;}'+
			'.search_page {float: left;margin-left:10px;}'+
			'.compile_page {float: left;margin-left:10px;}'+
			'.print_page {float: left;margin-left:10px;}'+
			'.header_option {float:right;}';
		 NewWindow.document.write('\<html\>\<body\>\<style\>');
		 NewWindow.document.write(debatDetailsStyle);
		 NewWindow.document.write('\</style\>');
		 NewWindow.document.write(doc);
		 NewWindow.document.write('\</body\>\</html\>');
		 NewWindow.document.write(print);
		 NewWindow.document.close();
	}
	
	function doDetails(library, id) {
		if (library == "docpa") {
			window.location = "/wps/portal/public/Accueil/TravailALaChambre/Recherche/RoleDesAffaires?action=doDocpaDetails&id=" + id;
		}
		if (library == "motion") {
			window.location = "/wps/portal/public/Accueil/TravailALaChambre/Recherche/RoleDesAffaires?action=doMotionDetails&id=" + id;
		}
		if (library == "questpa") {
			window.location = "/wps/portal/public/Accueil/TravailALaChambre/Recherche/RoleDesAffaires?action=doQuestpaDetails&id=" + id;
		}
		if (library == "docpaeuro") {
			window.location = "/wps/portal/public/Accueil/TravailALaChambre/Recherche/RoleDesAffaires?action=doDocpaeuroDetails&id=" + id;
		}
	}
