<script type="text/javascript">
    $(document).scroll(function () {
        var scroll = $(window).scrollTop();

        if (scroll > 300) {
            $("#scrolltotop").fadeIn();
        } else {
            $("#scrolltotop").fadeOut();
        }
    });

    $(document).ready(function () {
        $("#scrolltotop").click(function () {
            $("body,html").animate({ scrollTop: 0 }, '500');
            return false;
        });
    });
</script>

