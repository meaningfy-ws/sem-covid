    <link rel="stylesheet" href="https://code.jquery.com/ui/1.10.0/themes/base/jquery.ui.autocomplete.css" />
    <script src="https://code.jquery.com/ui/1.10.0/jquery-ui.js"></script>
    <button onclick="javascript:ctl00_ctl63_ctl00_ctl00_Search(); return false;" title="Avvia la ricerca" role="button">
        <em class="fa fa-search"></em>
    </button>
    <input name="ctl00$ctl63$ctl00$ctl00$TxtRicerca" type="text" id="ctl00_ctl63_ctl00_ctl00_TxtRicerca" class="rest-searchbox" label="cerca" name="Cerca" placeholder="Cerca in questo sito" title="Inserisci i termini da cercare" />
</div>

<script type="text/javascript">
    if (typeof mInstance === "undefined") {
        var mInstance = null;
    }

    if (typeof (Sys) != "undefined") {
        if (mInstance == null)
            mInstance = Sys.WebForms.PageRequestManager.getInstance();

        mInstance.add_pageLoaded(function (sender, args) {
                $("#ctl00_ctl63_ctl00_ctl00_TxtRicerca").keypress(function (event) {
                    if (event.which == 13) {
                    ctl00_ctl63_ctl00_ctl00_Search()
                    event.preventDefault();
                }
            });
        });
    }

    function ctl00_ctl63_ctl00_ctl00_Search() {
        var pathRicerca = '/Pagine/Cerca-nel-sito.aspx';
        var searchValue = $("#ctl00_ctl63_ctl00_ctl00_TxtRicerca").val();
        if (searchValue != "") {
            window.location.href = pathRicerca + '#k=' + searchValue;
        }
    }
</script>

<script type="text/javascript">
    // Parse an item and create an title/value hash table with all the properties available
    function getFields(results) {
        r = {};
        for (var i = 0; i < results.length; i++) {
            if (results[i] != undefined && results[i].Key != undefined) {
                if (results[i].Key == "Path") {
                    r[results[i].Key] = results[i].Value.replace("http://authoringlavoronew:1162/", "/");

                    if (results[i].Value == "http://authoringlavoronew:1162") {
                        r[results[i].Key] = "/";
                    }
                }
                else {
                    r[results[i].Key] = results[i].Value;
                }
                
            }
        }
        return r;
    }

    $(document).ready(function () {


        


        var autocomplete = $(".rest-searchbox").autocomplete({
            minLength: 3,
            source: function (request, response) {
                $.ajax({
                    beforeSend: function (request) {
                        request.setRequestHeader("Accept", "application/json;odata=verbose;charset=utf-8");
                    },
                    url: "/_api/search/query?QueryTemplatePropertiesUrl='spfile://webroot/queryparametertemplate.xml'&querytext='LavoroIstituzionaleContenuto:" + request.term + "* path:http://authoringlavoronew:1162//*'",
                    dataType: "json",
                    success: function (data) {
                        response($.map(data.d.query.PrimaryQueryResult.RelevantResults.Table.Rows.results, function (item) {
                            return {
                                fields: getFields(item.Cells.results)
                            }
                        }));
                    },
                    error: function (data) {
                        //alert('search error');
                    }
                });
            },
            // Run this when the item is in focused (not selected)
            focus: function (event, ui) {
                //$( "#restSearch" ).val(ui.item.value );
                return false;
            },
            // Run this when the item is selected
            select: function (event, ui) {
                location.href = ui.item.fields.Path;
            },
            appendTo: $('.rest-search-container')
        }).data("uiAutocomplete")._renderItem = function (ul, item) {
            // format the documents using the OOTB SharePoint icons
            img = "ic" + item.fields.FileType + ".png";
            if (item.fields.FileType == "html" || item.fields.FileType == null) {
                img = "html16.png";
            }
            return $("<li>").append('<a><img style="margin-right:3px;top:3px;" src="/_layouts/15/images/' + img + '">' + item.fields.Title + '</a>')
                .appendTo(ul);
        };
    });
</script>
<!--End Search Box-->

