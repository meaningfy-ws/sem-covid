    <script type="text/javascript">
        if (typeof mInstance === "undefined") {
            var mInstance = null;
        }

        if (typeof (Sys) != "undefined") {
            if (mInstance == null)
                mInstance = Sys.WebForms.PageRequestManager.getInstance();

            mInstance.add_pageLoaded(function (sender, args) {
                $(".menu-interno > ul > li .panel-children").hide();

                $(".menu-interno > ul > li .panel-fa .fa").click(function () {
                    if ($(this).hasClass("fa-plus")) {
                        $(this).removeClass("fa-plus");
                        $(this).addClass("fa-minus");
                        $(this).parent().next().slideDown();
                    } else {
                        $(this).removeClass("fa-minus");
                        $(this).addClass("fa-plus");
                        $(this).parent().next().slideUp();
                    }
                });

                $(".menu-interno > ul > li.active").find(".panel-fa .fa").click();
            });
        }
    </script>
</nav>

                
