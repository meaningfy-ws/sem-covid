<script>
				// temp instagram embed ios fixed, without this embeds don't change their height properly
				window.onload = function () {
					let	instaEmbedCounter = 0;
					let instaEmbedFixer = setInterval(function () {
						let instaEmbeds = document.querySelectorAll('.article__main iframe.instagram-media-rendered');
						if (instaEmbeds.length > 0) {
							for (let i = 0; i < instaEmbeds.length; i++) {
								instaEmbeds[i].style.minHeight = instaEmbedCounter+"px";
							}
						}
						instaEmbedCounter++;						
						if (instaEmbedCounter >= 15) {
							clearInterval(instaEmbedFixer);
						}
					}, 2000);					
				};
			</script>




