<script>                
        document.addEventListener("DOMContentLoaded", function() {

            var siuOnesigBtnState = false;
            var siuManagePushButtons = document.querySelectorAll('.siu-manage-push-notifications');
            var footManagePushButton = document.querySelector('#menu-footer-menu-1 .siu-manage-push-notifications');
            var footManagePushButtonLink = document.querySelector('#menu-footer-menu-1 .siu-manage-push-notifications a');

            footManagePushButtonLink.addEventListener('click', function () {
                if (!siuOnesigBtnState) {
                    document.getElementById('onesignal-bell-container').style.display = 'block';
                    footManagePushButtonLink.style.backgroundColor = '#5ab100';
                    footManagePushButtonLink.style.color = '#fff';
                    footManagePushButtonLink.text = "Skryť tlačidlo notifikácií";
                    siuOnesigBtnState = true;
                } else {
                    document.getElementById('onesignal-bell-container').style.display = 'none';
                    footManagePushButtonLink.style.backgroundColor = '#111';
                    footManagePushButtonLink.style.color = '#555';
                    footManagePushButtonLink.text = "Spravovať notifikácie";
                    siuOnesigBtnState = false;
                }
            });

        });
    </script>
<script>
    
    var ww = window.innerWidth;

    var siuOnesigTopContDisplay = false; // holds state of display for top container
    var siuOnesigTopCont = document.getElementById('siu-my-notification-top-button-container');
    var siuOnesigTopContClose = document.getElementById('siu-my-notification-top-close-button');    

    /** 
     * Called when our custom button is clicked
     */
    function onManageWebPushSubscriptionButtonClicked(event) {
        getSubscriptionState().then(function(state) {
            if (state.isPushEnabled) {
                /* Subscribed, opt them out */
                OneSignal.setSubscription(false);
            } else {                

                if (state.isOptedOut) {                    
                    /* Opted out, opt them back in */
                    OneSignal.setSubscription(true);
                } else {                    
                    /* Unsubscribed, subscribe them */
                    OneSignal.registerForPushNotifications();
                    siuOnesigTopCont.style.display = "none";
                    siuOnesigTopContDisplay = false;                    
                }
            }
        });
        event.preventDefault();
    }

    /**
     * Update the button text, used when the user's subscription state changes during the page's session
     * Also run on page load
     *     
     */
    function updateMangeWebPushSubscriptionButton(buttonSelector) {

        var hideWhenSubscribed = true;
        var subscribeText = "Zapnúť notifikácie";
        var unsubscribeText = "Unsubscribe from Notifications";

        getSubscriptionState().then(function(state) {
            var buttonText = !state.isPushEnabled || state.isOptedOut ? subscribeText : unsubscribeText;


            var element = document.querySelector(buttonSelector);
            if (element === null) {
                return;
            }

            element.removeEventListener('click', onManageWebPushSubscriptionButtonClicked);
            element.addEventListener('click', onManageWebPushSubscriptionButtonClicked);
            element.textContent = buttonText;

            if (hideWhenSubscribed && state.isPushEnabled) {                

                //console.log("siuOnesigTopContDisplay: " + siuOnesigTopContDisplay + ", state.isOptedOutxxx: " + state.isOptedOut);

                // dont display if subbed
                element.style.display = "none";
                
                if(siuOnesigTopContDisplay) {
                    // if top Cont visible hide it
                    siuOnesigTopCont.style.display = "none";
                    siuOnesigTopContDisplay = false;
                }


            } else {
                // if not subbed yet                
                
                element.style.display = "";
                
                //console.log("siuOnesigTopContDisplay: " + siuOnesigTopContDisplay + ", state.isOptedOutxyx: " + state.isOptedOut);

                if(!siuOnesigTopContDisplay && state.notificationPermission !== "denied" && state.isOptedOut != true) {
                    // this is true on page load                                        

                    // display the Top Container if not subbed
                    siuOnesigTopCont.style.display = "flex";
                    siuOnesigTopContDisplay = true;

                } else {
                    // if not subbed and update state
                    siuOnesigTopCont.style.display = "none";
                    siuOnesigTopContDisplay = false;

                }

            }
        });
    }

    function getSubscriptionState() {
        return Promise.all([
        OneSignal.isPushNotificationsEnabled(),
        OneSignal.getNotificationPermission(),
        OneSignal.isOptedOut()
        ]).then(function(result) {
            var isPushEnabled = result[0];
            var notificationPermission = result[1];
            var isOptedOut = result[2];

            return {
                isPushEnabled: isPushEnabled,
                notificationPermission: notificationPermission,
                isOptedOut: isOptedOut
            };
        });
    }

    function siuSetOnesigTopContCookie(expiry) {
        if(expiry == 3) {
            var inThreeDays = new Date(); // now
            inThreeDays.setDate(inThreeDays.getDate()+3); // now + 72 hours
            inThreeDays.setHours(0,0,0,0); // 3 days from now at 00:00    

            // set cookie that expired 3 days from now at 00:00
            document.cookie = "\"siuHideOnesigTopContent=1; expires=\"" + inThreeDays + "; secure; samesite=lax";
        } else if (expiry == 0) {
            // set cookie without expiration
            document.cookie = "siuHideOnesigTopContent=1; max-age=12960000; secure; samesite=lax";
        }
    }

    var OneSignal = OneSignal || [];
    var buttonSelector = "#my-notification-button"; // use this id on button

    if ( document.cookie.indexOf('siuHideOnesigTopContent') == -1 ) {

        if(siuOnesigTopContClose) {

            siuOnesigTopContClose.addEventListener('click', function(){

                siuSetOnesigTopContCookie(3);

                siuOnesigTopCont.style.display = "none";
                siuOnesigTopContDisplay = false;

            });

        }

        /* This example assumes you've already initialized OneSignal */
        OneSignal.push(function() {
            // If we're on an unsupported browser, do nothing
            if (!OneSignal.isPushNotificationsSupported()) {
                return;
            }

            // run on page load, check user's subscription state and run some stuff
            updateMangeWebPushSubscriptionButton(buttonSelector);
            
            OneSignal.on("subscriptionChange", function(isSubscribed) {
                /* If the user's subscription state changes during the page's session, update the button text */
                updateMangeWebPushSubscriptionButton(buttonSelector);
            });
        });
    }
    </script>
</body>
</html>
