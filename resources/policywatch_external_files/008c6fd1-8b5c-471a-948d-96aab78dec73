<script type='text/javascript'  src='https://covid19estamoson.gov.pt/wp-content/plugins/elementor/assets/js/preloaded-elements-handlers.min.js?m=1611665245'></script>
<!-- start Simple Custom CSS and JS -->
<script type="text/javascript">
jQuery(document).ready(function(jQuery) {	
	jQuery('.checkbox_mutiple_wrapper label').click(function(){
		if (jQuery(this).children('input').is(':checked')) {
			if(jQuery(this).children('input').val() == "Nenhuma"){
				jQuery(this).siblings('label').children('input').prop('checked',false);
			}else{
				jQuery(this).siblings('label').each(function(key, value){
					if(jQuery(value).children('input').val() == "Nenhuma"){
						jQuery(value).children('input').prop('checked',false);
					}
				});
			}
		}
	});
	
	jQuery('.professional_zone label').click(function(){
		if (jQuery(this).children('input').is(':checked')) {
			if(jQuery(this).children('input').val() != "Nenhuma"){
				jQuery('.condition_zone input').each(function(key, value){
					if(jQuery(value).val() != "Nenhuma"){ 
						jQuery(value).attr('checked',false);
					}else{
						jQuery(value).prop('checked',true);
					}
				});
			}
		}
	});
});</script>
<!-- end Simple Custom CSS and JS -->
