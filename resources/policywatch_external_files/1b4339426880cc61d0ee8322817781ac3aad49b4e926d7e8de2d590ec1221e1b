function changeAreaIfNotSelected(selection) {
  var selected = getCookie("selectedArea");
  var selection = selection;
  if ((selection === "Koko Suomi")) {
    createCookie("selectedArea", "noValue", 7);
    changeArea(selection);
  }
  jQuery(document).ready(function () {
    // lowercase ja encode ja suomeksi
    var selectedString = textToComparison(selection);
    selectedString = areaAlteration(selectedString);
    var areaId = selection = areaMap.get(selectedString);
    createCookie("selectedArea", areaId, 7);
  });
}
function createCookie(name, value, exdays) {
  var exdate = new Date();
  exdate.setDate(exdate.getDate() + exdays);
  var c_value = escape(value)
    + ((exdays == null) ? "" : "; expires=" + exdate.toUTCString())
    + ";domain=."+ window.location.hostname +"; path=/";
  document.cookie = name + "=" + c_value;
}
function getCookie(c_name) {
  var i, x, y, ARRcookies = document.cookie.split(";");
  for (i = 0; i < ARRcookies.length; i++) {
    x = ARRcookies[i].substr(0, ARRcookies[i].indexOf("="));
    y = ARRcookies[i].substr(ARRcookies[i].indexOf("=") + 1);
    x = x.replace(/^\s+|\s+$/g, "");
    if (x == c_name) {
      return unescape(y);
    }
  }
}
function textToComparison(areaString) {
  if (areaString !== undefined) {
    areaString = areaString.toLowerCase();
    areaString = areaString.replace(/[\u00E4]/g, "a");
    areaString = areaString.replace(/[\u00f6]/g, "o");
    areaString = areaString.replace(/[\u00D6]/g, "O");
    areaString = areaString.replace(/[\u00C4]/g, "A");
  }
  return areaString;
}
function areaAlteration(area) {
  /*
   * var areas = [ ["lappi","lappland"], ["etela-suomi","i sodra finland"],
   * ["ita-suomi","ostra finland"], ["lounais-suomi","sydvanstra finland"],
   * ["lansi-ja-keski-suomi","vastra och inre finland"],
   * ["pohjois-suomi","norra finland"] ];
   */
  var areas = [["lappi", "lappland"], ["hame", "tavastland"],
  ["etela-savo", "sodra savolax"],
  ["kaakkois-suomi", "sydostra finland"],
  ["kainuu", "kajanaland"], ["keski-suomi", "mellersta finland"],
  ["pirkanmaa", "birkaland"], ["pohjanmaa", "osterbotten"],
  ["etela-pohjanmaa", "sodra osterbotten"],
  ["pohjois-pohjanmaa", "norra osterbotten"],
  ["pohjois-karjala", "norra karelen"],
  ["pohjois-suomi", "norra finland"],
  ["pohjois-savo", "norra savolax"], ["satakunta", "satakunta"],
  ["uusimaa", "nyland"], ["varsinais-suomi", "egentliga finland"]
  ];
  var correctedValue = "";
  for (var i = 0; i < areas.length; i++) {
    var row = areas[i];
    for (var k = 0; k < row.length; k++) {
      var cell = row[k];
      if (area === cell) {
        correctedValue = row[0];
      }
    }
  }
  if (correctedValue !== "") {
    area = correctedValue;
  }
  return area;
}

if (isNaN(getCookie('selectedArea'))) {
    document.cookie = "selectedArea= ; path=/; expires=Thu, 01 Jan 1970 00:00:01 GMT;"
}

			
