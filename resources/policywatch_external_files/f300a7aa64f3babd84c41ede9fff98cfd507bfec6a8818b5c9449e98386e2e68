      // Redirect users to the www.khn domain instead of the non www variant.
      var wwwPrefixRe = /\.?www./g;
      var productionDomainRe = /khn.nl/g;
      var faqDomainRe = /vragen.khn.nl/g;
      var isFaqDomain = faqDomainRe.test(location.host);
      var isProductionDomain = productionDomainRe.test(location.host)
      var hasWwwPrefix = wwwPrefixRe.test(location.host);

      if (isFaqDomain) {
        return location.href = location.href.replace("://vragen.", "://www.");
      }

      if (!hasWwwPrefix && isProductionDomain) {
        return location.href = location.href.replace("://", "://www.");
      }
    }

