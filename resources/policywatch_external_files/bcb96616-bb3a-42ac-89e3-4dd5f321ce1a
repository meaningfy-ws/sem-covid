$(document).ready(function () {
	$(".stergeItemCart").click(function(){
	
$.ajax({
    type: 'POST',
    url:  'sterge-produs',
     data: {        
        item:$(this).attr('id'),
    },         
    success: function(data, status) {   	              	 
    
    	window.location.href="cos-cumparaturi";
    },
    beforeSend: function(){
        $("#loaderDiv").show();        
      },
  		complete: function(){
        $("#loaderDiv").hide();
        
      },
    error: function (xhr, ajaxOptions, thrownError) {
        alert(xhr.status);
        alert(thrownError);
      }

	});
	});
$("#goleste").click(function(){
	
				$.ajax({
				type: 'POST',
				url:  'goleste-cos',
				data: {        
				item:'xx'
				},         
				success: function(data, status) {   	              	 
			
				window.location.href="cos-cumparaturi";
				},
				beforeSend: function(){
				$("#loaderDiv").show();        
				},
					complete: function(){
				$("#loaderDiv").hide();
				
				},
				error: function (xhr, ajaxOptions, thrownError) {
				alert(xhr.status);
				alert(thrownError);
				}
				
				});
	});

});
</script>
