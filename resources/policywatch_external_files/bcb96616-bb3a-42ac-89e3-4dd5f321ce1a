<script>
$( document ).ready(function() {
$("input[name=pret]:radio").bind( "change", function(event, ui) {
    if( $(this).attr("id") == "cn"){ $("input[name=cn]").val("1");$("input[name=fn]").val("0");}
    if( $(this).attr("id") == "fn") {$("input[name=fn]").val("1"); $("input[name=cn]").val("0");}
    $('#final').html('Pret: <b class="pret">'+$(this).val()+',00 RON</b> cu TVA');

    // Call function here
});

var totalCount = 7; 

function setBGImage() {
	var num = Math.ceil( Math.random() * totalCount ); 
    var bgimage = 'imagini/bannere/LEGIS'+num+'.png';
    $('#top_img img').attr("src",bgimage); 
    var num = Math.ceil( Math.random() * totalCount );
    var bgimage = 'imagini/bannere/LEGIS'+num+'.png'; 
    $('#bottom_img img').attr("src",bgimage);
} 
setBGImage();
});

</script>
 
