<script>

function validateEmail() {
  var email=document.getElementById('email').value;
 if (email=="") { $("#err").html('Completati adresa de email!'); return false;}	
  var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
  if( !emailReg.test( email ) ) {
    $("#err").html('Adresa de email invalida!');
    return false;
  } else {
    return true;
  }
}
  
</script>
