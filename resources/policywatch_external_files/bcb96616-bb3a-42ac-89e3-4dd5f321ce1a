	 <script type="text/javascript" charset="utf-8">
		$(document).ready(function(){
			 
			 
			 $('#datepicker').datepicker();
			 $('#frm #criteriu').change(function() { 	 			
					switch($(this).val())
					{
					
					case 'data': 
						$('#frm #datepicker').removeClass( "hidden");
						if(! $('#frm #email').hasClass('hidden'))  $('#frm #email').addClass("hidden");			 	 
						break;
					case 'email':
						$('#frm #email').removeClass( "hidden");
						if(! $('#frm #datepicker').hasClass('hidden'))  $('#frm #datepicker').addClass("hidden");			 	 
						break;
					}
				});
		});
		
		
		
		</script>
